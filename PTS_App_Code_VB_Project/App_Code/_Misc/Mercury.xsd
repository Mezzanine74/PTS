            <MainSource>
              <DbSource ConnectionRef="MERCURYConnectionString (Web.config)" DbObjectName="SELECT TOP (100) PERCENT Table1_Project.ProjectID, RTRIM(Table1_Project.ProjectName) AS ProjectName, SUM(CASE WHEN Table2_POno.PO_Currency = N'Euro' THEN EuroPaidWithVAT ELSE 0 END) AS EurosPaidWithVAT, SUM(CASE WHEN Table2_POno.PO_Currency = N'Dollar' THEN DollarPaidWithVAT ELSE 0 END) AS DollarPaidWithVAT, SUM(CASE WHEN Table2_POno.PO_Currency = N'Rub' THEN RublePaidWithVAT ELSE 0 END) AS RublePaidWithVAT, SUM(CASE WHEN Table2_POno.PO_Currency = N'Euro' THEN BalanceEuroWithVAT ELSE 0 END) AS BalanceEuroWithVAT, SUM(CASE WHEN Table2_POno.PO_Currency = N'Dollar' THEN BalanceDollarWithVAT ELSE 0 END) AS BalanceDollarWithVAT, SUM(CASE WHEN Table2_POno.PO_Currency = N'Rub' THEN BalanceRubleWithVAT ELSE 0 END) AS BalanceRubleWithVAT, SUM(CASE WHEN Table2_POno.PO_Currency = N'Euro' THEN RublePaidWithVAT ELSE 0 END) AS RubleRefToEuro, SUM(CASE WHEN Table2_POno.PO_Currency = N'Euro' THEN RublePaidWithVAT ELSE 0 END) AS RublePaidRefToEuro, SUM(CASE WHEN Table2_POno.PO_Currency = N'Dollar' THEN RublePaidWithVAT ELSE 0 END) AS RublePaidRefToDollar, SUM(CASE WHEN Table2_POno.PO_Currency = N'Rub' THEN RublePaidWithVAT ELSE 0 END) AS RublePaidRefToRuble FROM Table2_PONo INNER JOIN Table1_Project ON Table2_PONo.Project_ID = Table1_Project.ProjectID INNER JOIN View_QryW3_WithVAT ON Table2_PONo.PO_No = View_QryW3_WithVAT.PO_No WHERE (Table1_Project.CurrentStatus = 1) GROUP BY RTRIM(Table1_Project.ProjectName), Table1_Project.ProjectID" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
