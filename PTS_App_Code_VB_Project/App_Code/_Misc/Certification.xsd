            <SelectCommand>
              <DbCommand CommandType="Text" ModifiedByUser="true">
                <CommandText>
                  --1	KS-2,3
                  --2	Tavarnaya Nakladnaya
                  --3	Act
                  --4	Invoice For Advance
                  --5	Fines
                  --6	Invoice For Retention

                  SELECT
                  CASE
                  WHEN [Document_TypeID] = 1 --1	KS-2,3
                  THEN
                  (
                  ISNULL([ExternalValueExcVAT],0)
                  - ISNULL([AdvanceExcVAT],0)
                  - ISNULL([RetentionExcVAT],0)
                  )

                  WHEN [Document_TypeID] = 2 --2	Tavarnaya Nakladnaya
                  THEN
                  (
                  ISNULL([ExternalValueExcVAT],0)
                  - ISNULL([AdvanceExcVAT],0)
                  - ISNULL([RetentionExcVAT],0)
                  )

                  WHEN [Document_TypeID] = 3 --3	Act
                  THEN
                  (
                  ISNULL([ExternalValueExcVAT],0)
                  - ISNULL([AdvanceExcVAT],0)
                  - ISNULL([RetentionExcVAT],0)
                  )

                  WHEN [Document_TypeID] = 4 --4	Invoice For Advance
                  THEN
                  (
                  ISNULL([AdvanceExcVAT],0)
                  )

                  WHEN [Document_TypeID] = 5 --5	Fines
                  THEN
                  (
                  ISNULL([ExternalValueExcVAT],0)
                  )

                  WHEN [Document_TypeID] = 6 --6	Invoice For Retention
                  THEN
                  (
                  ISNULL([RetentionExcVAT],0)
                  )

                  END AS TotalDocValue
                  FROM [Certification].[Table_Documents]
                  WHERE DocumentID = @DocumentID
