      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>if @ProjectID = 0  and  @SupplierID = N'0'
      SELECT * FROM [View_PendingListRev1] ORDER BY OrderCurrency, EuroWithVAT DESC 

      if @ProjectID &lt;&gt; 0 and  @SupplierID = N'0'
      SELECT * FROM [View_PendingListRev1] WHERE (ProjectID=@ProjectID) ORDER BY OrderCurrency, EuroWithVAT DESC

      if @ProjectID &lt;&gt; 0 and  @SupplierID &lt;&gt; N'0'
      SELECT * FROM [View_PendingListRev1] WHERE (ProjectID=@ProjectID) AND (SupplierID=@SupplierID) ORDER BY OrderCurrency, EuroWithVAT DESC

      if @ProjectID = 0 and  @SupplierID &lt;&gt; N'0'
      SELECT * FROM [View_PendingListRev1] WHERE (SupplierID=@SupplierID) ORDER BY OrderCurrency, EuroWithVAT DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
