    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="ProjectName">
          <DataField>ProjectName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RubleWithVAT">
          <DataField>RubleWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DollarWithVAT">
          <DataField>DollarWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="EuroWithVAT">
          <DataField>EuroWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT * from(
SELECT * from (
SELECT     TOP (100) PERCENT 0 as Status ,dbo.View_PendingList.ProjectName, SUM(CASE WHEN PO_Currency = N'Rub' THEN RubleWithVAT ELSE 0 END) AS RubleWithVAT, 
                      SUM(CASE WHEN PO_Currency = N'Dollar' THEN DollarWithVAT ELSE 0 END) AS DollarWithVAT, 
                      SUM(CASE WHEN PO_Currency = N'Euro' THEN EuroWithVAT ELSE 0 END) AS EuroWithVAT
FROM         dbo.View_PendingList INNER JOIN
                      dbo.Table1_Project ON dbo.View_PendingList.ProjectID = dbo.Table1_Project.ProjectID
WHERE     (dbo.Table1_Project.PendingSummary = 1)
GROUP BY dbo.View_PendingList.ProjectName
ORDER BY dbo.View_PendingList.ProjectName ) as DataSource2

UNION ALL

SELECT * from (
SELECT     TOP (100) PERCENT 1 as Status ,dbo.View_PendingList.ProjectName, SUM(CASE WHEN PO_Currency = N'Rub' THEN RubleWithVAT ELSE 0 END) AS RubleWithVAT, 
                      SUM(CASE WHEN PO_Currency = N'Dollar' THEN DollarWithVAT ELSE 0 END) AS DollarWithVAT, 
                      SUM(CASE WHEN PO_Currency = N'Euro' THEN EuroWithVAT ELSE 0 END) AS EuroWithVAT
FROM         dbo.View_PendingList INNER JOIN
                      dbo.Table1_Project ON dbo.View_PendingList.ProjectID = dbo.Table1_Project.ProjectID
WHERE     (dbo.Table1_Project.PendingSummary &lt;&gt; 1)
GROUP BY dbo.View_PendingList.ProjectName
ORDER BY dbo.View_PendingList.ProjectName ) as DataSource1
) As DataSource3
ORDER BY  Status ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
