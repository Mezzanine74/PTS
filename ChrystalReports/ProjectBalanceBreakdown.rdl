      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>IF @Currency = N'Euro' BEGIN SELECT     TOP (100) PERCENT dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectName, 
                                                                                          RTRIM(dbo.View_Projects_Balance_Pending_wthPending_WithVAT.SupplierName) AS SupplierName, 
                                                                                          dbo.View_Projects_Balance_Pending_wthPending_WithVAT.PoTotalEuroWithVAT AS PoTotalWithVAT, 
                                                                                          dbo.View_Projects_Balance_Pending_wthPending_WithVAT.BalanceEuroWithVAT AS BalanceWithVAT, 
                                                                                          dbo.View_Projects_Balance_Pending_wthPending_WithVAT.EuroPendingWithVAT AS PendingWithVAT, 
                                                                                          dbo.View_Projects_Balance_Pending_wthPending_WithVAT.DifferenceInEuro AS DifferenceIn, 
                                                                                          RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.PO_No) AS PO_No, 
                                                                                          dbo.View_QryW3_WithVAT.PoTotalEuroWithVAT AS Po_Sum_InWithVAT, 
                                                                                          dbo.View_QryW3_WithVAT.EuroPaidWithVAT AS Paid_Sum_InWithVAT, 
                                                                                          dbo.View_QryW3_WithVAT.BalanceEuroWithVAT - (CASE WHEN View_QryW3_WithVAT.EuroPendingWithVAT IS NULL 
                                                                                          THEN 0 ELSE View_QryW3_WithVAT.EuroPendingWithVAT END) AS Difference_Po, 
                                                                                          RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.Description) AS Description, RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.Invoice_No) 
                                                                                          AS Invoice_No, dbo.View_QryW3_WthPrjId_WithVAT.Invoice_Date, 
                                                                                          dbo.View_QryW3_WthPrjId_WithVAT.OrderValueEuro AS InvoiceValueInWithVAT, 
                                                                                          RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.PO_Currency) AS PO_Currency, dbo.View_QryW3_WthPrjId_WithVAT.PaymentDate, 
                                                                                          dbo.View_QryW3_WthPrjId_WithVAT.EuroPaidWithVAT AS PaidWithVAT, 
                                                                                          dbo.View_QryW3_WithVAT.BalanceEuroWithVAT AS PoBalanceWithVAT, 
                                                                                          dbo.View_QryW3_WithVAT.EuroPendingWithVAT AS PoPendingWithVAT, CONVERT(numeric(13, 4), 
                                                                                          dbo.View_QryW3_WithVAT.BalanceEuroWithVAT / dbo.View_QryW3_WithVAT.PoTotalEuroWithVAT) AS BalancePercent
                                                                   FROM          dbo.View_Projects_Balance_Pending_wthPending_WithVAT INNER JOIN
                                                                                          dbo.View_QryW3_WthPrjId_WithVAT ON 
                                                                                          dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectID = dbo.View_QryW3_WthPrjId_WithVAT.ProjectID AND 
                                                                                          dbo.View_Projects_Balance_Pending_wthPending_WithVAT.SupplierID = dbo.View_QryW3_WthPrjId_WithVAT.SupplierID INNER JOIN
                                                                                          dbo.View_QryW3_WithVAT ON dbo.View_QryW3_WthPrjId_WithVAT.PO_No = dbo.View_QryW3_WithVAT.PO_No
                                                                   GROUP BY dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectName, 
                                                                                          RTRIM(dbo.View_Projects_Balance_Pending_wthPending_WithVAT.SupplierName), 
                                                                                          dbo.View_Projects_Balance_Pending_wthPending_WithVAT.PoTotalEuroWithVAT, CONVERT(numeric(13, 4), 
                                                                                          dbo.View_QryW3_WithVAT.BalanceEuroWithVAT / dbo.View_QryW3_WithVAT.PoTotalEuroWithVAT), 
                                                                                          dbo.View_Projects_Balance_Pending_wthPending_WithVAT.BalanceEuroWithVAT, 
                                                                                          dbo.View_Projects_Balance_Pending_wthPending_WithVAT.EuroPendingWithVAT, RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.PO_No),
                                                                                           dbo.View_QryW3_WithVAT.PoTotalEuroWithVAT, RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.Description), 
                                                                                          RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.Invoice_No), dbo.View_QryW3_WthPrjId_WithVAT.Invoice_Date, 
                                                                                          dbo.View_QryW3_WthPrjId_WithVAT.OrderValueEuro, RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.PO_Currency), 
                                                                                          dbo.View_QryW3_WthPrjId_WithVAT.PaymentDate, dbo.View_QryW3_WthPrjId_WithVAT.EuroPaidWithVAT, 
                                                                                          dbo.View_QryW3_WithVAT.BalanceEuroWithVAT - (CASE WHEN View_QryW3_WithVAT.EuroPendingWithVAT IS NULL 
                                                                                          THEN 0 ELSE View_QryW3_WithVAT.EuroPendingWithVAT END), 
                                                                                          dbo.View_Projects_Balance_Pending_wthPending_WithVAT.DifferenceInEuro, dbo.View_QryW3_WithVAT.BalanceEuroWithVAT, 
                                                                                          dbo.View_QryW3_WithVAT.EuroPendingWithVAT, dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectID, 
                                                                                          dbo.View_QryW3_WithVAT.EuroPaidWithVAT
                                                                   HAVING      (dbo.View_QryW3_WithVAT.BalanceEuroWithVAT &lt;&gt; 0) AND 
                                                                                          (dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectID = @ProjectID)
                                                                   ORDER BY SupplierName, PO_No END 
IF @Currency = N'Dollar' BEGIN
                                                                                              SELECT     TOP (100) PERCENT dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectName, 
                                                                                                                      RTRIM(dbo.View_Projects_Balance_Pending_wthPending_WithVAT.SupplierName) AS SupplierName, 
                                                                                                                      dbo.View_Projects_Balance_Pending_wthPending_WithVAT.PoTotalDollarWithVAT AS PoTotalWithVAT, 
                                                                                                                      dbo.View_Projects_Balance_Pending_wthPending_WithVAT.BalanceDollarWithVAT AS BalanceWithVAT, 
                                                                                                                      dbo.View_Projects_Balance_Pending_wthPending_WithVAT.DollarPendingWithVAT AS PendingWithVAT, 
                                                                                                                      dbo.View_Projects_Balance_Pending_wthPending_WithVAT.DifferenceInDollar AS DifferenceIn, 
                                                                                                                      RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.PO_No) AS PO_No, 
                                                                                                                      dbo.View_QryW3_WithVAT.PoTotalDollarWithVAT AS Po_Sum_InWithVAT, 
                                                                                                                      dbo.View_QryW3_WithVAT.DollarPaidWithVAT AS Paid_Sum_InWithVAT, 
                                                                                                                      dbo.View_QryW3_WithVAT.BalanceDollarWithVAT - (CASE WHEN View_QryW3_WithVAT.DollarPendingWithVAT IS NULL 
                                                                                                                      THEN 0 ELSE View_QryW3_WithVAT.DollarPendingWithVAT END) AS Difference_Po, 
                                                                                                                      RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.Description) AS Description, 
                                                                                                                      RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.Invoice_No) AS Invoice_No, 
                                                                                                                      dbo.View_QryW3_WthPrjId_WithVAT.Invoice_Date, 
                                                                                                                      dbo.View_QryW3_WthPrjId_WithVAT.OrderValueDollar AS InvoiceValueInWithVAT, 
                                                                                                                      RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.PO_Currency) AS PO_Currency, 
                                                                                                                      dbo.View_QryW3_WthPrjId_WithVAT.PaymentDate, 
                                                                                                                      dbo.View_QryW3_WthPrjId_WithVAT.DollarPaidWithVAT AS PaidWithVAT, 
                                                                                                                      dbo.View_QryW3_WithVAT.BalanceDollarWithVAT AS PoBalanceWithVAT, 
                                                                                                                      dbo.View_QryW3_WithVAT.DollarPendingWithVAT AS PoPendingWithVAT, CONVERT(numeric(13, 4), 
                                                                                                                      dbo.View_QryW3_WithVAT.BalanceDollarWithVAT / dbo.View_QryW3_WithVAT.PoTotalDollarWithVAT) 
                                                                                                                      AS BalancePercent
                                                                                               FROM         dbo.View_Projects_Balance_Pending_wthPending_WithVAT INNER JOIN
                                                                                                                      dbo.View_QryW3_WthPrjId_WithVAT ON 
                                                                                                                      dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectID = dbo.View_QryW3_WthPrjId_WithVAT.ProjectID AND
                                                                                                                       dbo.View_Projects_Balance_Pending_wthPending_WithVAT.SupplierID = dbo.View_QryW3_WthPrjId_WithVAT.SupplierID INNER
                                                                                                                       JOIN
                                                                                                                      dbo.View_QryW3_WithVAT ON dbo.View_QryW3_WthPrjId_WithVAT.PO_No = dbo.View_QryW3_WithVAT.PO_No
                                                                                               GROUP BY dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectName, 
                                                                                                                      RTRIM(dbo.View_Projects_Balance_Pending_wthPending_WithVAT.SupplierName), 
                                                                                                                      dbo.View_Projects_Balance_Pending_wthPending_WithVAT.PoTotalDollarWithVAT, CONVERT(numeric(13, 4), 
                                                                                                                      dbo.View_QryW3_WithVAT.BalanceDollarWithVAT / dbo.View_QryW3_WithVAT.PoTotalDollarWithVAT), 
                                                                                                                      dbo.View_Projects_Balance_Pending_wthPending_WithVAT.BalanceDollarWithVAT, 
                                                                                                                      dbo.View_Projects_Balance_Pending_wthPending_WithVAT.DollarPendingWithVAT, 
                                                                                                                      RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.PO_No), dbo.View_QryW3_WithVAT.PoTotalDollarWithVAT, 
                                                                                                                      RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.Description), RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.Invoice_No), 
                                                                                                                      dbo.View_QryW3_WthPrjId_WithVAT.Invoice_Date, dbo.View_QryW3_WthPrjId_WithVAT.OrderValueDollar, 
                                                                                                                      RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.PO_Currency), dbo.View_QryW3_WthPrjId_WithVAT.PaymentDate, 
                                                                                                                      dbo.View_QryW3_WthPrjId_WithVAT.DollarPaidWithVAT, 
                                                                                                                      dbo.View_QryW3_WithVAT.BalanceDollarWithVAT - (CASE WHEN View_QryW3_WithVAT.DollarPendingWithVAT IS NULL 
                                                                                                                      THEN 0 ELSE View_QryW3_WithVAT.DollarPendingWithVAT END), 
                                                                                                                      dbo.View_Projects_Balance_Pending_wthPending_WithVAT.DifferenceInDollar, 
                                                                                                                      dbo.View_QryW3_WithVAT.BalanceDollarWithVAT, dbo.View_QryW3_WithVAT.DollarPendingWithVAT, 
                                                                                                                      dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectID, 
                                                                                                                      dbo.View_QryW3_WithVAT.DollarPaidWithVAT
                                                                                               HAVING      (dbo.View_QryW3_WithVAT.BalanceDollarWithVAT &lt;&gt; 0) AND 
                                                                                                                      (dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectID = @ProjectID)
                                                                                               ORDER BY SupplierName, PO_No END 
IF @Currency = N'Rub' BEGIN
                                                                                                                          SELECT     TOP (100) PERCENT dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectName, 
                                                                                                                                                  RTRIM(dbo.View_Projects_Balance_Pending_wthPending_WithVAT.SupplierName) AS SupplierName, 
                                                                                                                                                  dbo.View_Projects_Balance_Pending_wthPending_WithVAT.PoTotalRubleWithVAT AS PoTotalWithVAT, 
                                                                                                                                                  dbo.View_Projects_Balance_Pending_wthPending_WithVAT.BalanceRubleWithVAT AS BalanceWithVAT, 
                                                                                                                                                  dbo.View_Projects_Balance_Pending_wthPending_WithVAT.RublePendingWithVAT AS PendingWithVAT, 
                                                                                                                                                  dbo.View_Projects_Balance_Pending_wthPending_WithVAT.DifferenceInRuble AS DifferenceIn, 
                                                                                                                                                  RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.PO_No) AS PO_No, 
                                                                                                                                                  dbo.View_QryW3_WithVAT.PoTotalRubleWithVAT AS Po_Sum_InWithVAT, 
                                                                                                                                                  dbo.View_QryW3_WithVAT.RublePaidWithVAT AS Paid_Sum_InWithVAT, 
                                                                                                                                                  dbo.View_QryW3_WithVAT.BalanceRubleWithVAT - (CASE WHEN View_QryW3_WithVAT.RublePendingWithVAT
                                                                                                                                                   IS NULL THEN 0 ELSE View_QryW3_WithVAT.RublePendingWithVAT END) AS Difference_Po, 
                                                                                                                                                  RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.Description) AS Description, 
                                                                                                                                                  RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.Invoice_No) AS Invoice_No, 
                                                                                                                                                  dbo.View_QryW3_WthPrjId_WithVAT.Invoice_Date, 
                                                                                                                                                  dbo.View_QryW3_WthPrjId_WithVAT.OrderValueRuble AS InvoiceValueInWithVAT, 
                                                                                                                                                  RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.PO_Currency) AS PO_Currency, 
                                                                                                                                                  dbo.View_QryW3_WthPrjId_WithVAT.PaymentDate, 
                                                                                                                                                  dbo.View_QryW3_WthPrjId_WithVAT.RublePaidWithVAT AS PaidWithVAT, 
                                                                                                                                                  dbo.View_QryW3_WithVAT.BalanceRubleWithVAT AS PoBalanceWithVAT, 
                                                                                                                                                  dbo.View_QryW3_WithVAT.RublePendingWithVAT AS PoPendingWithVAT, CONVERT(numeric(13, 4), 
                                                                                                                                                  dbo.View_QryW3_WithVAT.BalanceRubleWithVAT / dbo.View_QryW3_WithVAT.PoTotalRubleWithVAT) 
                                                                                                                                                  AS BalancePercent
                                                                                                                           FROM         dbo.View_Projects_Balance_Pending_wthPending_WithVAT INNER JOIN
                                                                                                                                                  dbo.View_QryW3_WthPrjId_WithVAT ON 
                                                                                                                                                  dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectID = dbo.View_QryW3_WthPrjId_WithVAT.ProjectID
                                                                                                                                                   AND 
                                                                                                                                                  dbo.View_Projects_Balance_Pending_wthPending_WithVAT.SupplierID = dbo.View_QryW3_WthPrjId_WithVAT.SupplierID
                                                                                                                                                   INNER JOIN
                                                                                                                                                  dbo.View_QryW3_WithVAT ON 
                                                                                                                                                  dbo.View_QryW3_WthPrjId_WithVAT.PO_No = dbo.View_QryW3_WithVAT.PO_No
                                                                                                                           GROUP BY dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectName, 
                                                                                                                                                  RTRIM(dbo.View_Projects_Balance_Pending_wthPending_WithVAT.SupplierName), 
                                                                                                                                                  dbo.View_Projects_Balance_Pending_wthPending_WithVAT.PoTotalRubleWithVAT, CONVERT(numeric(13, 4), 
                                                                                                                                                  dbo.View_QryW3_WithVAT.BalanceRubleWithVAT / dbo.View_QryW3_WithVAT.PoTotalRubleWithVAT), 
                                                                                                                                                  dbo.View_Projects_Balance_Pending_wthPending_WithVAT.BalanceRubleWithVAT, 
                                                                                                                                                  dbo.View_Projects_Balance_Pending_wthPending_WithVAT.RublePendingWithVAT, 
                                                                                                                                                  RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.PO_No), dbo.View_QryW3_WithVAT.PoTotalRubleWithVAT, 
                                                                                                                                                  RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.Description), 
                                                                                                                                                  RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.Invoice_No), dbo.View_QryW3_WthPrjId_WithVAT.Invoice_Date, 
                                                                                                                                                  dbo.View_QryW3_WthPrjId_WithVAT.OrderValueRuble, 
                                                                                                                                                  RTRIM(dbo.View_QryW3_WthPrjId_WithVAT.PO_Currency), 
                                                                                                                                                  dbo.View_QryW3_WthPrjId_WithVAT.PaymentDate, dbo.View_QryW3_WthPrjId_WithVAT.RublePaidWithVAT, 
                                                                                                                                                  dbo.View_QryW3_WithVAT.BalanceRubleWithVAT - (CASE WHEN View_QryW3_WithVAT.RublePendingWithVAT
                                                                                                                                                   IS NULL THEN 0 ELSE View_QryW3_WithVAT.RublePendingWithVAT END), 
                                                                                                                                                  dbo.View_Projects_Balance_Pending_wthPending_WithVAT.DifferenceInRuble, 
                                                                                                                                                  dbo.View_QryW3_WithVAT.BalanceRubleWithVAT, dbo.View_QryW3_WithVAT.RublePendingWithVAT, 
                                                                                                                                                  dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectID, 
                                                                                                                                                  dbo.View_QryW3_WithVAT.RublePaidWithVAT
                                                                                                                           HAVING      (dbo.View_QryW3_WithVAT.BalanceRubleWithVAT &lt;&gt; 0) AND 
                                                                                                                                                  (dbo.View_Projects_Balance_Pending_wthPending_WithVAT.ProjectID = @ProjectID)
                                                                                                                           ORDER BY SupplierName, PO_No END</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Currency">
            <Value>=Parameters!Currency.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="DataSet2">
      <Fields>
        <Field Name="ProjectName">
          <DataField>ProjectName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PoTotalWithVAT">
          <DataField>PoTotalWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="BalanceWithVAT">
          <DataField>BalanceWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PendingWithVAT">
          <DataField>PendingWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DifferenceIn">
          <DataField>DifferenceIn</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>IF @Currency = N'Euro'

BEGIN
	SELECT     TOP (100) PERCENT ProjectName, SUM(PoTotalEuroWithVAT) AS PoTotalWithVAT, SUM(BalanceEuroWithVAT) AS BalanceWithVAT, SUM(EuroPendingWithVAT) 
						  AS PendingWithVAT, SUM(DifferenceInEuro) AS DifferenceIn
	FROM         dbo.View_Projects_Balance_Pending_wthPending_WithVAT
	WHERE     (ProjectID = @ProjectId)
	GROUP BY ProjectName
END

IF @Currency = N'Dollar'

BEGIN
	SELECT ProjectName, SUM(PoTotalDollarWithVAT) AS PoTotalWithVAT, SUM(BalanceDollarWithVAT) AS BalanceWithVAT,
					SUM(DollarPendingWithVAT) AS PendingWithVAT, SUM(DifferenceInDollar) AS DifferenceIn
	FROM         dbo.View_Projects_Balance_Pending_wthPending_WithVAT
	WHERE ProjectId = @ProjectId
	GROUP BY ProjectName
END

IF @Currency = N'Rub'

BEGIN
	SELECT ProjectName, SUM(PoTotalRubleWithVAT) AS PoTotalWithVAT, SUM(BalanceRubleWithVAT) AS BalanceWithVAT,
					SUM(RublePendingWithVAT) AS PendingWithVAT, SUM(DifferenceInRuble) AS DifferenceIn
	FROM         dbo.View_Projects_Balance_Pending_wthPending_WithVAT
	WHERE ProjectId = @ProjectId
	GROUP BY ProjectName
END</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProjectId">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Currency">
            <Value>=Parameters!Currency.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
