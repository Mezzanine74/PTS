                      AS [Contract No], dbo.Table_Contracts.ContractDate AS [Contract Date], RTRIM(dbo.Table_Contracts.ContractDescription) AS Description, 
                      dbo.Table_Contracts.ContractValue_withVAT, dbo.Table_Contracts.ContractCurrency, 
                      CASE WHEN SignBySupplier = 1 THEN N'YES' ELSE N'NO' END AS [Signed By Supplier], 
                      CASE WHEN SignByMercury = 1 THEN N'YES' ELSE N'NO' END AS [Signed By Mercury], 
                      CASE WHEN FrameContract = 1 THEN N'YES' ELSE N'NO' END AS [Frame Contract], ISNULL(dbo.Table_Contracts.PO_No, N'') AS [PO No], 
                      ISNULL(dbo.Table_Contracts.Advance, 0) AS Advance, ISNULL(dbo.Table_Contracts.Interim, 0) AS Interim, ISNULL(dbo.Table_Contracts.Shipment, 0) AS Shipment, 
                      ISNULL(dbo.Table_Contracts.Delivery, 0) AS Delivery, ISNULL(dbo.Table_Contracts.Retention, 0) AS Retention, 
                      ISNULL(dbo.View_QryW3_WithVAT.PoTotalEuroWithVAT, 0) AS [Po Total With VAT in Euro], ISNULL(dbo.View_QryW2_WithVAT.EuroPaidWithVAT, 0) 
                      AS [Paid With VAT in Euro], CONVERT(numeric(12, 2), ISNULL(dbo.View_QryW1_WithVAT.EuroPendingWithVAT, 0)) AS [Pending With VAT in Euro], 
                      CASE WHEN Exceptional = 1 THEN N'YES' ELSE N'NO' END AS [Exceptionally Approved]
FROM         dbo.Table_Contracts INNER JOIN
                      dbo.Table6_Supplier ON dbo.Table_Contracts.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
                      dbo.Table1_Project ON dbo.Table_Contracts.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
                      dbo.View_QryW3_WithVAT ON dbo.Table_Contracts.PO_No = dbo.View_QryW3_WithVAT.PO_No LEFT OUTER JOIN
                      dbo.View_QryW1_WithVAT ON dbo.Table_Contracts.PO_No = dbo.View_QryW1_WithVAT.PO_No LEFT OUTER JOIN
                      dbo.View_QryW2_WithVAT ON dbo.Table_Contracts.PO_No = dbo.View_QryW2_WithVAT.PO_No
WHERE     (dbo.Table_Contracts.ProjectID = @ProjectID) AND (SignBySupplier = 1) AND SignByMercury = 1

UNION ALL

