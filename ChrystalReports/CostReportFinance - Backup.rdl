  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="ProjectID">
          <DataField>ProjectID</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="CostCode">
          <DataField>CostCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CostCodeDesc">
          <DataField>CostCodeDesc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Budget">
          <DataField>Budget</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PlannedToSpend">
          <DataField>PlannedToSpend</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PoTotalEuroExcVAT">
          <DataField>PoTotalEuroExcVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DeletePossible">
          <DataField>DeletePossible</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ActualDeliveredEuro">
          <DataField>ActualDeliveredEuro</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CompletionPercent">
          <DataField>CompletionPercent</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CostVidisionID">
          <DataField>CostVidisionID</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="UpdatedPlannedRevenue">
          <DataField>UpdatedPlannedRevenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT [ProjectID]
      ,[CostCode]
      ,[CostCodeDesc]
      ,[Budget]
      ,[PlannedToSpend]
      ,[PoTotalEuroExcVAT]
      ,[DeletePossible]
      ,[ActualDeliveredEuro]
      ,[CompletionPercent]
      ,[CostVidisionID]
      ,UpdatedPlannedRevenue
 FROM (
SELECT     dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.ProjectID, RTRIM(dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.CostCode) 
                      AS CostCode, RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCodeDesc, CASE WHEN Budget IS NULL THEN 0 ELSE Budget END AS Budget, 
                      CASE WHEN PlannedToSpend IS NULL THEN 0 ELSE PlannedToSpend END AS PlannedToSpend, 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.PoTotalEuroExcVAT, CASE WHEN PoTotalEuroExcVAT IS NULL 
                      THEN 1 ELSE 0 END AS DeletePossible, dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.ActualDeliveredEuro, 
                      CASE WHEN CompletionPercent IS NULL THEN 0 ELSE CompletionPercent END AS CompletionPercent, dbo.Table7_CostDivision.CostVidisionID, 
                      CASE WHEN UpdatedPlannedRevenue IS NULL THEN 0 ELSE UpdatedPlannedRevenue END AS UpdatedPlannedRevenue
FROM         dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal INNER JOIN
                      dbo.Table7_CostCode ON dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.CostCode = dbo.Table7_CostCode.CostCode LEFT OUTER JOIN
                      dbo.Table7_CostDivision ON dbo.Table7_CostCode.CostVidisionID = dbo.Table7_CostDivision.CostVidisionID LEFT OUTER JOIN
                      dbo.Table_Budget ON dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.Currency = dbo.Table_Budget.Currency AND 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.ProjectID = dbo.Table_Budget.ProjectID AND 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.CostCode = dbo.Table_Budget.CostCode LEFT OUTER JOIN
                      dbo.Table_ProgressPercent ON dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.ProjectID = dbo.Table_ProgressPercent.ProjectID AND 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.CostCode = dbo.Table_ProgressPercent.CostCode
                      
UNION ALL

SELECT     dbo.Table_Budget.ProjectID, RTRIM(dbo.Table_Budget.CostCode) AS CostCode, RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCodeDesc, 
                      CASE WHEN Budget IS NULL THEN 0 ELSE Budget END AS Budget, CASE WHEN PlannedToSpend IS NULL 
                      THEN 0 ELSE PlannedToSpend END AS PlannedToSpend, 0 AS PoTotal, CASE WHEN PoTotalEuroExcVAT IS NULL 
                      THEN 1 ELSE 0 END AS DeletePossible, 0 AS TotalCollected, CASE WHEN CompletionPercent IS NULL 
                      THEN 0 ELSE CompletionPercent END AS CompletionPercent, dbo.Table7_CostDivision.CostVidisionID, CASE WHEN UpdatedPlannedRevenue IS NULL 
                      THEN 0 ELSE UpdatedPlannedRevenue END AS UpdatedPlannedRevenue
FROM         dbo.Table7_CostCode INNER JOIN
                      dbo.Table_Budget ON dbo.Table7_CostCode.CostCode = dbo.Table_Budget.CostCode LEFT OUTER JOIN
                      dbo.Table7_CostDivision ON dbo.Table7_CostCode.CostVidisionID = dbo.Table7_CostDivision.CostVidisionID LEFT OUTER JOIN
                      dbo.Table_ProgressPercent ON dbo.Table_Budget.ProjectID = dbo.Table_ProgressPercent.ProjectID AND 
                      dbo.Table_Budget.CostCode = dbo.Table_ProgressPercent.CostCode LEFT OUTER JOIN
                      dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal ON 
                      dbo.Table_Budget.ProjectID = dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.ProjectID AND 
                      dbo.Table_Budget.CostCode = dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.CostCode
WHERE     (dbo.Table_Budget.Currency = N'Euro') AND (dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.ProjectID IS NULL) AND 
                      (dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal.CostCode IS NULL)
                      ) AS DataSource1
                      WHERE ProjectID = @ProjectID
                      ORDER BY CostVidisionID, CostCode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
