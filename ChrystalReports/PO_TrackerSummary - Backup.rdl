        <Field Name="ContractCurrency">
          <DataField>ContractCurrency</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OriginalContractAmount">
          <DataField>OriginalContractAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CurrentContractValue">
          <DataField>CurrentContractValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="MarginBefore1PerSnagging">
          <DataField>MarginBefore1PerSnagging</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalAllowableSpend">
          <DataField>TotalAllowableSpend</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CurrentDateSpend">
          <DataField>CurrentDateSpend</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PercentCommitted">
          <DataField>PercentCommitted</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     TOP (100) PERCENT RTRIM(ProjectName) AS ProjectName, CompletionDate, RTRIM(ContractCurrency) AS ContractCurrency, 
                      ContractAmount AS OriginalContractAmount, CASE WHEN
                          (SELECT     SUM(AddendumAmount) AS TotalAddendum
                            FROM          dbo.Table_ProjectAddendum
                            WHERE      (Table_ProjectAddendum.ProjectID = dbo.Table1_Project.ProjectID)) IS NULL THEN 0 ELSE
                          (SELECT     SUM(AddendumAmount) AS TotalAddendum
                            FROM          dbo.Table_ProjectAddendum
                            WHERE      (Table_ProjectAddendum.ProjectID = dbo.Table1_Project.ProjectID)) END + ContractAmount AS CurrentContractValue, 
                      Margin AS MarginBefore1PerSnagging, (CASE WHEN
                          (SELECT     SUM(AddendumAmount) AS TotalAddendum
                            FROM          dbo.Table_ProjectAddendum
                            WHERE      (Table_ProjectAddendum.ProjectID = dbo.Table1_Project.ProjectID)) IS NULL THEN 0 ELSE
                          (SELECT     SUM(AddendumAmount) AS TotalAddendum
                            FROM          dbo.Table_ProjectAddendum
                            WHERE      (Table_ProjectAddendum.ProjectID = dbo.Table1_Project.ProjectID)) END + ContractAmount) * (100 - Margin) / 100 AS TotalAllowableSpend,
                          (SELECT     CASE WHEN rtrim(Table1_Project.ContractCurrency) = 'Rub' THEN OverallPoTotalRubleExcVAT WHEN rtrim(Table1_Project.ContractCurrency) 
                                                   = 'Dollar' THEN OverallPoTotalDollarExcVAT WHEN rtrim(Table1_Project.ContractCurrency) = 'Euro' THEN OverallPoTotalEuroExcVAT END AS Spend
                            FROM          dbo.Table_FollowUpReportSummary
                            WHERE      (DayOfRun = CONVERT(datetime, CONVERT(nchar(4), YEAR(GETDATE())) + '-' + CASE WHEN len(rtrim(CONVERT(nchar(2), Month(GETDATE())))) 
                                                   = 2 THEN rtrim(CONVERT(nchar(2), Month(GETDATE()))) ELSE '0' + rtrim(CONVERT(nchar(2), Month(GETDATE()))) 
                                                   END + '-' + CASE WHEN len(rtrim(CONVERT(nchar(2), Day(GETDATE())))) = 2 THEN rtrim(CONVERT(nchar(2), Day(GETDATE()))) 
                                                   ELSE '0' + rtrim(CONVERT(nchar(2), Day(GETDATE()))) END + ' 00:00:00')) AND (ProjectID = dbo.Table1_Project.ProjectID)) AS CurrentDateSpend,
                          (SELECT     CASE WHEN rtrim(Table1_Project.ContractCurrency) = 'Rub' THEN OverallPoTotalRubleExcVAT WHEN rtrim(Table1_Project.ContractCurrency) 
                                                   = 'Dollar' THEN OverallPoTotalDollarExcVAT WHEN rtrim(Table1_Project.ContractCurrency) = 'Euro' THEN OverallPoTotalEuroExcVAT END AS Spend
                            FROM          dbo.Table_FollowUpReportSummary AS Table_FollowUpReportSummary_1
                            WHERE      (DayOfRun = CONVERT(datetime, CONVERT(nchar(4), YEAR(GETDATE())) + '-' + CASE WHEN len(rtrim(CONVERT(nchar(2), Month(GETDATE())))) 
                                                   = 2 THEN rtrim(CONVERT(nchar(2), Month(GETDATE()))) ELSE '0' + rtrim(CONVERT(nchar(2), Month(GETDATE()))) 
                                                   END + '-' + CASE WHEN len(rtrim(CONVERT(nchar(2), Day(GETDATE())))) = 2 THEN rtrim(CONVERT(nchar(2), Day(GETDATE()))) 
                                                   ELSE '0' + rtrim(CONVERT(nchar(2), Day(GETDATE()))) END + ' 00:00:00')) AND (ProjectID = dbo.Table1_Project.ProjectID)) / ((CASE WHEN
                          (SELECT     SUM(AddendumAmount) AS TotalAddendum
                            FROM          dbo.Table_ProjectAddendum
                            WHERE      (Table_ProjectAddendum.ProjectID = dbo.Table1_Project.ProjectID)) IS NULL THEN 0 ELSE
                          (SELECT     SUM(AddendumAmount) AS TotalAddendum
                            FROM          dbo.Table_ProjectAddendum
                            WHERE      (Table_ProjectAddendum.ProjectID = dbo.Table1_Project.ProjectID)) END + ContractAmount) * (100 - Margin) / 100) AS PercentCommitted
FROM         dbo.Table1_Project
WHERE     (ContractCurrency IS NOT NULL) AND (ContractAmount IS NOT NULL)
ORDER BY ProjectName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
</Report>