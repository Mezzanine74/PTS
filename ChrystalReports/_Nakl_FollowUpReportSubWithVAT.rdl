      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>

			DECLARE @ProjectID_performance int
			SET @ProjectID_performance = @ProjectID

			DECLARE @Currency_Performance nVarChar(10)
			SET @Currency_Performance  = @Currency

			DECLARE @CostCode_Performance nVarChar(10)
			SET @CostCode_Performance  = @CostCode

			IF @CostCode_Performance = N'osman'
			SET @CostCode_Performance = N''

			IF @Currency_Performance = N'Euro'

			SELECT
			ProjectID
			,CostCode
			,PO_No
			,Invoice_No
			,Invoice_Date
			,InvoiceValue
			,PO_Currency
			,SiteRecordNo
			,PaymentDate
			,SupplierName
			,Description
			,PendingWithVAT
			,PaidWithVAT
			,VATpaid
			,OrderValue
			,Coeff	FROM (
			SELECT     dbo.Table1_Project.ProjectID, RTRIM(CONVERT(nchar(10), dbo.Table7_CostCode.CostCode)) + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription)
			AS CostCode, RTRIM(dbo.Table2_PONo.PO_No) AS PO_No, dbo.View_QryW3_PoDetail_WithVAT.Invoice_No, dbo.View_QryW3_PoDetail_WithVAT.Invoice_Date,
			dbo.View_QryW3_PoDetail_WithVAT.InvoiceValue, dbo.View_QryW3_PoDetail_WithVAT.PO_Currency, CASE WHEN RTRIM(dbo.View_QryW3_PoDetail_WithVAT.SiteRecordNo)
			IS NULL THEN N'' ELSE RTRIM(dbo.View_QryW3_PoDetail_WithVAT.SiteRecordNo) END AS SiteRecordNo, dbo.View_QryW3_PoDetail_WithVAT.PaymentDate,
			dbo.View_QryW3_PoDetail_WithVAT.SupplierName, dbo.View_QryW3_PoDetail_WithVAT.Description, CASE WHEN View_QryW3_PoDetail_WithVAT.EuroPendingWithVAT IS NULL
			THEN 0 ELSE View_QryW3_PoDetail_WithVAT.EuroPendingWithVAT END AS PendingWithVAT, CASE WHEN View_QryW3_PoDetail_WithVAT.EuroPaidWithVAT IS NULL
			THEN 0 ELSE View_QryW3_PoDetail_WithVAT.EuroPaidWithVAT END AS PaidWithVAT, CASE WHEN View_QryW3_PoDetail_WithVAT.VATpaidEuro IS NULL
			THEN 0 ELSE View_QryW3_PoDetail_WithVAT.VATpaidEuro END AS VATpaid, dbo.View_QryW3_PoDetail_WithVAT.OrderValueEuro AS OrderValue
			,Coeff
			FROM         dbo.Table8_ExchangeRates RIGHT OUTER JOIN
			dbo.Table4_PaymentRequest RIGHT OUTER JOIN
			dbo.View_QryW3_PoDetail_WithVAT ON dbo.Table4_PaymentRequest.InvoiceID = dbo.View_QryW3_PoDetail_WithVAT.InvoiceID ON
			dbo.Table8_ExchangeRates.Date = dbo.View_QryW3_PoDetail_WithVAT.PaymentDate RIGHT OUTER JOIN
			dbo.View_PoInvoiceNull INNER JOIN
			dbo.Table1_Project INNER JOIN
			dbo.Table2_PONo ON dbo.Table1_Project.ProjectID = dbo.Table2_PONo.Project_ID INNER JOIN
			dbo.Table7_CostCode ON dbo.Table2_PONo.CostCode = dbo.Table7_CostCode.CostCode ON
			dbo.View_PoInvoiceNull.PO_No = dbo.Table2_PONo.PO_No ON dbo.View_QryW3_PoDetail_WithVAT.PO_No = dbo.Table2_PONo.PO_No LEFT OUTER JOIN
			dbo.View_QryW3 ON dbo.Table2_PONo.PO_No = dbo.View_QryW3.PO_No CROSS JOIN
			dbo.View_MaxExchangeRate
			LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = Table2_PONo.PO_No
			WHERE     (dbo.View_PoInvoiceNull.IfNull = 'not null')

			UNION ALL

			SELECT     ProjectID, CostCode, View_QryOpenPosInEuro_WithVAT.PO_No AS PO_No, NULL AS Invoice_No, NULL AS Invoice_Date, NULL AS InvoiceValue, PO_Currency, NULL AS SiteRecordNo, NULL
			AS PaymentDate, SupplierName, Description, NULL AS PendingWithVAT, NULL AS PaidWithVAT, NULL AS VATpaid, Open_Euro AS OrderValue
			,Coeff
			FROM         dbo.View_QryOpenPosInEuro_WithVAT
			LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = View_QryOpenPosInEuro_WithVAT.PO_No

			UNION ALL

			SELECT     Table1_Project_1.ProjectID, RTRIM(CONVERT(nchar(10), Table7_CostCode_1.CostCode)) + N' - ' + RTRIM(Table7_CostCode_1.CodeDescription)
			AS CostCode, View_PoInvoiceNull_1.PO_No, NULL AS Invoice_No, NULL AS Invoice_Date, NULL AS InvoiceValue, Table2_PONo_1.PO_Currency, NULL
			AS SiteRecordNo, NULL AS PaymentDate, RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName, RTRIM(Table2_PONo_1.Description)
			AS Description, NULL AS PendingWithVAT, NULL AS PaidWithVAT, NULL AS VATpaid, View_QryW3_WithVAT_1.PoTotalEuroWithVAT AS OrderValue
			,Coeff
			FROM         dbo.Table2_PONo AS Table2_PONo_1 INNER JOIN
			dbo.Table1_Project AS Table1_Project_1 ON Table2_PONo_1.Project_ID = Table1_Project_1.ProjectID INNER JOIN
			dbo.Table6_Supplier ON Table2_PONo_1.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
			dbo.Table7_CostCode AS Table7_CostCode_1 ON Table2_PONo_1.CostCode = Table7_CostCode_1.CostCode INNER JOIN
			dbo.View_PoInvoiceNull AS View_PoInvoiceNull_1 ON Table2_PONo_1.PO_No = View_PoInvoiceNull_1.PO_No INNER JOIN
			dbo.View_QryW3_WithVAT AS View_QryW3_WithVAT_1 ON View_PoInvoiceNull_1.PO_No = View_QryW3_WithVAT_1.PO_No CROSS JOIN
			dbo.View_MaxExchangeRate AS View_MaxExchangeRate_1
			LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = Table2_PONo_1.PO_No
			WHERE     (View_PoInvoiceNull_1.IfNull = 'null') AND (View_QryW3_WithVAT_1.PoTotalEuroWithVAT &lt;&gt; 0)
			) AS DataSource_FollowUpReportSubEuroWithVAT

			WHERE     (ProjectID = @ProjectID_Performance) AND (CostCode LIKE '%' + @CostCode_Performance + '%')
			ORDER BY CostCode, PO_No


			IF @Currency_Performance = N'Dollar'

			SELECT
			ProjectID
			,CostCode
			,PO_No
			,Invoice_No
			,Invoice_Date
			,InvoiceValue
			,PO_Currency
			,SiteRecordNo
			,PaymentDate
			,SupplierName
			,Description
			,PendingWithVAT
			,PaidWithVAT
			,VATpaid
			,OrderValue
			,Coeff	FROM (
			SELECT     dbo.Table1_Project.ProjectID, RTRIM(CONVERT(nchar(10), dbo.Table7_CostCode.CostCode)) + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription)
			AS CostCode, RTRIM(dbo.Table2_PONo.PO_No) AS PO_No, dbo.View_QryW3_PoDetail_WithVAT.Invoice_No, dbo.View_QryW3_PoDetail_WithVAT.Invoice_Date,
			dbo.View_QryW3_PoDetail_WithVAT.InvoiceValue, dbo.View_QryW3_PoDetail_WithVAT.PO_Currency, CASE WHEN RTRIM(dbo.View_QryW3_PoDetail_WithVAT.SiteRecordNo)
			IS NULL THEN N'' ELSE RTRIM(dbo.View_QryW3_PoDetail_WithVAT.SiteRecordNo) END AS SiteRecordNo, dbo.View_QryW3_PoDetail_WithVAT.PaymentDate,
			dbo.View_QryW3_PoDetail_WithVAT.SupplierName, dbo.View_QryW3_PoDetail_WithVAT.Description, CASE WHEN View_QryW3_PoDetail_WithVAT.DollarPendingWithVAT IS NULL
			THEN 0 ELSE View_QryW3_PoDetail_WithVAT.DollarPendingWithVAT END AS PendingWithVAT, CASE WHEN View_QryW3_PoDetail_WithVAT.DollarPaidWithVAT IS NULL
			THEN 0 ELSE View_QryW3_PoDetail_WithVAT.DollarPaidWithVAT END AS PaidWithVAT, CASE WHEN View_QryW3_PoDetail_WithVAT.VATpaidDollar IS NULL
			THEN 0 ELSE View_QryW3_PoDetail_WithVAT.VATpaidDollar END AS VATpaid, dbo.View_QryW3_PoDetail_WithVAT.OrderValueDollar AS OrderValue
			,Coeff
			FROM         dbo.Table8_ExchangeRates RIGHT OUTER JOIN
			dbo.Table4_PaymentRequest RIGHT OUTER JOIN
			dbo.View_QryW3_PoDetail_WithVAT ON dbo.Table4_PaymentRequest.InvoiceID = dbo.View_QryW3_PoDetail_WithVAT.InvoiceID ON
			dbo.Table8_ExchangeRates.Date = dbo.View_QryW3_PoDetail_WithVAT.PaymentDate RIGHT OUTER JOIN
			dbo.View_PoInvoiceNull INNER JOIN
			dbo.Table1_Project INNER JOIN
			dbo.Table2_PONo ON dbo.Table1_Project.ProjectID = dbo.Table2_PONo.Project_ID INNER JOIN
			dbo.Table7_CostCode ON dbo.Table2_PONo.CostCode = dbo.Table7_CostCode.CostCode ON
			dbo.View_PoInvoiceNull.PO_No = dbo.Table2_PONo.PO_No ON dbo.View_QryW3_PoDetail_WithVAT.PO_No = dbo.Table2_PONo.PO_No LEFT OUTER JOIN
			dbo.View_QryW3 ON dbo.Table2_PONo.PO_No = dbo.View_QryW3.PO_No CROSS JOIN
			dbo.View_MaxExchangeRate
			LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = Table2_PONo.PO_No
			WHERE     (dbo.View_PoInvoiceNull.IfNull = 'not null')

			UNION ALL

			SELECT     ProjectID, CostCode, View_QryOpenPosInDollar_WithVAT.PO_No AS PO_No, NULL AS Invoice_No, NULL AS Invoice_Date, NULL AS InvoiceValue, PO_Currency, NULL AS SiteRecordNo, NULL
			AS PaymentDate, SupplierName, Description, NULL AS PendingWithVAT, NULL AS PaidWithVAT, NULL AS VATpaid, Open_Dollar AS OrderValue
			,Coeff
			FROM         dbo.View_QryOpenPosInDollar_WithVAT
			LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = View_QryOpenPosInDollar_WithVAT.PO_No

			UNION ALL

			SELECT     Table1_Project_1.ProjectID, RTRIM(CONVERT(nchar(10), Table7_CostCode_1.CostCode)) + N' - ' + RTRIM(Table7_CostCode_1.CodeDescription)
			AS CostCode, View_PoInvoiceNull_1.PO_No, NULL AS Invoice_No, NULL AS Invoice_Date, NULL AS InvoiceValue, Table2_PONo_1.PO_Currency, NULL
			AS SiteRecordNo, NULL AS PaymentDate, RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName, RTRIM(Table2_PONo_1.Description)
			AS Description, NULL AS PendingWithVAT, NULL AS PaidWithVAT, NULL AS VATpaid, View_QryW3_WithVAT_1.PoTotalDollarWithVAT AS OrderValue
			,Coeff
			FROM         dbo.Table2_PONo AS Table2_PONo_1 INNER JOIN
			dbo.Table1_Project AS Table1_Project_1 ON Table2_PONo_1.Project_ID = Table1_Project_1.ProjectID INNER JOIN
			dbo.Table6_Supplier ON Table2_PONo_1.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
			dbo.Table7_CostCode AS Table7_CostCode_1 ON Table2_PONo_1.CostCode = Table7_CostCode_1.CostCode INNER JOIN
			dbo.View_PoInvoiceNull AS View_PoInvoiceNull_1 ON Table2_PONo_1.PO_No = View_PoInvoiceNull_1.PO_No INNER JOIN
			dbo.View_QryW3_WithVAT AS View_QryW3_WithVAT_1 ON View_PoInvoiceNull_1.PO_No = View_QryW3_WithVAT_1.PO_No CROSS JOIN
			dbo.View_MaxExchangeRate AS View_MaxExchangeRate_1
			LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = Table2_PONo_1.PO_No
			WHERE     (View_PoInvoiceNull_1.IfNull = 'null') AND (View_QryW3_WithVAT_1.PoTotalDollarWithVAT &lt;&gt; 0)
			) AS DataSource_FollowUpReportSubDollarWithVAT

			WHERE     (ProjectID = @ProjectID_Performance) AND (CostCode LIKE '%' + @CostCode_Performance + '%')
			ORDER BY CostCode, PO_No

			IF @Currency_Performance = N'Ruble'

			SELECT
			ProjectID
			,CostCode
			,PO_No
			,Invoice_No
			,Invoice_Date
			,InvoiceValue
			,PO_Currency
			,SiteRecordNo
			,PaymentDate
			,SupplierName
			,Description
			,PendingWithVAT
			,PaidWithVAT
			,VATpaid
			,OrderValue
			,Coeff	FROM (
			SELECT     dbo.Table1_Project.ProjectID, RTRIM(CONVERT(nchar(10), dbo.Table7_CostCode.CostCode)) + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription)
			AS CostCode, RTRIM(dbo.Table2_PONo.PO_No) AS PO_No, dbo.View_QryW3_PoDetail_WithVAT.Invoice_No, dbo.View_QryW3_PoDetail_WithVAT.Invoice_Date,
			dbo.View_QryW3_PoDetail_WithVAT.InvoiceValue, dbo.View_QryW3_PoDetail_WithVAT.PO_Currency, CASE WHEN RTRIM(dbo.View_QryW3_PoDetail_WithVAT.SiteRecordNo)
			IS NULL THEN N'' ELSE RTRIM(dbo.View_QryW3_PoDetail_WithVAT.SiteRecordNo) END AS SiteRecordNo, dbo.View_QryW3_PoDetail_WithVAT.PaymentDate,
			dbo.View_QryW3_PoDetail_WithVAT.SupplierName, dbo.View_QryW3_PoDetail_WithVAT.Description, CASE WHEN View_QryW3_PoDetail_WithVAT.RublePendingWithVAT IS NULL
			THEN 0 ELSE View_QryW3_PoDetail_WithVAT.RublePendingWithVAT END AS PendingWithVAT, CASE WHEN View_QryW3_PoDetail_WithVAT.RublePaidWithVAT IS NULL
			THEN 0 ELSE View_QryW3_PoDetail_WithVAT.RublePaidWithVAT END AS PaidWithVAT, CASE WHEN View_QryW3_PoDetail_WithVAT.VATpaidRuble IS NULL
			THEN 0 ELSE View_QryW3_PoDetail_WithVAT.VATpaidRuble END AS VATpaid, dbo.View_QryW3_PoDetail_WithVAT.OrderValueRuble AS OrderValue
			,Coeff
			FROM         dbo.Table8_ExchangeRates RIGHT OUTER JOIN
			dbo.Table4_PaymentRequest RIGHT OUTER JOIN
			dbo.View_QryW3_PoDetail_WithVAT ON dbo.Table4_PaymentRequest.InvoiceID = dbo.View_QryW3_PoDetail_WithVAT.InvoiceID ON
			dbo.Table8_ExchangeRates.Date = dbo.View_QryW3_PoDetail_WithVAT.PaymentDate RIGHT OUTER JOIN
			dbo.View_PoInvoiceNull INNER JOIN
			dbo.Table1_Project INNER JOIN
			dbo.Table2_PONo ON dbo.Table1_Project.ProjectID = dbo.Table2_PONo.Project_ID INNER JOIN
			dbo.Table7_CostCode ON dbo.Table2_PONo.CostCode = dbo.Table7_CostCode.CostCode ON
			dbo.View_PoInvoiceNull.PO_No = dbo.Table2_PONo.PO_No ON dbo.View_QryW3_PoDetail_WithVAT.PO_No = dbo.Table2_PONo.PO_No LEFT OUTER JOIN
			dbo.View_QryW3 ON dbo.Table2_PONo.PO_No = dbo.View_QryW3.PO_No CROSS JOIN
			dbo.View_MaxExchangeRate
			LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = Table2_PONo.PO_No
			WHERE     (dbo.View_PoInvoiceNull.IfNull = 'not null')

			UNION ALL

			SELECT     ProjectID, CostCode, View_QryOpenPosInRuble_WithVAT.PO_No AS PO_No, NULL AS Invoice_No, NULL AS Invoice_Date, NULL AS InvoiceValue, PO_Currency, NULL AS SiteRecordNo, NULL
			AS PaymentDate, SupplierName, Description, NULL AS PendingWithVAT, NULL AS PaidWithVAT, NULL AS VATpaid, Open_Ruble AS OrderValue
			,Coeff
			FROM         dbo.View_QryOpenPosInRuble_WithVAT
			LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = View_QryOpenPosInRuble_WithVAT.PO_No

			UNION ALL

			SELECT     Table1_Project_1.ProjectID, RTRIM(CONVERT(nchar(10), Table7_CostCode_1.CostCode)) + N' - ' + RTRIM(Table7_CostCode_1.CodeDescription)
			AS CostCode, View_PoInvoiceNull_1.PO_No, NULL AS Invoice_No, NULL AS Invoice_Date, NULL AS InvoiceValue, Table2_PONo_1.PO_Currency, NULL
			AS SiteRecordNo, NULL AS PaymentDate, RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName, RTRIM(Table2_PONo_1.Description)
			AS Description, NULL AS PendingWithVAT, NULL AS PaidWithVAT, NULL AS VATpaid, View_QryW3_WithVAT_1.PoTotalRubleWithVAT AS OrderValue
			,Coeff
			FROM         dbo.Table2_PONo AS Table2_PONo_1 INNER JOIN
			dbo.Table1_Project AS Table1_Project_1 ON Table2_PONo_1.Project_ID = Table1_Project_1.ProjectID INNER JOIN
			dbo.Table6_Supplier ON Table2_PONo_1.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
			dbo.Table7_CostCode AS Table7_CostCode_1 ON Table2_PONo_1.CostCode = Table7_CostCode_1.CostCode INNER JOIN
			dbo.View_PoInvoiceNull AS View_PoInvoiceNull_1 ON Table2_PONo_1.PO_No = View_PoInvoiceNull_1.PO_No INNER JOIN
			dbo.View_QryW3_WithVAT AS View_QryW3_WithVAT_1 ON View_PoInvoiceNull_1.PO_No = View_QryW3_WithVAT_1.PO_No CROSS JOIN
			dbo.View_MaxExchangeRate AS View_MaxExchangeRate_1
			LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = Table2_PONo_1.PO_No
			WHERE     (View_PoInvoiceNull_1.IfNull = 'null') AND (View_QryW3_WithVAT_1.PoTotalRubleWithVAT &lt;&gt; 0)
			) AS DataSource_FollowUpReportSubRubleWithVAT

			WHERE     (ProjectID = @ProjectID_Performance) AND (CostCode LIKE '%' + @CostCode_Performance + '%')
			ORDER BY CostCode, PO_No</CommandText>
        <QueryParameters>
