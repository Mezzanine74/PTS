      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
			SELECT [CostCode]
			,[CostCodeDesc]
			,[Budget]
			,[PlannedToSpend]
			,[PoTotalEuroExcVAT]
			,[DeletePossible]
			,[ActualDeliveredEuro]
			,[CompletionPercent]
			,[CostDivisionDescription]
			,costvidisionID
			,UpdatedPlannedRevenue
			,ProjectName
			,ProjectID
			,OriginalBOQ
			,CountOfComment
			FROM (

			SELECT     RTRIM(dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.CostCode) AS CostCode, RTRIM(dbo.Table7_CostCode.CodeDescription)
			AS CostCodeDesc, CASE WHEN Budget IS NULL THEN 0 ELSE Budget END AS Budget, ((CASE WHEN PlannedToSpend IS NULL
			THEN 0 ELSE PlannedToSpend END) + (CASE WHEN PlannedToSpendCO IS NULL
			THEN 0 ELSE PlannedToSpendCO END)) AS PlannedToSpend, dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.PoTotalEuroExcVAT,
			CASE WHEN PoTotalEuroExcVAT IS NULL THEN 1 ELSE 0 END AS DeletePossible,
			dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.ActualDeliveredEuro, CASE WHEN CompletionPercent IS NULL
			THEN 0 ELSE CompletionPercent END AS CompletionPercent, RTRIM(dbo.Table7_CostDivision.CostDivisionDescription) AS CostDivisionDescription,
			dbo.Table7_CostDivision.CostVidisionID, CASE WHEN UpdatedPlannedRevenue IS NULL
			THEN 0 ELSE UpdatedPlannedRevenue END AS UpdatedPlannedRevenue, RTRIM(dbo.Table1_Project.ProjectName) AS ProjectName,
			dbo.Table1_Project.ProjectID, CASE WHEN OriginalBOQ IS NULL THEN 0 ELSE OriginalBOQ END AS OriginalBOQ,
			case when CountOfComment is null then 0 else CountOfComment end AS CountOfComment
			FROM         dbo.View_CountOfCommentOnBudget RIGHT OUTER JOIN
			dbo.Table_Budget ON dbo.View_CountOfCommentOnBudget.BudgetID = dbo.Table_Budget.BudgetID RIGHT OUTER JOIN
			dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3 INNER JOIN
			dbo.Table7_CostCode ON dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.CostCode = dbo.Table7_CostCode.CostCode INNER JOIN
			dbo.Table1_Project ON dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.Table7_CostDivision ON dbo.Table7_CostCode.CostVidisionID = dbo.Table7_CostDivision.CostVidisionID ON
			dbo.Table_Budget.Currency = dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.Currency AND
			dbo.Table_Budget.ProjectID = dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.ProjectID AND
			dbo.Table_Budget.CostCode = dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.CostCode LEFT OUTER JOIN
			dbo.Table_ProgressPercent ON dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.ProjectID = dbo.Table_ProgressPercent.ProjectID AND
			dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.CostCode = dbo.Table_ProgressPercent.CostCode
			WHERE     (dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.ProjectID = @ProjectID)

			UNION ALL

			SELECT     RTRIM(dbo.Table_Budget.CostCode) AS CostCode, RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCodeDesc, CASE WHEN Budget IS NULL
			THEN 0 ELSE Budget END AS Budget, ((CASE WHEN PlannedToSpend IS NULL
			THEN 0 ELSE PlannedToSpend END) + (CASE WHEN PlannedToSpendCO IS NULL
			THEN 0 ELSE PlannedToSpendCO END)) AS PlannedToSpend, 0 AS PoTotal,
			CASE WHEN PoTotalEuroExcVAT IS NULL THEN 1 ELSE 0 END AS DeletePossible, 0 AS TotalCollected, CASE WHEN CompletionPercent IS NULL
			THEN 0 ELSE CompletionPercent END AS CompletionPercent, RTRIM(dbo.Table7_CostDivision.CostDivisionDescription) AS CostDivisionDescription,
			dbo.Table7_CostDivision.CostVidisionID, CASE WHEN UpdatedPlannedRevenue IS NULL
			THEN 0 ELSE UpdatedPlannedRevenue END AS UpdatedPlannedRevenue, RTRIM(dbo.Table1_Project.ProjectName) AS ProjectName,
			dbo.Table1_Project.ProjectID, CASE WHEN OriginalBOQ IS NULL THEN 0 ELSE OriginalBOQ END AS OriginalBOQ,
			CASE WHEN CountOfComment IS NULL THEN 0 ELSE CountOfComment END AS CountOfComment
			FROM         dbo.Table7_CostCode INNER JOIN
			dbo.Table_Budget ON dbo.Table7_CostCode.CostCode = dbo.Table_Budget.CostCode INNER JOIN
			dbo.Table1_Project ON dbo.Table_Budget.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CountOfCommentOnBudget ON dbo.Table_Budget.BudgetID = dbo.View_CountOfCommentOnBudget.BudgetID LEFT OUTER JOIN
			dbo.Table7_CostDivision ON dbo.Table7_CostCode.CostVidisionID = dbo.Table7_CostDivision.CostVidisionID LEFT OUTER JOIN
			dbo.Table_ProgressPercent ON dbo.Table_Budget.ProjectID = dbo.Table_ProgressPercent.ProjectID AND
			dbo.Table_Budget.CostCode = dbo.Table_ProgressPercent.CostCode LEFT OUTER JOIN
			dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3 ON
			dbo.Table_Budget.ProjectID = dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.ProjectID AND
			dbo.Table_Budget.CostCode = dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.CostCode
			WHERE     (dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.ProjectID IS NULL) AND (dbo.Table_Budget.Currency = N'Euro') AND
			(dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREVFinal3.CostCode IS NULL) AND (dbo.Table_Budget.ProjectID = @ProjectID) AND
			(dbo.Table_Budget.CostCode NOT IN
			(SELECT     dbo.Table2_PONo.CostCode
			FROM          dbo.Table2_PONo INNER JOIN
			(SELECT     PO_No, SUM(TotalPrice) AS TotalPrice
			FROM          dbo.Table2_PONo_Sub
			GROUP BY PO_No) AS DataSource117 ON DataSource117.PO_No = dbo.Table2_PONo.PO_No AND
			dbo.Table2_PONo.TotalPrice = DataSource117.TotalPrice
			WHERE      (dbo.Table2_PONo.Project_ID = @ProjectID)
			GROUP BY dbo.Table2_PONo.CostCode))
			) AS DataSource1
		</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
