  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>DataSource1</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>616e1c8f-5fae-481e-84a7-35cc329ae84b</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Currency">
            <Value>=Parameters!Currency.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @Currency = N'Euro'

			SELECT
			ProjectID
			,CostCode
			,PO_No
			,Invoice_No
			,Invoice_Date
			,InvoiceValue
			,PO_Currency
			,SiteRecordNo
			,PaymentDate
			,SupplierName
			,Description
			,PendingExcVAT
			,PaidExcVAT
			,VATpaid
			,OrderValue
			,Coeff	FROM (
			SELECT     dbo.Table1_Project.ProjectID, RTRIM(CONVERT(nchar(20), dbo.Table7_CostCode.CostCode)) + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCode, 
								  RTRIM(dbo.Table2_PONo.PO_No) AS PO_No, dbo.View_QryW3_PoDetail.Invoice_No, dbo.View_QryW3_PoDetail.Invoice_Date, 
								  dbo.View_QryW3_PoDetail.InvoiceValue, dbo.View_QryW3_PoDetail.PO_Currency, CASE WHEN RTRIM(dbo.View_QryW3_PoDetail.SiteRecordNo) IS NULL 
								  THEN N'' ELSE RTRIM(dbo.View_QryW3_PoDetail.SiteRecordNo) END AS SiteRecordNo, dbo.View_QryW3_PoDetail.PaymentDate, 
								  dbo.View_QryW3_PoDetail.SupplierName, dbo.View_QryW3_PoDetail.Description, CASE WHEN View_QryW3_PoDetail.EuroPendingExcVAT IS NULL 
								  THEN 0 ELSE View_QryW3_PoDetail.EuroPendingExcVAT END AS PendingExcVAT, CASE WHEN View_QryW3_PoDetail.EuroPaidExcVAT IS NULL 
								  THEN 0 ELSE View_QryW3_PoDetail.EuroPaidExcVAT END AS PaidExcVAT, CASE WHEN View_QryW3_PoDetail.VATpaidEuro IS NULL 
								  THEN 0 ELSE View_QryW3_PoDetail.VATpaidEuro END AS VATpaid, dbo.View_QryW3_PoDetail.OrderValueEuro AS OrderValue, 
								  dbo.View_PO_TotalCollectedDocsCoeff.Coeff
			FROM         dbo.Table8_ExchangeRates RIGHT OUTER JOIN
								  dbo.Table4_PaymentRequest RIGHT OUTER JOIN
								  dbo.View_QryW3_PoDetail ON dbo.Table4_PaymentRequest.InvoiceID = dbo.View_QryW3_PoDetail.InvoiceID ON 
								  dbo.Table8_ExchangeRates.Date = dbo.View_QryW3_PoDetail.PaymentDate RIGHT OUTER JOIN
								  dbo.View_PoInvoiceNull INNER JOIN
								  dbo.Table1_Project INNER JOIN
								  dbo.Table2_PONo ON dbo.Table1_Project.ProjectID = dbo.Table2_PONo.Project_ID INNER JOIN
								  dbo.Table7_CostCode ON dbo.Table2_PONo.CostCode = dbo.Table7_CostCode.CostCode ON dbo.View_PoInvoiceNull.PO_No = dbo.Table2_PONo.PO_No ON 
								  dbo.View_QryW3_PoDetail.PO_No = dbo.Table2_PONo.PO_No LEFT OUTER JOIN
								  dbo.View_QryW3 ON dbo.Table2_PONo.PO_No = dbo.View_QryW3.PO_No CROSS JOIN
								  dbo.View_MaxExchangeRate LEFT OUTER JOIN
								  dbo.View_PO_TotalCollectedDocsCoeff ON dbo.View_PO_TotalCollectedDocsCoeff.PO_No = dbo.Table2_PONo.PO_No
			WHERE     (dbo.View_PoInvoiceNull.IfNull = 'not null') AND (dbo.Table2_PONo.SupplierID = @SupplierID) AND Table1_Project.ProjectID = @ProjectID

			UNION ALL

			SELECT     dbo.View_QryOpenPosInEuro.ProjectID, dbo.View_QryOpenPosInEuro.CostCode, dbo.View_QryOpenPosInEuro.PO_No AS PO_No, NULL AS Invoice_No, NULL 
								  AS Invoice_Date, NULL AS InvoiceValue, dbo.View_QryOpenPosInEuro.PO_Currency, NULL AS SiteRecordNo, NULL AS PaymentDate, 
								  dbo.View_QryOpenPosInEuro.SupplierName, dbo.View_QryOpenPosInEuro.Description, NULL AS PendingExcVAT, NULL AS PaidExcVAT, NULL AS VATpaid, 
								  dbo.View_QryOpenPosInEuro.Open_Euro AS OrderValue, dbo.View_PO_TotalCollectedDocsCoeff.Coeff
			FROM         dbo.View_QryOpenPosInEuro INNER JOIN
