  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="SupplierName">
          <DataField>SupplierName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SupplierID">
          <DataField>SupplierID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PO_No">
          <DataField>PO_No</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PO_Currency">
          <DataField>PO_Currency</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalPoTotalWithVAT">
          <DataField>TotalPoTotalWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalPaidWithVAT">
          <DataField>TotalPaidWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalCollectedDocsWithVAT">
          <DataField>TotalCollectedDocsWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     TOP (100) PERCENT RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName, dbo.Table6_Supplier.SupplierID, dbo.View_QryW3_WithVAT.PO_No, 
                      dbo.Table2_PONo.PO_Currency, 
                      SUM(CASE WHEN Table2_PONo.PO_Currency = N'Rub' THEN PoTotalRubleWithVAT WHEN Table2_PONo.PO_Currency = N'Euro' THEN PoTotalEuroWithVAT WHEN Table2_PONo.PO_Currency
                       = N'Dollar' THEN PoTotalDollarWithVAT END) AS TotalPoTotalWithVAT, 
                      SUM(CASE WHEN Table2_PONo.PO_Currency = N'Rub' THEN RublePaidWithVAT WHEN Table2_PONo.PO_Currency = N'Euro' THEN EuroPaidWithVAT WHEN Table2_PONo.PO_Currency
                       = N'Dollar' THEN DollarPaidWithVAT END) AS TotalPaidWithVAT, 
                      SUM((CASE WHEN Table2_PONo.PO_Currency = N'Rub' THEN PoTotalRubleWithVAT WHEN Table2_PONo.PO_Currency = N'Euro' THEN PoTotalEuroWithVAT WHEN Table2_PONo.PO_Currency
                       = N'Dollar' THEN PoTotalDollarWithVAT END) * dbo.View_PO_TotalCollectedDocsCoeff.Coeff) AS TotalCollectedDocsWithVAT
FROM         dbo.View_QryW3_WithVAT INNER JOIN
                      dbo.Table2_PONo ON dbo.View_QryW3_WithVAT.PO_No = dbo.Table2_PONo.PO_No INNER JOIN
                      dbo.Table6_Supplier ON dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
                      dbo.View_PO_TotalCollectedDocsCoeff ON dbo.View_QryW3_WithVAT.PO_No = dbo.View_PO_TotalCollectedDocsCoeff.PO_No
WHERE     (dbo.Table2_PONo.Project_ID = @ProjectID)
GROUP BY RTRIM(dbo.Table6_Supplier.SupplierName), dbo.Table6_Supplier.SupplierID, dbo.View_QryW3_WithVAT.PO_No, dbo.Table2_PONo.PO_Currency</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="ProjectID">
      <DataType>String</DataType>
      <Prompt>Project ID</Prompt>
    </ReportParameter>
  </ReportParameters>
</Report>