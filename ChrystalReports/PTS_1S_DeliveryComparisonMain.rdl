  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT ProjectID, SupplierID FROM (

	SELECT ProjectID, SupplierID 
	FROM View_DeliveryDocEntryPTS 

	UNION ALL

	SELECT [Номер проекта],[SupplierID]
	  FROM [dbo].[1SdeliveryDocEntries]
	WHERE [Номер проекта] &lt;&gt; 0
	) AS SourcePrj_Supplier
	GROUP BY ProjectID, SupplierID 
	HAVING ProjectID = @ProjectID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
