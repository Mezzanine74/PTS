      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>

			DECLARE @ProjectID_performance int
			SET @ProjectID_performance = @ProjectID

			DECLARE @Currency_Performance nVarChar(10)
			SET @Currency_Performance  = @Currency

			DECLARE @ProjectType nvarChar(20)

			DECLARE @ProjectID_Performance_Performance int
			DECLARE @Currency_Performance_Performance nVarChar(10)

			SET @ProjectID_Performance_Performance = @ProjectID_Performance
			SET @Currency_Performance_Performance = @Currency_Performance

			SET @ProjectType = (SELECT Type FROM Table1_Project WHERE ProjectID = @ProjectID_Performance_Performance)

			IF @Currency_Performance_Performance = N'Rub' AND @ProjectType = N'DataCenter'
			-- RUBLE dataCenter
			SELECT     TOP (100) PERCENT dbo.View_CostCodeSummary0_DCC.CostCode1, dbo.View_CostCodeSummary0_DCC.CostCode2, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary2.VATpaidRuble) AS VATpaid, dbo.View_Project_VATrec_outs.OutstandingVATRuble AS OutstandingVAT,
			dbo.View_Project_VATrec_outs.VATRecoveredRuble AS VATRecovered,
			dbo.View_CostCodeSummary0_DCC.TotalRublePendingExcVAT AS TotalPendingExcVAT,
			dbo.View_CostCodeSummary0_DCC.TotalRublePaidExcVAT AS TotalPaidExcVAT,
			dbo.View_CostCodeSummary0_DCC.TotalPoTotalRubleExcVAT AS TotalPoTotalExcVAT,
			dbo.View_CostCodeSummary0_DCC.TotalBalanceRubleExcVAT AS TotalBalanceExcVAT,
			dbo.View_CostCodeSummary0_DCC.TotalCollectedDocRubleExcVAT AS TotalCollected
			FROM         dbo.View_CostCodeSummary2 RIGHT OUTER JOIN
			dbo.View_Project_VATrec_outs RIGHT OUTER JOIN
			dbo.Table1_Project ON dbo.View_Project_VATrec_outs.Project_ID = dbo.Table1_Project.ProjectID ON
			dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary0_DCC RIGHT OUTER JOIN
			dbo.View_DCcostCodeTitle ON dbo.View_CostCodeSummary0_DCC.CostCodeAlias = dbo.View_DCcostCodeTitle.CostCode AND
			dbo.View_CostCodeSummary0_DCC.ProjectID = dbo.View_DCcostCodeTitle.ProjectID ON
			dbo.Table1_Project.ProjectID = dbo.View_DCcostCodeTitle.ProjectID
			WHERE     (dbo.Table1_Project.ProjectID = @ProjectID_Performance_Performance)
			ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_DCC.CostCode1

			IF @Currency_Performance_Performance = N'Dollar' AND @ProjectType = N'DataCenter'
			-- DOLLAR dataCenter
			SELECT     TOP (100) PERCENT dbo.View_CostCodeSummary0_DCC.CostCode1, dbo.View_CostCodeSummary0_DCC.CostCode2, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary2.VATpaidDollar) AS VATpaid, dbo.View_Project_VATrec_outs.OutstandingVATDollar AS OutstandingVAT,
			dbo.View_Project_VATrec_outs.VATRecoveredDollar AS VATRecovered,
			dbo.View_CostCodeSummary0_DCC.TotalDollarPendingExcVAT AS TotalPendingExcVAT,
			dbo.View_CostCodeSummary0_DCC.TotalDollarPaidExcVAT AS TotalPaidExcVAT,
			dbo.View_CostCodeSummary0_DCC.TotalPoTotalDollarExcVAT AS TotalPoTotalExcVAT,
			dbo.View_CostCodeSummary0_DCC.TotalBalanceDollarExcVAT AS TotalBalanceExcVAT,
			dbo.View_CostCodeSummary0_DCC.TotalCollectedDocDollarExcVAT AS TotalCollected
			FROM         dbo.View_CostCodeSummary2 RIGHT OUTER JOIN
			dbo.View_Project_VATrec_outs RIGHT OUTER JOIN
			dbo.Table1_Project ON dbo.View_Project_VATrec_outs.Project_ID = dbo.Table1_Project.ProjectID ON
			dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary0_DCC RIGHT OUTER JOIN
			dbo.View_DCcostCodeTitle ON dbo.View_CostCodeSummary0_DCC.CostCodeAlias = dbo.View_DCcostCodeTitle.CostCode AND
			dbo.View_CostCodeSummary0_DCC.ProjectID = dbo.View_DCcostCodeTitle.ProjectID ON
			dbo.Table1_Project.ProjectID = dbo.View_DCcostCodeTitle.ProjectID
			WHERE     (dbo.Table1_Project.ProjectID = @ProjectID_Performance_Performance)
			ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_DCC.CostCode1

			IF @Currency_Performance_Performance = N'Euro' AND @ProjectType = N'DataCenter'
			-- EURO dataCenter
			SELECT     TOP (100) PERCENT dbo.View_CostCodeSummary0_DCC.CostCode1, dbo.View_CostCodeSummary0_DCC.CostCode2, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary2.VATpaidEuro) AS VATpaid, dbo.View_Project_VATrec_outs.OutstandingVATEuro AS OutstandingVAT,
			dbo.View_Project_VATrec_outs.VATRecoveredEuro AS VATRecovered,
			dbo.View_CostCodeSummary0_DCC.TotalEuroPendingExcVAT AS TotalPendingExcVAT,
			dbo.View_CostCodeSummary0_DCC.TotalEuroPaidExcVAT AS TotalPaidExcVAT,
			dbo.View_CostCodeSummary0_DCC.TotalPoTotalEuroExcVAT AS TotalPoTotalExcVAT,
			dbo.View_CostCodeSummary0_DCC.TotalBalanceEuroExcVAT AS TotalBalanceExcVAT,
			dbo.View_CostCodeSummary0_DCC.TotalCollectedDocEuroExcVAT AS TotalCollected
			FROM         dbo.View_CostCodeSummary2 RIGHT OUTER JOIN
			dbo.View_Project_VATrec_outs RIGHT OUTER JOIN
			dbo.Table1_Project ON dbo.View_Project_VATrec_outs.Project_ID = dbo.Table1_Project.ProjectID ON
			dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary0_DCC RIGHT OUTER JOIN
			dbo.View_DCcostCodeTitle ON dbo.View_CostCodeSummary0_DCC.CostCodeAlias = dbo.View_DCcostCodeTitle.CostCode AND
			dbo.View_CostCodeSummary0_DCC.ProjectID = dbo.View_DCcostCodeTitle.ProjectID ON
			dbo.Table1_Project.ProjectID = dbo.View_DCcostCodeTitle.ProjectID
			WHERE     (dbo.Table1_Project.ProjectID = @ProjectID_Performance_Performance)
			ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_DCC.CostCode1

			-- RUBLE FitOut
			IF @Currency_Performance_Performance = N'Rub' AND @ProjectType = N'FitOut'
			SELECT     TOP (100) PERCENT dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1, dbo.View_CostCodeSummary0.CostCode2,
			CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0.TotalRublePendingExcVAT) AS TotalPendingExcVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0.TotalRublePaidExcVAT) AS TotalPaidExcVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0.TotalPoTotalRubleExcVAT) AS TotalPoTotalExcVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0.TotalBalanceRubleExcVAT) AS TotalBalanceExcVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary2.VATpaidRuble) AS VATpaid, dbo.View_Project_VATrec_outs.OutstandingVATRuble AS OutstandingVAT,
			dbo.View_Project_VATrec_outs.VATRecoveredRuble AS VATRecovered,
			dbo.View_CostCodeSummary0.TotalCollectedDocRubleExcVAT AS TotalCollected
			FROM         dbo.View_Project_VATrec_outs RIGHT OUTER JOIN
			dbo.Table1_Project ON dbo.View_Project_VATrec_outs.Project_ID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary2 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary2.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary0 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary0.ProjectID
			WHERE     (dbo.Table1_Project.ProjectID = @ProjectID_Performance_Performance)
			ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1

			-- DOLLAR FitOut
			IF @Currency_Performance_Performance = N'Dollar' AND @ProjectType = N'FitOut'
			SELECT     TOP (100) PERCENT dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1, dbo.View_CostCodeSummary0.CostCode2,
			CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0.TotalDollarPendingExcVAT) AS TotalPendingExcVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0.TotalDollarPaidExcVAT) AS TotalPaidExcVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0.TotalPoTotalDollarExcVAT) AS TotalPoTotalExcVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0.TotalBalanceDollarExcVAT) AS TotalBalanceExcVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary2.VATpaidDollar) AS VATpaid, dbo.View_Project_VATrec_outs.OutstandingVATDollar AS OutstandingVAT,
			dbo.View_Project_VATrec_outs.VATRecoveredDollar AS VATRecovered,
			dbo.View_CostCodeSummary0.TotalCollectedDocDollarExcVAT AS TotalCollected
			FROM         dbo.View_Project_VATrec_outs RIGHT OUTER JOIN
			dbo.Table1_Project ON dbo.View_Project_VATrec_outs.Project_ID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary2 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary2.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary0 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary0.ProjectID
			WHERE     (dbo.Table1_Project.ProjectID = @ProjectID_Performance_Performance)
			ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1

			-- Euro FitOut
			IF @Currency_Performance_Performance = N'Euro' AND @ProjectType = N'FitOut'
			SELECT     TOP (100) PERCENT dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1, dbo.View_CostCodeSummary0.CostCode2,
			CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0.TotalEuroPendingExcVAT) AS TotalPendingExcVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0.TotalEuroPaidExcVAT) AS TotalPaidExcVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0.TotalPoTotalEuroExcVAT) AS TotalPoTotalExcVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0.TotalBalanceEuroExcVAT) AS TotalBalanceExcVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary2.VATpaidEuro) AS VATpaid, dbo.View_Project_VATrec_outs.OutstandingVATEuro AS OutstandingVAT,
			dbo.View_Project_VATrec_outs.VATRecoveredEuro AS VATRecovered,
			dbo.View_CostCodeSummary0.TotalCollectedDocEuroExcVAT AS TotalCollected
			FROM         dbo.View_Project_VATrec_outs RIGHT OUTER JOIN
			dbo.Table1_Project ON dbo.View_Project_VATrec_outs.Project_ID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary2 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary2.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary0 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary0.ProjectID
			WHERE     (dbo.Table1_Project.ProjectID = @ProjectID_Performance_Performance)
			ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Currency">
            <Value>=Parameters!Currency.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
