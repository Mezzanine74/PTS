  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Email">
          <DataField>Email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RoleName">
          <DataField>RoleName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="number">
          <DataField>number</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     TOP (100) PERCENT RTRIM(dbo.aspnet_Membership.Email) AS Email, RTRIM(dbo.aspnet_Roles.RoleName) AS RoleName, 1 AS number
FROM         dbo.aspnet_Users INNER JOIN
                      dbo.aspnet_Membership ON dbo.aspnet_Users.UserId = dbo.aspnet_Membership.UserId AND 
                      dbo.aspnet_Users.UserId = dbo.aspnet_Membership.UserId INNER JOIN
                      dbo.Table_Prj_User_Junction ON dbo.aspnet_Users.UserId = dbo.Table_Prj_User_Junction.UserID INNER JOIN
                      dbo.aspnet_UsersInRoles ON dbo.aspnet_Users.UserId = dbo.aspnet_UsersInRoles.UserId AND 
                      dbo.aspnet_Users.UserId = dbo.aspnet_UsersInRoles.UserId INNER JOIN
                      dbo.aspnet_Roles ON dbo.aspnet_UsersInRoles.RoleId = dbo.aspnet_Roles.RoleId AND 
                      dbo.aspnet_UsersInRoles.RoleId = dbo.aspnet_Roles.RoleId
WHERE     (dbo.Table_Prj_User_Junction.ProjectID = @ProjectID)
ORDER BY Email</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
