  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ContractID">
            <Value>=Parameters!ContractID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT * FROM (
SELECT     Table_Contracts.ContractID, 0 AS AddendumID, RTRIM(Table1_Project.ProjectName) AS [Project Name], RTRIM(Table6_Supplier.SupplierName) AS [Supplier Name], 
                      RTRIM(Table_Contracts.ContractNo) AS [Contract No], Table_Contracts.ContractDate AS [Contract Date], RTRIM(Table_Contracts.ContractDescription) AS Description, 
                      Table_Contracts.ContractValue_withVAT, Table_Contracts.ContractCurrency, CASE WHEN SignBySupplier = 1 THEN N'YES' ELSE N'NO' END AS [Signed By Supplier], 
                      CASE WHEN SignByMercury = 1 THEN N'YES' ELSE N'NO' END AS [Signed By Mercury], 
                      CASE WHEN FrameContract = 1 THEN N'YES' ELSE N'NO' END AS [Frame Contract], 0 AS AddendumTypes, Table_Contracts.Budget AS BudgetWithVAT
					  ,(ISNULL(Budget,0) - ContractValue_withVAT) AS Variation_With_VAT , CONVERT(numeric(12,2),(ISNULL(Budget,0) - ContractValue_withVAT) / (CASE WHEN ISNULL(VATpercent,0) = 0 THEN 1 ELSE ((ISNULL(VATpercent,0) + 100) / CONVERT(decimal(10,2),100) ) END)) AS Variation_EXC_VAT
FROM         Table_Contracts INNER JOIN
                      Table6_Supplier ON Table_Contracts.SupplierID = Table6_Supplier.SupplierID INNER JOIN
                      Table1_Project ON Table_Contracts.ProjectID = Table1_Project.ProjectID
WHERE     (Table_Contracts.ContractID = @ContractID)  

	UNION ALL

SELECT     Table_Addendums.ContractID, Table_Addendums.AddendumID, RTRIM(Table1_Project.ProjectName) AS [Project Name], RTRIM(Table6_Supplier.SupplierName) 
                      AS [Supplier Name], RTRIM(Table_Addendums.AddendumNo) AS [Contract No], Table_Addendums.AddendumDate AS [Contract Date], 
                      RTRIM(Table_Addendums.AddendumDescription) AS Description, Table_Addendums.AddendumValue_WithVAT, Table_Contracts.ContractCurrency, 
                      CASE WHEN AddendumSignBySupplier = 1 THEN N'YES' ELSE N'NO' END AS [Signed By Supplier], 
                      CASE WHEN AddendumSignByMercury = 1 THEN N'YES' ELSE N'NO' END AS [Signed By Mercury], 
                      CASE WHEN FrameContract = 1 THEN N'YES' ELSE N'NO' END AS [Frame Contract], Table_Addendums.AddendumTypes, Table_Addendums.Budget AS BudgetWithVAT
					  ,(ISNULL(Table_Addendums.Budget,0) - Table_Addendums.AddendumValue_WithVAT) AS Variation_With_VAT , CONVERT(numeric(12,2),(ISNULL(Table_Addendums.Budget,0) - Table_Addendums.AddendumValue_WithVAT) / (CASE WHEN ISNULL(Table_Addendums.VATpercent,0) = 0 THEN 1 ELSE ((ISNULL(Table_Addendums.VATpercent,0) + 100) / CONVERT(decimal(10,2),100) ) END)) AS Variation_EXC_VAT
FROM         Table_Contracts INNER JOIN
                      Table6_Supplier ON Table_Contracts.SupplierID = Table6_Supplier.SupplierID INNER JOIN
                      Table1_Project ON Table_Contracts.ProjectID = Table1_Project.ProjectID INNER JOIN
                      Table_Addendums ON Table_Contracts.ContractID = Table_Addendums.ContractID
WHERE     (Table_Contracts.ContractID = @ContractID))
AS SourceContractAndAddendum</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
