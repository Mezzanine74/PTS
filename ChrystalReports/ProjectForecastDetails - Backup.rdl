  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="ForecastTypeDescription">
          <DataField>ForecastTypeDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalForecast">
          <DataField>TotalForecast</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     TOP (100) PERCENT dbo.Table_ForecastType.ForecastTypeDescription, LEFT(dbo.Table_ProjectForecast.ForecastYearMonth, 4) + '-' + DATENAME(month, 
                      CONVERT(DATETIME, LEFT(dbo.Table_ProjectForecast.ForecastYearMonth, 4) + ' - ' + SUBSTRING(dbo.Table_ProjectForecast.ForecastYearMonth, 5, 2) 
                      + '-01 00:00:00', 102)) AS Month, dbo.Table_ProjectForecast.ForecastAmount AS TotalForecast
FROM         dbo.Table_ProjectForecast INNER JOIN
                      dbo.Table1_Project AS Table1_Project_3 ON dbo.Table_ProjectForecast.ProjectID = Table1_Project_3.ProjectID INNER JOIN
                      dbo.Table_ForecastType ON dbo.Table_ProjectForecast.ForecastType = dbo.Table_ForecastType.ForecastTypeID
WHERE     (CONVERT(DATETIME, LEFT(dbo.Table_ProjectForecast.ForecastYearMonth, 4) + '-' + SUBSTRING(dbo.Table_ProjectForecast.ForecastYearMonth, 5, 2) 
                      + '-01 00:00:00', 102) &gt;= CONVERT(DATETIME, LEFT('201102', 4) + '-' + SUBSTRING('201102', 5, 2) + '-01 00:00:00', 102)) AND (Table1_Project_3.ProjectID = 127)
ORDER BY CONVERT(DATETIME, LEFT(dbo.Table_ProjectForecast.ForecastYearMonth, 4) + ' - ' + SUBSTRING(dbo.Table_ProjectForecast.ForecastYearMonth, 5, 2) 
                      + '-01 00:00:00', 102)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
