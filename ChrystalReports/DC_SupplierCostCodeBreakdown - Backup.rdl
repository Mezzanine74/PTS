  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="SupplierName">
          <DataField>SupplierName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CostCode">
          <DataField>CostCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PoTotalEuroExcVAT">
          <DataField>PoTotalEuroExcVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="EuroPaidExcVAT">
          <DataField>EuroPaidExcVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="BalanceEuroExcVAT">
          <DataField>BalanceEuroExcVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName, RTRIM(dbo.Table7_CostCode.CostCode) 
                      + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCode, SUM(dbo.View_QryW3.PoTotalEuroExcVAT) AS PoTotalEuroExcVAT, 
                      SUM(dbo.View_QryW3.EuroPaidExcVAT) AS EuroPaidExcVAT, SUM(dbo.View_QryW3.BalanceEuroExcVAT) AS BalanceEuroExcVAT
FROM         dbo.View_QryW3 INNER JOIN
                      dbo.Table2_PONo ON dbo.View_QryW3.PO_No = dbo.Table2_PONo.PO_No INNER JOIN
                      dbo.Table6_Supplier ON dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
                      dbo.Table7_CostCode ON dbo.Table2_PONo.CostCode = dbo.Table7_CostCode.CostCode
WHERE     (dbo.Table2_PONo.Project_ID = 108)
GROUP BY RTRIM(dbo.Table6_Supplier.SupplierName), RTRIM(dbo.Table7_CostCode.CostCode) + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
