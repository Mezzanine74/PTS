          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ContractCurrency">
          <DataField>ContractCurrency</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Signed_By_Supplier">
          <DataField>Signed By Supplier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Signed_By_Mercury">
          <DataField>Signed By Mercury</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Frame_Contract">
          <DataField>Frame Contract</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT * FROM (
SELECT     dbo.Table_Contracts.ContractID, 0 AS AddendumID, RTRIM(dbo.Table1_Project.ProjectName) AS [Project Name], RTRIM(dbo.Table6_Supplier.SupplierName) 
                      AS [Supplier Name], RTRIM(dbo.Table_Contracts.ContractNo) AS [Contract No], dbo.Table_Contracts.ContractDate AS [Contract Date], 
                      RTRIM(dbo.Table_Contracts.ContractDescription) AS Description, dbo.Table_Contracts.ContractValue_withVAT, dbo.Table_Contracts.ContractCurrency, 
                      CASE WHEN SignBySupplier = 1 THEN N'YES' ELSE N'NO' END AS [Signed By Supplier], 
                      CASE WHEN SignByMercury = 1 THEN N'YES' ELSE N'NO' END AS [Signed By Mercury], 
                      CASE WHEN FrameContract = 1 THEN N'YES' ELSE N'NO' END AS [Frame Contract]
FROM         dbo.Table_Contracts INNER JOIN
                      dbo.Table6_Supplier ON dbo.Table_Contracts.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
                      dbo.Table1_Project ON dbo.Table_Contracts.ProjectID = dbo.Table1_Project.ProjectID
WHERE     (dbo.Table_Contracts.ContractID = @ContractID)

UNION ALL

SELECT     dbo.Table_Addendums.ContractID, dbo.Table_Addendums.AddendumID, RTRIM(dbo.Table1_Project.ProjectName) AS [Project Name], 
                      RTRIM(dbo.Table6_Supplier.SupplierName) AS [Supplier Name], RTRIM(dbo.Table_Addendums.AddendumNo) AS [Contract No], 
                      dbo.Table_Addendums.AddendumDate AS [Contract Date], RTRIM(dbo.Table_Addendums.AddendumDescription) AS Description, 
                      dbo.Table_Addendums.AddendumValue_WithVAT, dbo.Table_Contracts.ContractCurrency, 
                      CASE WHEN AddendumSignBySupplier = 1 THEN N'YES' ELSE N'NO' END AS [Signed By Supplier], 
                      CASE WHEN AddendumSignByMercury = 1 THEN N'YES' ELSE N'NO' END AS [Signed By Mercury], 
                      CASE WHEN FrameContract = 1 THEN N'YES' ELSE N'NO' END AS [Frame Contract]
FROM         dbo.Table_Contracts INNER JOIN
                      dbo.Table6_Supplier ON dbo.Table_Contracts.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
                      dbo.Table1_Project ON dbo.Table_Contracts.ProjectID = dbo.Table1_Project.ProjectID INNER JOIN
                      dbo.Table_Addendums ON dbo.Table_Contracts.ContractID = dbo.Table_Addendums.ContractID
WHERE     (dbo.Table_Contracts.ContractID = @ContractID) ) AS SourceContractAndAddendum</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ContractID">
            <Value>=Parameters!ContractID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="ContractID">
      <DataType>String</DataType>
      <Prompt>Contract ID</Prompt>
    </ReportParameter>
  </ReportParameters>
</Report>