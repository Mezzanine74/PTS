  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     RTRIM(dbo.Table1_Project.ProjectName) AS ProjectName, RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName, RTRIM(dbo.Table6_Supplier.SupplierID) 
                      AS SupplierID, dbo.Table2_PONo.PO_No, dbo.Table2_PONo.PO_Currency, 
                      SUM(ISNULL(CASE WHEN Table2_PONo.PO_Currency = N'Rub' THEN PoTotalRubleWithVAT WHEN Table2_PONo.PO_Currency = N'Dollar' THEN PoTotalDollarWithVAT
                       WHEN Table2_PONo.PO_Currency = N'Euro' THEN PoTotalEuroWithVAT END, 0)) AS POTotalWithVAT, 
                      SUM(ISNULL(CASE WHEN Table2_PONo.PO_Currency = N'Rub' THEN RublePaidWithVAT WHEN Table2_PONo.PO_Currency = N'Dollar' THEN DollarPaidWithVAT WHEN
                       Table2_PONo.PO_Currency = N'Euro' THEN EuroPaidWithVAT END, 0)) AS PaidWithVAT, 
                      SUM(ISNULL(CASE WHEN Table2_PONo.PO_Currency = N'Rub' THEN RublePendingWithVAT WHEN Table2_PONo.PO_Currency = N'Dollar' THEN DollarPendingWithVAT
                       WHEN Table2_PONo.PO_Currency = N'Euro' THEN EuroPendingWithVAT END, 0)) AS PendingWithVAT, 
                      SUM(ISNULL(CASE WHEN Table2_PONo.PO_Currency = N'Rub' THEN BalanceRubleWithVAT WHEN Table2_PONo.PO_Currency = N'Dollar' THEN BalanceDollarWithVAT
                       WHEN Table2_PONo.PO_Currency = N'Euro' THEN BalanceEuroWithVAT END, 0)) AS BalanceWithVAT
FROM         dbo.Table2_PONo INNER JOIN
                      dbo.Table1_Project ON dbo.Table2_PONo.Project_ID = dbo.Table1_Project.ProjectID AND dbo.Table2_PONo.Project_ID = dbo.Table1_Project.ProjectID INNER JOIN
                      dbo.Table6_Supplier ON dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID AND 
                      dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
                      dbo.View_QryW3_WithVAT ON dbo.Table2_PONo.PO_No = dbo.View_QryW3_WithVAT.PO_No INNER JOIN
                          (SELECT     Table2_PONo_1.Project_ID
                            FROM          dbo.Table2_PONo AS Table2_PONo_1 INNER JOIN
                                                   dbo.Table3_Invoice ON Table2_PONo_1.PO_No = dbo.Table3_Invoice.PO_No INNER JOIN
                                                   dbo.Table4_PaymentRequest ON dbo.Table3_Invoice.InvoiceID = dbo.Table4_PaymentRequest.InvoiceID LEFT OUTER JOIN
                                                   dbo.Table5_PayLog ON dbo.Table4_PaymentRequest.PayReqNo = dbo.Table5_PayLog.PayReqNo
                            WHERE      (dbo.Table5_PayLog.PayReqNo IS NULL)
                            GROUP BY Table2_PONo_1.Project_ID) AS ProjectsInPending ON dbo.Table1_Project.ProjectID = ProjectsInPending.Project_ID
GROUP BY RTRIM(dbo.Table1_Project.ProjectName), RTRIM(dbo.Table6_Supplier.SupplierName), RTRIM(dbo.Table6_Supplier.SupplierID), dbo.Table2_PONo.PO_Currency, 
                      dbo.Table2_PONo.PO_No</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
