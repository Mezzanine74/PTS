    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>Libido</DataSourceName>
        <CommandText>SELECT     MIN(DATEDIFF(day, NeZaman, GETDATE())) AS LastLove
FROM         dbo.Table3_BulusmaDetaylari
WHERE     (Netice = N'love')</CommandText>
      </Query>
