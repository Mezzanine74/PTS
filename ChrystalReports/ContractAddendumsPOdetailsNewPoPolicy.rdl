  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ContractID">
            <Value>=Parameters!ContractID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AddendumID">
            <Value>=Parameters!AddendumID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT * FROM (
SELECT     dbo.Table_Contracts.ContractID, 0 AS AddendumID, ISNULL(dbo.Table_Contracts.PO_No, N'') AS [PO No], 
                      CASE WHEN FrameContract = 1 THEN ApprMx.View_FramePoTotals.TotalPoWithVAT ELSE (ISNULL(CASE WHEN ContractCurrency = N'Rub' THEN dbo.View_QryW3_WithVAT.PoTotalRubleWithVAT
                       WHEN ContractCurrency = N'Euro' THEN dbo.View_QryW3_WithVAT.PoTotalEuroWithVAT WHEN ContractCurrency = N'Dollar' THEN dbo.View_QryW3_WithVAT.PoTotalDollarWithVAT
                       END, 0)) END AS [Po Total With VAT], 
                      CASE WHEN FrameContract = 1 THEN ApprMx.View_FramePoTotals.PaidWithVAT ELSE (ISNULL(CASE WHEN ContractCurrency = N'Rub' THEN dbo.View_QryW2_WithVAT.RublePaidWithVAT
                       WHEN ContractCurrency = N'Euro' THEN dbo.View_QryW2_WithVAT.EuroPaidWithVAT WHEN ContractCurrency = N'Dollar' THEN dbo.View_QryW2_WithVAT.DollarPaidWithVAT
                       END, 0)) END AS [Paid With VAT], CASE WHEN FrameContract = 1 THEN ApprMx.View_FramePoTotals.PendingWithVAT ELSE (CONVERT(numeric(12, 2), 
                      ISNULL(CASE WHEN ContractCurrency = N'Rub' THEN dbo.View_QryW1_WithVAT.RublePendingWithVAT WHEN ContractCurrency = N'Euro' THEN dbo.View_QryW1_WithVAT.EuroPendingWithVAT
                       WHEN ContractCurrency = N'Dollar' THEN dbo.View_QryW1_WithVAT.DollarPendingWithVAT END, 0))) END AS [Pending With VAT], 
                      CASE WHEN Exceptional = 1 THEN N'YES' ELSE N'NO' END AS [Exceptionally Approved]
FROM         dbo.Table_Contracts LEFT OUTER JOIN
                      ApprMx.View_FramePoTotals ON dbo.Table_Contracts.ContractID = ApprMx.View_FramePoTotals.ContractID LEFT OUTER JOIN
                      dbo.View_QryW3_WithVAT ON dbo.Table_Contracts.PO_No = dbo.View_QryW3_WithVAT.PO_No LEFT OUTER JOIN
                      dbo.View_QryW1_WithVAT ON dbo.Table_Contracts.PO_No = dbo.View_QryW1_WithVAT.PO_No LEFT OUTER JOIN
                      dbo.View_QryW2_WithVAT ON dbo.Table_Contracts.PO_No = dbo.View_QryW2_WithVAT.PO_No
WHERE     (dbo.Table_Contracts.ContractID = @ContractID)

UNION ALL

SELECT     dbo.Table_Addendums.ContractID, dbo.Table_Addendums.AddendumID, ISNULL(dbo.Table_Addendums.PO_No, N'') AS [PO No], 
                      ISNULL(CASE WHEN ContractCurrency = N'Rub' THEN dbo.View_QryW3_WithVAT.PoTotalRubleWithVAT WHEN ContractCurrency = N'Euro' THEN dbo.View_QryW3_WithVAT.PoTotalEuroWithVAT
                       WHEN ContractCurrency = N'Dollar' THEN dbo.View_QryW3_WithVAT.PoTotalDollarWithVAT END, 0) AS [Po Total With VAT], 
                      ISNULL(CASE WHEN ContractCurrency = N'Rub' THEN dbo.View_QryW2_WithVAT.RublePaidWithVAT WHEN ContractCurrency = N'Euro' THEN dbo.View_QryW2_WithVAT.EuroPaidWithVAT
                       WHEN ContractCurrency = N'Dollar' THEN dbo.View_QryW2_WithVAT.DollarPaidWithVAT END, 0) AS [Paid With VAT], CONVERT(numeric(12, 2), 
                      ISNULL(CASE WHEN ContractCurrency = N'Rub' THEN dbo.View_QryW1_WithVAT.RublePendingWithVAT WHEN ContractCurrency = N'Euro' THEN dbo.View_QryW1_WithVAT.EuroPendingWithVAT
                       WHEN ContractCurrency = N'Dollar' THEN dbo.View_QryW1_WithVAT.DollarPendingWithVAT END, 0)) AS [Pending With VAT], 
                      CASE WHEN Table_Addendums.Exceptional = 1 THEN N'YES' ELSE N'NO' END AS [Exceptionally Approved]
FROM         dbo.Table_Addendums INNER JOIN
                      dbo.Table_Contracts ON dbo.Table_Addendums.ContractID = dbo.Table_Contracts.ContractID LEFT OUTER JOIN
                      dbo.View_QryW2_WithVAT ON dbo.Table_Addendums.PO_No = dbo.View_QryW2_WithVAT.PO_No LEFT OUTER JOIN
                      dbo.View_QryW1_WithVAT ON dbo.Table_Addendums.PO_No = dbo.View_QryW1_WithVAT.PO_No LEFT OUTER JOIN
                      dbo.View_QryW3_WithVAT ON dbo.Table_Addendums.PO_No = dbo.View_QryW3_WithVAT.PO_No
WHERE     (dbo.Table_Addendums.ContractID = @ContractID)
) AS Source
WHERE (AddendumID = @AddendumID)</CommandText>
