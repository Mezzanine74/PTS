        </Field>
        <Field Name="TotalDone">
          <DataField>TotalDone</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalPartial">
          <DataField>TotalPartial</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>DECLARE @ProjectID int = 135
DECLARE @Currency nVarChar(20) = N'Ruble'

SELECT     
  RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName

, (CASE WHEN @Currency = N'Dollar' THEN SUM(dbo.View_QryW3.PoTotalDollarExcVAT) 
		WHEN @Currency = N'Ruble' THEN SUM(dbo.View_QryW3.PoTotalRubleExcVAT) 
		WHEN @Currency = N'Euro' THEN SUM(dbo.View_QryW3.PoTotalEuroExcVAT) 
																			END) AS TotalPoTotalExcVAT
																			
, (CASE WHEN @Currency = N'Dollar' THEN SUM(dbo.View_QryW3.DollarPendingExcVAT) 
		WHEN @Currency = N'Ruble' THEN SUM(dbo.View_QryW3.RublePendingExcVAT) 
		WHEN @Currency = N'Euro' THEN SUM(dbo.View_QryW3.EuroPendingExcVAT) 
																			END) AS TotalPendingExcVAT

, (CASE WHEN @Currency = N'Dollar' THEN SUM(dbo.View_QryW3.DollarPaidExcVAT) 
		WHEN @Currency = N'Ruble' THEN SUM(dbo.View_QryW3.RublePaidExcVAT) 
		WHEN @Currency = N'Euro' THEN SUM(dbo.View_QryW3.EuroPaidExcVAT) 
																			END) AS TotalPaidExcVAT

, (CASE WHEN @Currency = N'Dollar' THEN SUM(dbo.View_QryW3.BalanceDollarExcVAT) 
		WHEN @Currency = N'Ruble' THEN SUM(dbo.View_QryW3.BalanceRubleExcVAT) 
		WHEN @Currency = N'Euro' THEN SUM(dbo.View_QryW3.BalanceEuroExcVAT) 
																			END) AS TotalBalanceExcVAT

, (CASE WHEN @Currency = N'Dollar' THEN SUM(CASE WHEN DoneDollarPO IS NULL THEN 0 ELSE DoneDollarPO END) 
		WHEN @Currency = N'Ruble' THEN SUM(CASE WHEN DoneRublePO IS NULL THEN 0 ELSE DoneRublePO END) 
		WHEN @Currency = N'Euro' THEN SUM(CASE WHEN DoneEuroPO IS NULL THEN 0 ELSE DoneEuroPO END) 
																			END) AS TotalDone																		
, (CASE WHEN @Currency = N'Dollar' THEN SUM(CASE WHEN PartialDollarPO IS NULL THEN 0 ELSE PartialDollarPO END) 
		WHEN @Currency = N'Ruble' THEN SUM(CASE WHEN PartialRublePO IS NULL THEN 0 ELSE PartialRublePO END) 
		WHEN @Currency = N'Euro' THEN SUM(CASE WHEN PartialEuroPO IS NULL THEN 0 ELSE PartialEuroPO END) 
																			END) AS TotalPartial
FROM         dbo.Table2_PONo INNER JOIN
                      dbo.Table6_Supplier ON dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID RIGHT OUTER JOIN
                      dbo.Table1_Project ON dbo.Table2_PONo.Project_ID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
                      dbo.View_QryW3 ON dbo.Table2_PONo.PO_No = dbo.View_QryW3.PO_No
WHERE     (dbo.Table1_Project.ProjectID = @ProjectID)
GROUP BY RTRIM(dbo.Table6_Supplier.SupplierName)
ORDER BY TotalPoTotalExcVAT DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
