      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1, dbo.View_CostCodeSummary0.CostCode2, CONVERT(decimal(12, 2),   
                       dbo.View_CostCodeSummary0.TotalRublePendingExcVAT) AS TotalRublePendingExcVAT, CONVERT(decimal(12, 2),   
                       dbo.View_CostCodeSummary0.TotalRublePaidExcVAT) AS TotalRublePaidExcVAT, CONVERT(decimal(12, 2),   
                       dbo.View_CostCodeSummary0.TotalPoTotalRubleExcVAT) AS TotalPoTotalRubleExcVAT, CONVERT(decimal(12, 2),   
                       dbo.View_CostCodeSummary0.TotalBalanceRubleExcVAT) AS TotalBalanceRubleExcVAT, CONVERT(decimal(12, 2),   
                       dbo.View_CostCodeSummary1.OverallRublePendingExcVAT) AS OverallRublePendingExcVAT, CONVERT(decimal(12, 2),   
                       dbo.View_CostCodeSummary1.OverallRublePaidExcVAT) AS OverallRublePaidExcVAT, CONVERT(decimal(12, 2),   
                       dbo.View_CostCodeSummary1.OverallPoTotalRubleExcVAT) AS OverallPoTotalRubleExcVAT, CONVERT(decimal(12, 2),   
                       dbo.View_CostCodeSummary1.OverallBalanceRubleExcVAT) AS OverallBalanceRubleExcVAT, CONVERT(decimal(12, 2),   
                       dbo.View_CostCodeSummary2.VATpaidRuble) AS VATpaidRuble, CONVERT(decimal(12, 2), dbo.View_CostCodeSummary2.OutstandingVATRuble)   
                       AS OutstandingVATRuble, CONVERT(decimal(12, 2), dbo.View_CostCodeSummary2.VATRecoveredRuble) AS VATRecoveredRuble,  
                       CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0.TotalDoneRublePO) AS TotalDoneRublePO,  
                       CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0.TotalPartialRublePO) AS TotalPartialRublePO,  
                       CONVERT(decimal(12, 2), dbo.View_CostCodeSummary1.OverallDoneRublePO) AS OverallDoneRublePO,  
                       CONVERT(decimal(12, 2), dbo.View_CostCodeSummary1.OverallPartialRublePO) AS OverallPartialRublePO  
 FROM         dbo.View_CostCodeSummary2 RIGHT OUTER JOIN  
                       dbo.Table1_Project ON dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN  
                       dbo.View_CostCodeSummary1 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary1.ProjectID LEFT OUTER JOIN  
                       dbo.View_CostCodeSummary0 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary0.ProjectID WHERE  (dbo.Table1_Project.ProjectID =   @ProjectID )
 ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1</CommandText>
