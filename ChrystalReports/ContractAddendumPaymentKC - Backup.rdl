          <DataField>Month_Year</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Submitted_KC_2">
          <DataField>Submitted_KC_2</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT 
ContractID,
AddendumId,
Company,
Contract,
[Contract No],
[Contract Date],
RTRIM(Description) AS Description,
[Contract Status],
[Contract Sum],
Currency,
[Advance %],
[Retention %],
Source.PO_No,
[PO Value],
[Total Paid To Date],
[PO Value] - [Total Paid To Date] AS [Balance to be paid],
case when [PO Value]  &lt; 0.05 then 0 else convert(numeric(12,2),[Total Paid To Date] / [PO Value]  * 100) end AS [% paid to date],
AktDocs.Month_Year,
AktDocs.Submitted_KC_2

 FROM (
SELECT     Table_Contracts.ContractID, 0 AS AddendumId, Table6_Supplier.SupplierName AS Company, N'Main Contract' AS Contract, 
                      Table_Contracts.ContractNo AS [Contract No], Table_Contracts.ContractDate AS [Contract Date], Table_Contracts.ContractDescription AS Description, 
                      CASE WHEN SignBySupplier = 1 THEN N'Signed' ELSE N'Negotiating' END AS [Contract Status], Table_Contracts.ContractValue_withVAT AS [Contract Sum], 
                      Table_Contracts.ContractCurrency AS Currency, Table_Contracts.Advance AS [Advance %], Table_Contracts.Retention AS [Retention %], 

					  ISNULL(Table_Contracts.PO_No,N'') AS PO_No,

					  CASE WHEN View_QryW3_WithVAT.PO_No IS NULL THEN
					   
					  case 
					  when Table_Contracts.ContractCurrency = N'Rub' then  Table_Contracts.ContractValue_withVAT
					  when Table_Contracts.ContractCurrency = N'Dollar' then  Table_Contracts.ContractValue_withVAT * [dbo].[View_MaxExchangeRate].RubbleDollar
					  when Table_Contracts.ContractCurrency = N'Euro' then  Table_Contracts.ContractValue_withVAT * [dbo].[View_MaxExchangeRate].RubbleEuro
					  END

					  ELSE POTotalRubleWithVAT END AS [PO Value],

                      CASE WHEN View_QryW3_WithVAT.PO_No IS NULL THEN 0 ELSE ISNULL(RublePaidWithVAT,0) END AS [Total Paid To Date] 
FROM         Table_Contracts INNER JOIN
                      Table6_Supplier ON Table_Contracts.SupplierID = Table6_Supplier.SupplierID LEFT OUTER JOIN
                      View_QryW3_WithVAT ON Table_Contracts.PO_No = View_QryW3_WithVAT.PO_No
CROSS JOIN [dbo].[View_MaxExchangeRate]
WHERE     (Table_Contracts.ProjectID = @ProjectId) AND (Table_Contracts.FrameContract = 0) AND (Table_Contracts.ContractType = N'Sub')

UNION ALL

SELECT     Table_Contracts.ContractID, Table_Addendums.AddendumID, N'' AS Company, N'Addendum' AS Contract, 
                      Table_Addendums.AddendumNo AS [Contract No], Table_Addendums.AddendumDate AS [Contract Date], Table_Addendums.AddendumDescription AS Description, 
                      CASE WHEN AddendumSignBySupplier = 1 THEN N'Signed' ELSE N'Negotiating' END AS [Contract Status], 
                      Table_Addendums.AddendumValue_WithVAT AS [Contract Sum], Table_Contracts.ContractCurrency AS Currency, Table_Addendums.Advance AS [Advance %], 
                      Table_Addendums.AddendumRetention AS [Retention %], 
					  ISNULL(Table_Addendums.PO_No,N'') AS PO_No,
					  CASE WHEN View_QryW3_WithVAT.PO_No IS NULL 
                      THEN CASE WHEN Table_Contracts.ContractCurrency = N'Rub' THEN Table_Addendums.AddendumValue_WithVAT WHEN Table_Contracts.ContractCurrency = N'Dollar'
                       THEN Table_Addendums.AddendumValue_WithVAT * [dbo].[View_MaxExchangeRate].RubbleDollar WHEN Table_Contracts.ContractCurrency = N'Euro' THEN Table_Addendums.AddendumValue_WithVAT
                       * [dbo].[View_MaxExchangeRate].RubbleEuro END ELSE POTotalRubleWithVAT END AS [PO Value], CASE WHEN View_QryW3_WithVAT.PO_No IS NULL 
                      THEN 0 ELSE ISNULL(RublePaidWithVAT,0) END AS [Total Paid To Date]
FROM         Table_Contracts INNER JOIN
                      Table_Addendums ON Table_Contracts.ContractID = Table_Addendums.ContractID INNER JOIN
                      Table6_Supplier ON Table_Contracts.SupplierID = Table6_Supplier.SupplierID LEFT OUTER JOIN
                      View_QryW3_WithVAT ON Table_Addendums.PO_No = View_QryW3_WithVAT.PO_No CROSS JOIN
                      View_MaxExchangeRate
WHERE     (Table_Contracts.ProjectID = @ProjectId) AND (Table_Contracts.FrameContract = 0) AND (Table_Contracts.ContractType = N'Sub')
) AS Source 

LEFT OUTER JOIN 

(
SELECT      View_DeliveryDocEntryPTS.PO_No, 
			CASE WHEN LEN(RTRIM(Convert(nvarchar(2),MONTH(View_DeliveryDocEntryPTS.AktDate)))) = 1 THEN N'0'+RTRIM(Convert(nvarchar(2),MONTH(View_DeliveryDocEntryPTS.AktDate))) ELSE RTRIM(Convert(nvarchar(2),MONTH(View_DeliveryDocEntryPTS.AktDate))) END + N'/' + Convert(nvarchar(4),YEAR(View_DeliveryDocEntryPTS.AktDate)) AS Month_Year, 
			View_DeliveryDocEntryPTS.AktValue AS Submitted_KC_2
FROM         View_DeliveryDocEntryPTS INNER JOIN
                      Table2_PONo ON View_DeliveryDocEntryPTS.PO_No = Table2_PONo.PO_No
WHERE     (Table2_PONo.Project_ID = @ProjectId) and (Nak_Rub_WithVAT IS NULL OR Nak_Rub_WithVAT = 0) AND AktValue IS NOT NULL AND AktDate IS NOT NULL
) AS AktDocs ON Source.PO_No = AktDocs.PO_No</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ProjectId">
            <Value>=Parameters!ProjectId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="ProjectId">
      <DataType>String</DataType>
      <Prompt>Project Id</Prompt>
    </ReportParameter>
  </ReportParameters>
