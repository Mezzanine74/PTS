  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="CostCode">
          <DataField>CostCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="costvidisionID">
          <DataField>costvidisionID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProjectID">
          <DataField>ProjectID</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="CostDivision2ID">
          <DataField>CostDivision2ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CostCodeDesc">
          <DataField>CostCodeDesc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Budget_At_Beginning_of_Project">
          <DataField>Budget At Beginning of Project</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Budget_T_O_D_A_Y">
          <DataField>Budget T O D A Y</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Manual_Increase_in_Budget">
          <DataField>Manual Increase in Budget</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PoTotalExcVAT">
          <DataField>PoTotalExcVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PaidExcVAT">
          <DataField>PaidExcVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSourceBudgetBackup</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT 
	   UnionAll.[CostCode]
      ,UnionAll.[costvidisionID]
      ,UnionAll.[ProjectID]
      ,UnionAll.[CostDivision2ID]
	  ,UnionAll.CostCodeDesc
      ,MIN_Report.[Budget] AS [Budget At Beginning of Project]
      ,MAX_Report.[Budget] AS [Budget T O D A Y]
	  ,ISNULL(MIN_Report.[Budget],0) - ISNULL(MAX_Report.[Budget],0) AS [Manual Increase in Budget]
      ,MAX_Report.[PoTotalExcVAT]
      ,MAX_Report.[PaidExcVAT]

FROM (

SELECT 
	   [CostCode]
      ,[costvidisionID]
      ,[ProjectID]
      ,[CostDivision2ID]
	  ,[CostCodeDesc]
 FROM (

SELECT 
	   [CostCode]
      ,[costvidisionID]
      ,[ProjectID]
      ,[CostDivision2ID]
	  ,[CostCodeDesc]
  FROM [dbo].[Table_BudgetBackup]
  WHERE ProjectID = @ProjectID AND BackupDate = 
