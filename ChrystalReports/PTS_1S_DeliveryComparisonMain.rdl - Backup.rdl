  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="ProjectID">
          <DataField>ProjectID</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="SupplierID">
          <DataField>SupplierID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT ProjectID, SupplierID FROM (

	SELECT ProjectID, SupplierID 
	FROM View_PTSdeliveryDocEntry 

	UNION ALL

	SELECT [Номер проекта],[SupplierID]
	  FROM [dbo].[1SdeliveryDocEntries]
	WHERE [Номер проекта] &lt;&gt; 0
	) AS SourcePrj_Supplier
	GROUP BY ProjectID, SupplierID 
	HAVING ProjectID = @ProjectID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
