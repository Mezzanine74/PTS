  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     TOP (100) PERCENT [Дата входящего документа], [Номер входящего документа], [Вид документа], Контрагент, [Договор контрагента], [Имя проекта], DocValue
FROM         dbo.[1SdeliveryDocEntries]
WHERE     ([Номер проекта] = @ProjectID) AND (SupplierID = @SupplierID)
ORDER BY DocValue DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
