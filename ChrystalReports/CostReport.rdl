      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT * FROM (
SELECT     dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.ProjectID, RTRIM(dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.CostCode) AS CostCode, 
                      RTRIM(dbo.Table7_CostCode.CodeDescription)  AS CostCodeDesc, CASE WHEN Budget IS NULL THEN 0 ELSE Budget END AS Budget, 
                      CASE WHEN PlannedToSpend IS NULL THEN 0 ELSE PlannedToSpend END AS PlannedToSpend, CASE WHEN EuroPaidExcVAT IS NULL 
                      THEN 0 ELSE EuroPaidExcVAT END AS EuroPaidExcVAT, dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.PoTotalEuroExcVAT, 
                      CASE WHEN PoTotalEuroExcVAT IS NULL THEN 1 ELSE 0 END AS DeletePossible, dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.ActualDeliveredEuro, 
                      CASE WHEN CompletionPercent IS NULL THEN 0 ELSE CompletionPercent END AS CompletionPercent, dbo.Table7_CostDivision.CostVidisionID
FROM         dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV INNER JOIN
                      dbo.Table7_CostCode ON dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.CostCode = dbo.Table7_CostCode.CostCode LEFT OUTER JOIN
                      dbo.Table7_CostDivision ON dbo.Table7_CostCode.CostVidisionID = dbo.Table7_CostDivision.CostVidisionID LEFT OUTER JOIN
                      dbo.Table_Budget ON dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.Currency = dbo.Table_Budget.Currency AND 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.ProjectID = dbo.Table_Budget.ProjectID AND 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.CostCode = dbo.Table_Budget.CostCode LEFT OUTER JOIN
                      dbo.Table_ProgressPercent ON dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.ProjectID = dbo.Table_ProgressPercent.ProjectID AND 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.CostCode = dbo.Table_ProgressPercent.CostCode

UNION ALL

SELECT     dbo.Table_Budget.ProjectID, RTRIM(dbo.Table_Budget.CostCode) AS CostCode, RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCodeDesc, 
                      CASE WHEN Budget IS NULL THEN 0 ELSE Budget END AS Budget, CASE WHEN PlannedToSpend IS NULL THEN 0 ELSE PlannedToSpend END AS PlannedToSpend, 
                      CASE WHEN EuroPaidExcVAT IS NULL THEN 0 ELSE EuroPaidExcVAT END AS EuroPaidExcVAT, 0 AS PoTotal, CASE WHEN PoTotalEuroExcVAT IS NULL 
                      THEN 1 ELSE 0 END AS DeletePossible, 0 AS TotalCollected, CASE WHEN CompletionPercent IS NULL THEN 0 ELSE CompletionPercent END AS CompletionPercent, 
                      dbo.Table7_CostDivision.CostVidisionID
FROM         dbo.Table7_CostCode INNER JOIN
                      dbo.Table_Budget ON dbo.Table7_CostCode.CostCode = dbo.Table_Budget.CostCode LEFT OUTER JOIN
                      dbo.Table7_CostDivision ON dbo.Table7_CostCode.CostVidisionID = dbo.Table7_CostDivision.CostVidisionID LEFT OUTER JOIN
                      dbo.Table_ProgressPercent ON dbo.Table_Budget.ProjectID = dbo.Table_ProgressPercent.ProjectID AND 
                      dbo.Table_Budget.CostCode = dbo.Table_ProgressPercent.CostCode LEFT OUTER JOIN
                      dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV ON dbo.Table_Budget.ProjectID = dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.ProjectID AND 
                      dbo.Table_Budget.CostCode = dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.CostCode
WHERE     (dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.ProjectID IS NULL) AND (dbo.View_Qry_TotalPendingOrderPerCostCodeEuroREV.CostCode IS NULL) AND 
                      (dbo.Table_Budget.Currency = N'Euro')
                      ) AS DataSource1
                      WHERE ProjectID = @ProjectID
                      ORDER BY CostVidisionID, CostCode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="ProjectID">
      <DataType>String</DataType>
      <Prompt>Project ID</Prompt>
    </ReportParameter>
  </ReportParameters>
</Report>