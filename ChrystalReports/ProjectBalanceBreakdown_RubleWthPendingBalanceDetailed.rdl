      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     SupplierName, PoTotalRubleExcVAT, BalanceRubleExcVAT, RublePendingExcVAT, DifferenceInRuble, PO_No, Po_Sum_InRubleExcVAT, Difference_PoRuble, 
                      Description, Invoice_No, Invoice_Date, InvoiceValueInRubleExcVAT, PO_Currency, PaymentDate, RublePaidExcVAT, PoBalanceRubleExcVAT, 
                      PoRublePendingExcVAT
FROM         View_Balance_Pending_BreakDown_Ruble_wthPending
WHERE     (ProjectName = @ProjectName) AND (PoBalanceRubleExcVAT &lt;&gt; 0)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProjectName">
            <Value>=Parameters!ProjectName.Value</Value>
          </QueryParameter>
        </QueryParameters>
