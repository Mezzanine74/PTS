      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     TOP (100) PERCENT ProjectName, SUM(PoTotalRubleExcVAT) AS PoTotalRubleExcVAT, SUM(BalanceRubleExcVAT) AS BalanceRubleExcVAT,
					SUM(RublePendingExcVAT) AS RublePendingExcVAT, SUM(DifferenceInRuble) AS DifferenceInRuble
					FROM         dbo.View_Projects_Balance_Pending_wthPending
					WHERE ProjectName = @ProjectName
					GROUP BY ProjectName</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProjectName">
            <Value>=Parameters!ProjectName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
