        <CommandText>DECLARE @ProjectType nvarChar(20) 

SET @ProjectType = (SELECT Type FROM Table1_Project WHERE ProjectID = @ProjectID)

IF @Currency = N'Rub' AND @ProjectType = N'DataCenter'
-- RUBLE dataCenter
SELECT dbo.View_CostCodeSummary0_DCC.CostCode1, dbo.View_CostCodeSummary0_DCC.CostCode2, 
                      CONVERT(decimal(12, 2), dbo.View_CostCodeSummary2.VATpaidRuble) AS VATpaid, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary2.OutstandingVATRuble) AS OutstandingVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary2.VATRecoveredRuble) AS VATRecovered, dbo.View_CostCodeSummary0_DCC.TotalRublePendingExcVAT AS TotalPendingExcVAT, 
                      dbo.View_CostCodeSummary0_DCC.TotalRublePaidExcVAT AS TotalPaidExcVAT, dbo.View_CostCodeSummary0_DCC.TotalPoTotalRubleExcVAT AS TotalPoTotalExcVAT, 
                      dbo.View_CostCodeSummary0_DCC.TotalBalanceRubleExcVAT AS TotalBalanceExcVAT, dbo.View_CostCodeSummary0_DCC.TotalDoneRublePO AS TotalDonePO, 
                      dbo.View_CostCodeSummary0_DCC.TotalPartialRublePO AS TotalPartialPO
FROM         dbo.View_CostCodeSummary0_DCC RIGHT OUTER JOIN
                      dbo.View_DCcostCodeTitle ON dbo.View_CostCodeSummary0_DCC.CostCodeAlias = dbo.View_DCcostCodeTitle.CostCode AND 
                      dbo.View_CostCodeSummary0_DCC.ProjectID = dbo.View_DCcostCodeTitle.ProjectID RIGHT OUTER JOIN
                      dbo.View_CostCodeSummary2 RIGHT OUTER JOIN
                      dbo.Table1_Project ON dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID ON 
                      dbo.View_DCcostCodeTitle.ProjectID = dbo.Table1_Project.ProjectID
WHERE     (dbo.Table1_Project.ProjectID = @ProjectID)
ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_DCC.CostCode1

IF @Currency = N'Dollar' AND @ProjectType = N'DataCenter'
-- DOLLAR dataCenter
SELECT dbo.View_CostCodeSummary0_DCC.CostCode1, dbo.View_CostCodeSummary0_DCC.CostCode2, 
                      CONVERT(decimal(12, 2), dbo.View_CostCodeSummary2.VATpaidDollar) AS VATpaid, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary2.OutstandingVATDollar) AS OutstandingVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary2.VATRecoveredDollar) AS VATRecovered, dbo.View_CostCodeSummary0_DCC.TotalDollarPendingExcVAT AS TotalPendingExcVAT, 
                      dbo.View_CostCodeSummary0_DCC.TotalDollarPaidExcVAT AS TotalPaidExcVAT, dbo.View_CostCodeSummary0_DCC.TotalPoTotalDollarExcVAT AS TotalPoTotalExcVAT, 
                      dbo.View_CostCodeSummary0_DCC.TotalBalanceDollarExcVAT AS TotalBalanceExcVAT, dbo.View_CostCodeSummary0_DCC.TotalDoneDollarPO AS TotalDonePO, 
                      dbo.View_CostCodeSummary0_DCC.TotalPartialDollarPO AS TotalPartialPO
FROM         dbo.View_CostCodeSummary0_DCC RIGHT OUTER JOIN
                      dbo.View_DCcostCodeTitle ON dbo.View_CostCodeSummary0_DCC.CostCodeAlias = dbo.View_DCcostCodeTitle.CostCode AND 
                      dbo.View_CostCodeSummary0_DCC.ProjectID = dbo.View_DCcostCodeTitle.ProjectID RIGHT OUTER JOIN
                      dbo.View_CostCodeSummary2 RIGHT OUTER JOIN
                      dbo.Table1_Project ON dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID ON 
                      dbo.View_DCcostCodeTitle.ProjectID = dbo.Table1_Project.ProjectID
WHERE     (dbo.Table1_Project.ProjectID = @ProjectID)
ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_DCC.CostCode1

IF @Currency = N'Euro' AND @ProjectType = N'DataCenter'
-- EURO dataCenter
SELECT dbo.View_CostCodeSummary0_DCC.CostCode1, dbo.View_CostCodeSummary0_DCC.CostCode2, 
                      CONVERT(decimal(12, 2), dbo.View_CostCodeSummary2.VATpaidEuro) AS VATpaid, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary2.OutstandingVATEuro) AS OutstandingVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary2.VATRecoveredEuro) AS VATRecovered, dbo.View_CostCodeSummary0_DCC.TotalEuroPendingExcVAT AS TotalPendingExcVAT, 
                      dbo.View_CostCodeSummary0_DCC.TotalEuroPaidExcVAT AS TotalPaidExcVAT, dbo.View_CostCodeSummary0_DCC.TotalPoTotalEuroExcVAT AS TotalPoTotalExcVAT, 
                      dbo.View_CostCodeSummary0_DCC.TotalBalanceEuroExcVAT AS TotalBalanceExcVAT, dbo.View_CostCodeSummary0_DCC.TotalDoneEuroPO AS TotalDonePO, 
                      dbo.View_CostCodeSummary0_DCC.TotalPartialEuroPO AS TotalPartialPO
FROM         dbo.View_CostCodeSummary0_DCC RIGHT OUTER JOIN
                      dbo.View_DCcostCodeTitle ON dbo.View_CostCodeSummary0_DCC.CostCodeAlias = dbo.View_DCcostCodeTitle.CostCode AND 
                      dbo.View_CostCodeSummary0_DCC.ProjectID = dbo.View_DCcostCodeTitle.ProjectID RIGHT OUTER JOIN
                      dbo.View_CostCodeSummary2 RIGHT OUTER JOIN
                      dbo.Table1_Project ON dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID ON 
                      dbo.View_DCcostCodeTitle.ProjectID = dbo.Table1_Project.ProjectID
WHERE     (dbo.Table1_Project.ProjectID = @ProjectID)
ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_DCC.CostCode1

-- RUBLE FitOut
IF @Currency = N'Rub' AND @ProjectType = N'FitOut'
SELECT  dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1, dbo.View_CostCodeSummary0.CostCode2, 
                      CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0.TotalRublePendingExcVAT) AS TotalPendingExcVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary0.TotalRublePaidExcVAT) AS TotalPaidExcVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary0.TotalPoTotalRubleExcVAT) AS TotalPoTotalExcVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary0.TotalBalanceRubleExcVAT) AS TotalBalanceExcVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary2.VATpaidRuble) AS VATpaid, CONVERT(decimal(12, 2), dbo.View_CostCodeSummary2.OutstandingVATRuble) 
                      AS OutstandingVAT, CONVERT(decimal(12, 2), dbo.View_CostCodeSummary2.VATRecoveredRuble) AS VATRecovered, 
                      CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0.TotalDoneRublePO) AS TotalDonePO, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary0.TotalPartialRublePO) AS TotalPartialPO
FROM         dbo.View_CostCodeSummary2 RIGHT OUTER JOIN
                      dbo.Table1_Project ON dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
                      dbo.View_CostCodeSummary0 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary0.ProjectID
WHERE     (dbo.Table1_Project.ProjectID = @ProjectID)
ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1

-- DOLLAR FitOut
IF @Currency = N'Dollar' AND @ProjectType = N'FitOut'
SELECT  dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1, dbo.View_CostCodeSummary0.CostCode2, 
                      CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0.TotalDollarPendingExcVAT) AS TotalPendingExcVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary0.TotalDollarPaidExcVAT) AS TotalPaidExcVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary0.TotalPoTotalDollarExcVAT) AS TotalPoTotalExcVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary0.TotalBalanceDollarExcVAT) AS TotalBalanceExcVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary2.VATpaidDollar) AS VATpaid, CONVERT(decimal(12, 2), dbo.View_CostCodeSummary2.OutstandingVATDollar) 
                      AS OutstandingVAT, CONVERT(decimal(12, 2), dbo.View_CostCodeSummary2.VATRecoveredDollar) AS VATRecovered, 
                      CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0.TotalDoneDollarPO) AS TotalDonePO, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary0.TotalPartialDollarPO) AS TotalPartialPO
FROM         dbo.View_CostCodeSummary2 RIGHT OUTER JOIN
                      dbo.Table1_Project ON dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
                      dbo.View_CostCodeSummary0 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary0.ProjectID
WHERE     (dbo.Table1_Project.ProjectID = @ProjectID)
ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1

-- Euro FitOut
IF @Currency = N'Euro' AND @ProjectType = N'FitOut'
SELECT  dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1, dbo.View_CostCodeSummary0.CostCode2, 
                      CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0.TotalEuroPendingExcVAT) AS TotalPendingExcVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary0.TotalEuroPaidExcVAT) AS TotalPaidExcVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary0.TotalPoTotalEuroExcVAT) AS TotalPoTotalExcVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary0.TotalBalanceEuroExcVAT) AS TotalBalanceExcVAT, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary2.VATpaidEuro) AS VATpaid, CONVERT(decimal(12, 2), dbo.View_CostCodeSummary2.OutstandingVATEuro) 
                      AS OutstandingVAT, CONVERT(decimal(12, 2), dbo.View_CostCodeSummary2.VATRecoveredEuro) AS VATRecovered, 
                      CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0.TotalDoneEuroPO) AS TotalDonePO, CONVERT(decimal(12, 2), 
                      dbo.View_CostCodeSummary0.TotalPartialEuroPO) AS TotalPartialPO
FROM         dbo.View_CostCodeSummary2 RIGHT OUTER JOIN
                      dbo.Table1_Project ON dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
                      dbo.View_CostCodeSummary0 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary0.ProjectID
WHERE     (dbo.Table1_Project.ProjectID = @ProjectID)
ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0.CostCode1</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Currency">
            <Value>=Parameters!Currency.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
