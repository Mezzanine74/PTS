  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProjectId">
            <Value>=Parameters!ProjectId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     

NULL AS ContractID,
NULL AS AddendumID,
NULL AS Project_Name,
NULL AS Supplier_Name,
NULL AS Contract_No	,
NULL AS Contract_Date,
NULL AS Description,
SUM(ContractValue_withVAT) AS ContractValue_withVAT, 
NULL AS ContractCurrency,
NULL AS Signed_By_Supplier,
NULL AS Signed_By_Mercury,
NULL AS Frame_Contract,
NULL AS AddendumTypes,
SUM(BudgetWithVAT) AS BudgetWithVAT, 
SUM(Variation_With_VAT) AS Variation_With_VAT, 
SUM(Variation_EXC_VAT) AS Variation_EXC_VAT,
NULL AS CostCode

FROM         (SELECT     SUM(
					  CASE 
					  WHEN FrameContract = 1 THEN
					  Table_Contracts.Budget
					  WHEN FrameContract = 0 THEN
                      CONVERT(numeric(12, 2), 
                      CASE WHEN dbo.Table_Contracts.ContractCurrency = N'Rub' THEN Table_Contracts.ContractValue_withVAT WHEN dbo.Table_Contracts.ContractCurrency = N'Euro' THEN
                       Table_Contracts.ContractValue_withVAT * Table8_ExchangeRates.RubbleEuro WHEN dbo.Table_Contracts.ContractCurrency = N'Dollar' THEN Table_Contracts.ContractValue_withVAT
                       * Table8_ExchangeRates.RubbleDollar END)
					   END) AS ContractValue_withVAT
											   , SUM(Table_Contracts.Budget) AS BudgetWithVAT, 
                                              SUM(ISNULL(Table_Contracts.Budget, 0) - CONVERT(numeric(12, 2), 
                                              CASE WHEN dbo.Table_Contracts.ContractCurrency = N'Rub' THEN Table_Contracts.ContractValue_withVAT WHEN dbo.Table_Contracts.ContractCurrency = N'Euro'
                                               THEN Table_Contracts.ContractValue_withVAT * Table8_ExchangeRates.RubbleEuro WHEN dbo.Table_Contracts.ContractCurrency = N'Dollar' THEN Table_Contracts.ContractValue_withVAT
                                               * Table8_ExchangeRates.RubbleDollar END)) AS Variation_With_VAT, SUM(CONVERT(numeric(12, 2), (ISNULL(Table_Contracts.Budget, 0) 
                                              - CONVERT(numeric(12, 2), 
                                              CASE WHEN dbo.Table_Contracts.ContractCurrency = N'Rub' THEN Table_Contracts.ContractValue_withVAT WHEN dbo.Table_Contracts.ContractCurrency = N'Euro'
                                               THEN Table_Contracts.ContractValue_withVAT * Table8_ExchangeRates.RubbleEuro WHEN dbo.Table_Contracts.ContractCurrency = N'Dollar' THEN Table_Contracts.ContractValue_withVAT
                                               * Table8_ExchangeRates.RubbleDollar END)) / (CASE WHEN ISNULL(VATpercent, 0) = 0 THEN 1 ELSE ((ISNULL(VATpercent, 0) + 100) 
                                              / CONVERT(decimal(10, 2), 100)) END))) AS Variation_EXC_VAT
                       FROM          Table_Contracts INNER JOIN
                                              Table6_Supplier ON Table_Contracts.SupplierID = Table6_Supplier.SupplierID INNER JOIN
                                              Table1_Project ON Table_Contracts.ProjectID = Table1_Project.ProjectID LEFT OUTER JOIN
                                              Table7_CostCode ON Table_Contracts.CostCode = Table7_CostCode.CostCode LEFT OUTER JOIN
                                              Table8_ExchangeRates ON Table_Contracts.ContractDate = Table8_ExchangeRates.Date
                       WHERE      (Table1_Project.ProjectID = @ProjectId)
                       UNION ALL
                       SELECT     SUM(CONVERT(numeric(12, 2), 
                                             CASE WHEN dbo.Table_Contracts.ContractCurrency = N'Rub' THEN Table_Addendums.AddendumValue_WithVAT WHEN dbo.Table_Contracts.ContractCurrency
                                              = N'Euro' THEN Table_Addendums.AddendumValue_WithVAT * Table8_ExchangeRates.RubbleEuro WHEN dbo.Table_Contracts.ContractCurrency = N'Dollar'
                                              THEN Table_Addendums.AddendumValue_WithVAT * Table8_ExchangeRates.RubbleDollar END)) AS AddendumValue_WithVAT, 
                                             SUM(Table_Addendums.Budget) AS BudgetWithVAT, SUM(ISNULL(Table_Addendums.Budget, 0) - CONVERT(numeric(12, 2), 
                                             CASE WHEN dbo.Table_Contracts.ContractCurrency = N'Rub' THEN Table_Addendums.AddendumValue_WithVAT WHEN dbo.Table_Contracts.ContractCurrency
                                              = N'Euro' THEN Table_Addendums.AddendumValue_WithVAT * Table8_ExchangeRates.RubbleEuro WHEN dbo.Table_Contracts.ContractCurrency = N'Dollar'
                                              THEN Table_Addendums.AddendumValue_WithVAT * Table8_ExchangeRates.RubbleDollar END)) AS Variation_With_VAT, SUM(CONVERT(numeric(12, 2), 
                                             (ISNULL(Table_Addendums.Budget, 0) - CONVERT(numeric(12, 2), 
                                             CASE WHEN dbo.Table_Contracts.ContractCurrency = N'Rub' THEN Table_Addendums.AddendumValue_WithVAT WHEN dbo.Table_Contracts.ContractCurrency
                                              = N'Euro' THEN Table_Addendums.AddendumValue_WithVAT * Table8_ExchangeRates.RubbleEuro WHEN dbo.Table_Contracts.ContractCurrency = N'Dollar'
                                              THEN Table_Addendums.AddendumValue_WithVAT * Table8_ExchangeRates.RubbleDollar END)) / (CASE WHEN ISNULL(Table_Addendums.VATpercent, 0) 
                                             = 0 THEN 1 ELSE ((ISNULL(Table_Addendums.VATpercent, 0) + 100) / CONVERT(decimal(10, 2), 100)) END))) AS Variation_EXC_VAT
                       FROM         Table_Contracts INNER JOIN
                                             Table6_Supplier ON Table_Contracts.SupplierID = Table6_Supplier.SupplierID INNER JOIN
                                             Table1_Project ON Table_Contracts.ProjectID = Table1_Project.ProjectID INNER JOIN
                                             Table_Addendums ON Table_Contracts.ContractID = Table_Addendums.ContractID LEFT OUTER JOIN
                                             Table7_CostCode ON Table_Addendums.CostCode = Table7_CostCode.CostCode LEFT OUTER JOIN
                                             Table8_ExchangeRates ON Table_Addendums.AddendumDate = Table8_ExchangeRates.Date
                       WHERE     (Table1_Project.ProjectID = @ProjectId)) AS SourceContractAndAddendum</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
