  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Currency">
            <Value>=Parameters!Currency.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CostCode">
            <Value>=Parameters!CostCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ProjectID_performance int
			SET @ProjectID_performance = @ProjectID

			DECLARE @Currency_Performance nVarChar(10)
			SET @Currency_Performance  = @Currency

			DECLARE @CostCode_Performance nVarChar(30)
			SET @CostCode_Performance  = @CostCode

			IF @CostCode_Performance = N'osman'
			SET @CostCode_Performance = N''

			DECLARE @CostCode_Filter nvarChar(500)
			SET @CostCode_Filter = RTRIM(CONVERT(nchar(30), (SELECT @CostCode_Performance))) + N' - ' + (SELECT RTRIM(CodeDescription) FROM Table7_CostCode WHERE CostCode = @CostCode_Performance)

IF @CostCode &lt;&gt; N'ALL'
BEGIN
			IF @Currency_Performance = N'Euro'

			SELECT
			ProjectID
			,CostCode
			,PO_No
			,Invoice_No
			,Invoice_Date
			,InvoiceValue
			,PO_Currency
			,SiteRecordNo
			,PaymentDate
			,SupplierName
			,Description
			,PendingExcVAT
			,PaidExcVAT
			,VATpaid
			,OrderValue
			,Coeff	FROM (
			SELECT     dbo.Table1_Project.ProjectID, RTRIM(CONVERT(nchar(30), dbo.Table7_CostCode.CostCode)) + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription)
			AS CostCode, RTRIM(dbo.Table2_PONo.PO_No) AS PO_No, dbo.View_QryW3_PoDetail.Invoice_No, dbo.View_QryW3_PoDetail.Invoice_Date,
			dbo.View_QryW3_PoDetail.InvoiceValue, dbo.View_QryW3_PoDetail.PO_Currency, CASE WHEN RTRIM(dbo.View_QryW3_PoDetail.SiteRecordNo)
			IS NULL THEN N'' ELSE RTRIM(dbo.View_QryW3_PoDetail.SiteRecordNo) END AS SiteRecordNo, dbo.View_QryW3_PoDetail.PaymentDate,
			dbo.View_QryW3_PoDetail.SupplierName, dbo.View_QryW3_PoDetail.Description, CASE WHEN View_QryW3_PoDetail.EuroPendingExcVAT IS NULL
			THEN 0 ELSE View_QryW3_PoDetail.EuroPendingExcVAT END AS PendingExcVAT, CASE WHEN View_QryW3_PoDetail.EuroPaidExcVAT IS NULL
			THEN 0 ELSE View_QryW3_PoDetail.EuroPaidExcVAT END AS PaidExcVAT, CASE WHEN View_QryW3_PoDetail.VATpaidEuro IS NULL
			THEN 0 ELSE View_QryW3_PoDetail.VATpaidEuro END AS VATpaid, dbo.View_QryW3_PoDetail.OrderValueEuro AS OrderValue
			,Coeff
			FROM         dbo.Table8_ExchangeRates RIGHT OUTER JOIN
			dbo.Table4_PaymentRequest RIGHT OUTER JOIN
			dbo.View_QryW3_PoDetail ON dbo.Table4_PaymentRequest.InvoiceID = dbo.View_QryW3_PoDetail.InvoiceID ON
			dbo.Table8_ExchangeRates.Date = dbo.View_QryW3_PoDetail.PaymentDate RIGHT OUTER JOIN
			dbo.View_PoInvoiceNull INNER JOIN
			dbo.Table1_Project INNER JOIN
			dbo.Table2_PONo ON dbo.Table1_Project.ProjectID = dbo.Table2_PONo.Project_ID INNER JOIN
			dbo.Table7_CostCode ON dbo.Table2_PONo.CostCode = dbo.Table7_CostCode.CostCode ON
			dbo.View_PoInvoiceNull.PO_No = dbo.Table2_PONo.PO_No ON dbo.View_QryW3_PoDetail.PO_No = dbo.Table2_PONo.PO_No LEFT OUTER JOIN
			dbo.View_QryW3 ON dbo.Table2_PONo.PO_No = dbo.View_QryW3.PO_No CROSS JOIN
			dbo.View_MaxExchangeRate
			LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = Table2_PONo.PO_No
			WHERE     (View_PoInvoiceNull.IfNull = 'not null') AND (Table2_PONo.Project_ID = @ProjectID_performance) AND (RTRIM(CONVERT(nchar(30), Table7_CostCode.CostCode)) 
                      + N' - ' + RTRIM(Table7_CostCode.CodeDescription) = @CostCode_Filter)



			UNION ALL

			SELECT     ProjectID, CostCode, View_QryOpenPosInEuro.PO_No AS PO_No, NULL AS Invoice_No, NULL AS Invoice_Date, NULL AS InvoiceValue, PO_Currency, NULL AS SiteRecordNo, NULL
			AS PaymentDate, SupplierName, Description, NULL AS PendingExcVAT, NULL AS PaidExcVAT, NULL AS VATpaid, Open_Euro AS OrderValue
			,Coeff
			FROM         dbo.View_QryOpenPosInEuro
			LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = View_QryOpenPosInEuro.PO_No
			WHERE     (View_QryOpenPosInEuro.ProjectID = @ProjectID_performance) AND (View_QryOpenPosInEuro.CostCode = @CostCode_Filter)

			UNION ALL

			SELECT     Table1_Project_1.ProjectID, RTRIM(CONVERT(nchar(30), Table7_CostCode_1.CostCode)) + N' - ' + RTRIM(Table7_CostCode_1.CodeDescription)
			AS CostCode, View_PoInvoiceNull_1.PO_No, NULL AS Invoice_No, NULL AS Invoice_Date, NULL AS InvoiceValue, Table2_PONo_1.PO_Currency, NULL
			AS SiteRecordNo, NULL AS PaymentDate, RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName, RTRIM(Table2_PONo_1.Description)
			AS Description, NULL AS PendingExcVAT, NULL AS PaidExcVAT, NULL AS VATpaid, View_QryW3_1.PoTotalEuroExcVAT AS OrderValue
			,Coeff
			FROM         dbo.Table2_PONo AS Table2_PONo_1 INNER JOIN
			dbo.Table1_Project AS Table1_Project_1 ON Table2_PONo_1.Project_ID = Table1_Project_1.ProjectID INNER JOIN
			dbo.Table6_Supplier ON Table2_PONo_1.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
			dbo.Table7_CostCode AS Table7_CostCode_1 ON Table2_PONo_1.CostCode = Table7_CostCode_1.CostCode INNER JOIN
			dbo.View_PoInvoiceNull AS View_PoInvoiceNull_1 ON Table2_PONo_1.PO_No = View_PoInvoiceNull_1.PO_No INNER JOIN
			dbo.View_QryW3 AS View_QryW3_1 ON View_PoInvoiceNull_1.PO_No = View_QryW3_1.PO_No CROSS JOIN
			dbo.View_MaxExchangeRate AS View_MaxExchangeRate_1
			LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = Table2_PONo_1.PO_No
			WHERE     (View_QryW3_1.PoTotalEuroExcVAT &lt;&gt; 0) AND (View_PoInvoiceNull_1.IfNull = 'null') AND (Table1_Project_1.ProjectID = @ProjectID_performance) AND 
                      (RTRIM(CONVERT(nchar(30), Table7_CostCode_1.CostCode)) + N' - ' + RTRIM(Table7_CostCode_1.CodeDescription) = @CostCode_Filter)


					) AS DataSource_FollowUpReportSubEuroExcVAT

					ORDER BY CostCode, PO_No

IF @Currency_Performance = N'Dollar'

SELECT 
 ProjectID
,CostCode
,PO_No
,Invoice_No
,Invoice_Date
,InvoiceValue
,PO_Currency
,SiteRecordNo
,PaymentDate
,SupplierName
,Description
,PendingExcVAT
,PaidExcVAT
,VATpaid
,OrderValue 
,Coeff	FROM (
					SELECT     dbo.Table1_Project.ProjectID, RTRIM(CONVERT(nchar(30), dbo.Table7_CostCode.CostCode)) + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription) 
                      AS CostCode, RTRIM(dbo.Table2_PONo.PO_No) AS PO_No, dbo.View_QryW3_PoDetail.Invoice_No, dbo.View_QryW3_PoDetail.Invoice_Date, 
                      dbo.View_QryW3_PoDetail.InvoiceValue, dbo.View_QryW3_PoDetail.PO_Currency, CASE WHEN RTRIM(dbo.View_QryW3_PoDetail.SiteRecordNo) 
                      IS NULL THEN N'' ELSE RTRIM(dbo.View_QryW3_PoDetail.SiteRecordNo) END AS SiteRecordNo, dbo.View_QryW3_PoDetail.PaymentDate, 
                      dbo.View_QryW3_PoDetail.SupplierName, dbo.View_QryW3_PoDetail.Description, CASE WHEN View_QryW3_PoDetail.DollarPendingExcVAT IS NULL 
                      THEN 0 ELSE View_QryW3_PoDetail.DollarPendingExcVAT END AS PendingExcVAT, CASE WHEN View_QryW3_PoDetail.DollarPaidExcVAT IS NULL 
                      THEN 0 ELSE View_QryW3_PoDetail.DollarPaidExcVAT END AS PaidExcVAT, CASE WHEN View_QryW3_PoDetail.VATpaidDollar IS NULL 
                      THEN 0 ELSE View_QryW3_PoDetail.VATpaidDollar END AS VATpaid, dbo.View_QryW3_PoDetail.OrderValueDollar AS OrderValue
					  ,Coeff
					FROM         dbo.Table8_ExchangeRates RIGHT OUTER JOIN
                      dbo.Table4_PaymentRequest RIGHT OUTER JOIN
                      dbo.View_QryW3_PoDetail ON dbo.Table4_PaymentRequest.InvoiceID = dbo.View_QryW3_PoDetail.InvoiceID ON 
                      dbo.Table8_ExchangeRates.Date = dbo.View_QryW3_PoDetail.PaymentDate RIGHT OUTER JOIN
                      dbo.View_PoInvoiceNull INNER JOIN
                      dbo.Table1_Project INNER JOIN
                      dbo.Table2_PONo ON dbo.Table1_Project.ProjectID = dbo.Table2_PONo.Project_ID INNER JOIN
                      dbo.Table7_CostCode ON dbo.Table2_PONo.CostCode = dbo.Table7_CostCode.CostCode ON 
                      dbo.View_PoInvoiceNull.PO_No = dbo.Table2_PONo.PO_No ON dbo.View_QryW3_PoDetail.PO_No = dbo.Table2_PONo.PO_No LEFT OUTER JOIN
                      dbo.View_QryW3 ON dbo.Table2_PONo.PO_No = dbo.View_QryW3.PO_No CROSS JOIN
                      dbo.View_MaxExchangeRate
                      LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = Table2_PONo.PO_No 
					WHERE     (View_PoInvoiceNull.IfNull = 'not null') AND (Table2_PONo.Project_ID = @ProjectID_performance) AND (RTRIM(CONVERT(nchar(30), Table7_CostCode.CostCode)) 
                      + N' - ' + RTRIM(Table7_CostCode.CodeDescription) = @CostCode_Filter)

                       UNION ALL
                       
					SELECT     ProjectID, CostCode, View_QryOpenPosInDollar.PO_No AS PO_No, NULL AS Invoice_No, NULL AS Invoice_Date, NULL AS InvoiceValue, PO_Currency, NULL AS SiteRecordNo, NULL 
                      AS PaymentDate, SupplierName, Description, NULL AS PendingExcVAT, NULL AS PaidExcVAT, NULL AS VATpaid, Open_Dollar AS OrderValue
					  ,Coeff
					FROM         dbo.View_QryOpenPosInDollar
                      LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = View_QryOpenPosInDollar.PO_No 					
					WHERE     (View_QryOpenPosInDollar.ProjectID = @ProjectID_performance) AND (View_QryOpenPosInDollar.CostCode = @CostCode_Filter)
	
					

                       UNION ALL
                       
					SELECT     Table1_Project_1.ProjectID, RTRIM(CONVERT(nchar(30), Table7_CostCode_1.CostCode)) + N' - ' + RTRIM(Table7_CostCode_1.CodeDescription) 
                      AS CostCode, View_PoInvoiceNull_1.PO_No, NULL AS Invoice_No, NULL AS Invoice_Date, NULL AS InvoiceValue, Table2_PONo_1.PO_Currency, NULL 
                      AS SiteRecordNo, NULL AS PaymentDate, RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName, RTRIM(Table2_PONo_1.Description) 
                      AS Description, NULL AS PendingExcVAT, NULL AS PaidExcVAT, NULL AS VATpaid, View_QryW3_1.PoTotalDollarExcVAT AS OrderValue
					  ,Coeff
					FROM         dbo.Table2_PONo AS Table2_PONo_1 INNER JOIN
                      dbo.Table1_Project AS Table1_Project_1 ON Table2_PONo_1.Project_ID = Table1_Project_1.ProjectID INNER JOIN
                      dbo.Table6_Supplier ON Table2_PONo_1.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
                      dbo.Table7_CostCode AS Table7_CostCode_1 ON Table2_PONo_1.CostCode = Table7_CostCode_1.CostCode INNER JOIN
                      dbo.View_PoInvoiceNull AS View_PoInvoiceNull_1 ON Table2_PONo_1.PO_No = View_PoInvoiceNull_1.PO_No INNER JOIN
                      dbo.View_QryW3 AS View_QryW3_1 ON View_PoInvoiceNull_1.PO_No = View_QryW3_1.PO_No CROSS JOIN
                      dbo.View_MaxExchangeRate AS View_MaxExchangeRate_1
                      LEFT OUTER JOIN View_PO_TotalCollectedDocsCoeff ON View_PO_TotalCollectedDocsCoeff.PO_No = Table2_PONo_1.PO_No 
					WHERE     (View_QryW3_1.PoTotalDollarExcVAT &lt;&gt; 0) AND (View_PoInvoiceNull_1.IfNull = 'null') AND (Table1_Project_1.ProjectID = @ProjectID_performance) AND 
                      (RTRIM(CONVERT(nchar(30), Table7_CostCode_1.CostCode)) + N' - ' + RTRIM(Table7_CostCode_1.CodeDescription) = @CostCode_Filter)


					) AS DataSource_FollowUpReportSubDollarExcVAT
					ORDER BY CostCode, PO_No

IF @Currency_Performance = N'Ruble'

SELECT 
 ProjectID
,CostCode
,PO_No
,Invoice_No
,Invoice_Date
,InvoiceValue
,PO_Currency
,SiteRecordNo
,PaymentDate
,SupplierName
,Description
,PendingExcVAT
,PaidExcVAT
,VATpaid
,OrderValue 
,Coeff FROM (
SELECT     Table1_Project.ProjectID, RTRIM(CONVERT(nchar(30), Table7_CostCode.CostCode)) + N' - ' + RTRIM(Table7_CostCode.CodeDescription) AS CostCode, 
                      RTRIM(Table2_PONo.PO_No) AS PO_No, View_QryW3_PoDetail.Invoice_No, View_QryW3_PoDetail.Invoice_Date, View_QryW3_PoDetail.InvoiceValue, 
                      View_QryW3_PoDetail.PO_Currency, CASE WHEN RTRIM(dbo.View_QryW3_PoDetail.SiteRecordNo) IS NULL 
                      THEN N'' ELSE RTRIM(dbo.View_QryW3_PoDetail.SiteRecordNo) END AS SiteRecordNo, View_QryW3_PoDetail.PaymentDate, View_QryW3_PoDetail.SupplierName, 
                      View_QryW3_PoDetail.Description, CASE WHEN View_QryW3_PoDetail.RublePendingExcVAT IS NULL 
                      THEN 0 ELSE View_QryW3_PoDetail.RublePendingExcVAT END AS PendingExcVAT, CASE WHEN View_QryW3_PoDetail.RublePaidExcVAT IS NULL 
                      THEN 0 ELSE View_QryW3_PoDetail.RublePaidExcVAT END AS PaidExcVAT, CASE WHEN View_QryW3_PoDetail.VATpaidRuble IS NULL 
