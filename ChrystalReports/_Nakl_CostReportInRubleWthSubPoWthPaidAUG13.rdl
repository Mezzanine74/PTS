<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition"><DataSources><DataSource Name="DataSourceAugust2013"><DataSourceReference>DataSourceAugust2013</DataSourceReference><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">8f9a21a1-fbcc-46dd-87bb-d41d65e15ed7</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="DataSet1"><Fields><Field Name="CostCode"><DataField>CostCode</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="CostCodeDesc"><DataField>CostCodeDesc</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Budget"><DataField>Budget</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="PlannedToSpend"><DataField>PlannedToSpend</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="PoTotalRubleExcVAT"><DataField>PoTotalRubleExcVAT</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="DeletePossible"><DataField>DeletePossible</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="ActualDeliveredRuble"><DataField>ActualDeliveredRuble</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="CompletionPercent"><DataField>CompletionPercent</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="CostDivisionDescription"><DataField>CostDivisionDescription</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="UpdatedPlannedRevenue"><DataField>UpdatedPlannedRevenue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="ProjectName"><DataField>ProjectName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="costvidisionID"><DataField>costvidisionID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int16</rd:TypeName></Field><Field Name="ProjectID"><DataField>ProjectID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int16</rd:TypeName></Field><Field Name="OriginalBOQ"><DataField>OriginalBOQ</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="CountOfComment"><DataField>CountOfComment</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="RublePaidExcVAT"><DataField>RublePaidExcVAT</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field></Fields><Query><DataSourceName>DataSourceAugust2013</DataSourceName><CommandText>

			DECLARE @ProjectID_performance int
			SET @ProjectID_performance = @ProjectID

			IF (SELECT COUNT(BOQorderID) FROM Table_Progress_BOQ WHERE ProjectID = @ProjectID_Performance) &gt; 0

SELECT [CostCode]
			,[CostCodeDesc]
			,[Budget]
			,[PlannedToSpend]
			,[PoTotalRubleExcVAT]
			,[DeletePossible]
			,[ActualDeliveredRuble]
			,[CompletionPercent]
			,[CostDivisionDescription]
			,costvidisionID
			,UpdatedPlannedRevenue
			,ProjectName
			,ProjectID
			,OriginalBOQ
			,CountOfComment
			,RublePaidExcVAT
			FROM (
SELECT     RTRIM(dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.CostCode) AS CostCode, RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCodeDesc, 
                      CASE WHEN Budget IS NULL THEN 0 ELSE Budget END AS Budget, (CASE WHEN PlannedToSpend IS NULL THEN 0 ELSE PlannedToSpend END) 
                      + (CASE WHEN PlannedToSpendCO IS NULL THEN 0 ELSE PlannedToSpendCO END) AS PlannedToSpend, 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.PoTotalRubleExcVAT, CASE WHEN PoTotalRubleExcVAT IS NULL 
                      THEN 1 ELSE 0 END AS DeletePossible, dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ActualDeliveredRuble,
					  CASE WHEN CompletionPercent IS NULL THEN 0 ELSE CompletionPercent END AS CompletionPercent,
                      RTRIM(dbo.Table7_CostDivision.CostDivisionDescription) AS CostDivisionDescription, dbo.Table7_CostDivision.CostVidisionID, 
                      CASE WHEN UpdatedPlannedRevenue IS NULL THEN 0 ELSE UpdatedPlannedRevenue END AS UpdatedPlannedRevenue, RTRIM(dbo.Table1_Project.ProjectName) 
                      AS ProjectName, dbo.Table1_Project.ProjectID, CASE WHEN OriginalBOQ IS NULL THEN 0 ELSE OriginalBOQ END AS OriginalBOQ, 
                      CASE WHEN CountOfComment IS NULL THEN 0 ELSE CountOfComment END AS CountOfComment, 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.RublePaidExcVAT
FROM         dbo.Table7_CostDivision RIGHT OUTER JOIN
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4 INNER JOIN
                      dbo.Table7_CostCode ON dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.CostCode = dbo.Table7_CostCode.CostCode INNER JOIN
                      dbo.Table1_Project ON dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
                          (SELECT     dbo.Table_Progress_BOQ.ProjectID, RTRIM(dbo.Table_Progress_BOQ.CostCode) AS CostCode, CONVERT(decimal(5, 2), 
                                                   SUM(dbo.Table_Progress_BOQ.Qty * ((CASE WHEN ((dbo.Table_Progress_BOQ.Prelim_Matr_Rate * TotalProgress.Prelim_Matr_Percentage / 100 + dbo.Table_Progress_BOQ.Matr_Matr_Rate
                                                    * TotalProgress.Matr_Matr_Percentage / 100) + dbo.Table_Progress_BOQ.Sub_Matr_Rate * TotalProgress.Sub_Matr_Percentage / 100) IS NULL 
                                                   THEN 0 ELSE ((dbo.Table_Progress_BOQ.Prelim_Matr_Rate * TotalProgress.Prelim_Matr_Percentage / 100 + dbo.Table_Progress_BOQ.Matr_Matr_Rate *
                                                    TotalProgress.Matr_Matr_Percentage / 100) + dbo.Table_Progress_BOQ.Sub_Matr_Rate * TotalProgress.Sub_Matr_Percentage / 100) END) 
                                                   + (CASE WHEN (dbo.Table_Progress_BOQ.Prelim_Labor_Rate * TotalProgress.Prelim_Labor_Percentage / 100 + dbo.Table_Progress_BOQ.Labor_Labor_Rate
                                                    * TotalProgress.Labor_Labor_Percentage / 100) IS NULL 
                                                   THEN 0 ELSE (dbo.Table_Progress_BOQ.Prelim_Labor_Rate * TotalProgress.Prelim_Labor_Percentage / 100 + dbo.Table_Progress_BOQ.Labor_Labor_Rate
                                                    * TotalProgress.Labor_Labor_Percentage / 100) END))) 
                                                   / SUM(dbo.Table_Progress_BOQ.Qty * ((dbo.Table_Progress_BOQ.Prelim_Matr_Rate + dbo.Table_Progress_BOQ.Matr_Matr_Rate + dbo.Table_Progress_BOQ.Sub_Matr_Rate)
                                                    + (dbo.Table_Progress_BOQ.Prelim_Labor_Rate + dbo.Table_Progress_BOQ.Labor_Labor_Rate))) * 100) AS CompletionPercent
                            FROM          dbo.Table_Progress_BOQ INNER JOIN
                                                   dbo.Table7_CostCode AS Table7_CostCode_1 ON dbo.Table_Progress_BOQ.CostCode = Table7_CostCode_1.CostCode LEFT OUTER JOIN
                                                       (SELECT     dbo.Table_Progress_Progress.ItemID, SUM(dbo.Table_Progress_Progress.Prelim_Matr_Qty / Table_Progress_BOQ_1.Qty * 100) 
                                                                                AS Prelim_Matr_Percentage, SUM(dbo.Table_Progress_Progress.Prelim_Labor_Qty / Table_Progress_BOQ_1.Qty * 100) 
                                                                                AS Prelim_Labor_Percentage, SUM(dbo.Table_Progress_Progress.Matr_Matr_Qty / Table_Progress_BOQ_1.Qty * 100) 
                                                                                AS Matr_Matr_Percentage, SUM(dbo.Table_Progress_Progress.Labor_Labor_Qty / Table_Progress_BOQ_1.Qty * 100) 
                                                                                AS Labor_Labor_Percentage, SUM(dbo.Table_Progress_Progress.Sub_Matr_Qty / Table_Progress_BOQ_1.Qty * 100) 
                                                                                AS Sub_Matr_Percentage
                                                         FROM          dbo.Table_Progress_BOQ AS Table_Progress_BOQ_1 INNER JOIN
                                                                                dbo.Table_Progress_Progress ON Table_Progress_BOQ_1.ItemID = dbo.Table_Progress_Progress.ItemID
                                                         GROUP BY dbo.Table_Progress_Progress.ItemID) AS TotalProgress ON dbo.Table_Progress_BOQ.ItemID = TotalProgress.ItemID
                            GROUP BY RTRIM(dbo.Table_Progress_BOQ.CostCode), dbo.Table_Progress_BOQ.ProjectID
                            HAVING      (dbo.Table_Progress_BOQ.ProjectID = @ProjectID_Performance)) AS ProgressPercentForCostReport ON 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ProjectID = ProgressPercentForCostReport.ProjectID AND 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.CostCode = ProgressPercentForCostReport.CostCode ON 
                      dbo.Table7_CostDivision.CostVidisionID = dbo.Table7_CostCode.CostVidisionID LEFT OUTER JOIN
                      dbo.View_CountOfCommentOnBudget RIGHT OUTER JOIN
                      dbo.Table_Budget ON dbo.View_CountOfCommentOnBudget.BudgetID = dbo.Table_Budget.BudgetID ON 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.Currency = dbo.Table_Budget.Currency AND 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ProjectID = dbo.Table_Budget.ProjectID AND 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.CostCode = dbo.Table_Budget.CostCode
WHERE     (dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ProjectID = @ProjectID_Performance)

UNION ALL

SELECT     RTRIM(dbo.Table_Budget.CostCode) AS CostCode, RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCodeDesc, CASE WHEN Budget IS NULL 
                      THEN 0 ELSE Budget END AS Budget, (CASE WHEN PlannedToSpend IS NULL THEN 0 ELSE PlannedToSpend END) + (CASE WHEN PlannedToSpendCO IS NULL 
                      THEN 0 ELSE PlannedToSpendCO END) AS PlannedToSpend, 0 AS PoTotal, CASE WHEN PoTotalRubleExcVAT IS NULL THEN 1 ELSE 0 END AS DeletePossible, 
                      0 AS TotalCollected, CASE WHEN CompletionPercent IS NULL THEN 0 ELSE CompletionPercent END AS CompletionPercent,
                      RTRIM(dbo.Table7_CostDivision.CostDivisionDescription) AS CostDivisionDescription, dbo.Table7_CostDivision.CostVidisionID, 
                      CASE WHEN UpdatedPlannedRevenue IS NULL THEN 0 ELSE UpdatedPlannedRevenue END AS UpdatedPlannedRevenue, RTRIM(dbo.Table1_Project.ProjectName) 
                      AS ProjectName, dbo.Table1_Project.ProjectID, CASE WHEN OriginalBOQ IS NULL THEN 0 ELSE OriginalBOQ END AS OriginalBOQ, 
                      CASE WHEN CountOfComment IS NULL THEN 0 ELSE CountOfComment END AS CountOfComment, 0 AS RublePaidExcVAT 
FROM         dbo.Table7_CostCode INNER JOIN
                      dbo.Table_Budget ON dbo.Table7_CostCode.CostCode = dbo.Table_Budget.CostCode INNER JOIN
                      dbo.Table1_Project ON dbo.Table_Budget.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
                          (SELECT     dbo.Table_Progress_BOQ.ProjectID, RTRIM(dbo.Table_Progress_BOQ.CostCode) AS CostCode, CONVERT(decimal(5, 2), 
                                                   SUM(dbo.Table_Progress_BOQ.Qty * ((CASE WHEN ((dbo.Table_Progress_BOQ.Prelim_Matr_Rate * TotalProgress.Prelim_Matr_Percentage / 100 + dbo.Table_Progress_BOQ.Matr_Matr_Rate
                                                    * TotalProgress.Matr_Matr_Percentage / 100) + dbo.Table_Progress_BOQ.Sub_Matr_Rate * TotalProgress.Sub_Matr_Percentage / 100) IS NULL 
                                                   THEN 0 ELSE ((dbo.Table_Progress_BOQ.Prelim_Matr_Rate * TotalProgress.Prelim_Matr_Percentage / 100 + dbo.Table_Progress_BOQ.Matr_Matr_Rate *
                                                    TotalProgress.Matr_Matr_Percentage / 100) + dbo.Table_Progress_BOQ.Sub_Matr_Rate * TotalProgress.Sub_Matr_Percentage / 100) END) 
                                                   + (CASE WHEN (dbo.Table_Progress_BOQ.Prelim_Labor_Rate * TotalProgress.Prelim_Labor_Percentage / 100 + dbo.Table_Progress_BOQ.Labor_Labor_Rate
                                                    * TotalProgress.Labor_Labor_Percentage / 100) IS NULL 
                                                   THEN 0 ELSE (dbo.Table_Progress_BOQ.Prelim_Labor_Rate * TotalProgress.Prelim_Labor_Percentage / 100 + dbo.Table_Progress_BOQ.Labor_Labor_Rate
                                                    * TotalProgress.Labor_Labor_Percentage / 100) END))) 
                                                   / SUM(dbo.Table_Progress_BOQ.Qty * ((dbo.Table_Progress_BOQ.Prelim_Matr_Rate + dbo.Table_Progress_BOQ.Matr_Matr_Rate + dbo.Table_Progress_BOQ.Sub_Matr_Rate)
                                                    + (dbo.Table_Progress_BOQ.Prelim_Labor_Rate + dbo.Table_Progress_BOQ.Labor_Labor_Rate))) * 100) AS CompletionPercent
                            FROM          dbo.Table_Progress_BOQ INNER JOIN
                                                   dbo.Table7_CostCode AS Table7_CostCode_1 ON dbo.Table_Progress_BOQ.CostCode = Table7_CostCode_1.CostCode LEFT OUTER JOIN
                                                       (SELECT     dbo.Table_Progress_Progress.ItemID, SUM(dbo.Table_Progress_Progress.Prelim_Matr_Qty / Table_Progress_BOQ_1.Qty * 100) 
                                                                                AS Prelim_Matr_Percentage, SUM(dbo.Table_Progress_Progress.Prelim_Labor_Qty / Table_Progress_BOQ_1.Qty * 100) 
                                                                                AS Prelim_Labor_Percentage, SUM(dbo.Table_Progress_Progress.Matr_Matr_Qty / Table_Progress_BOQ_1.Qty * 100) 
                                                                                AS Matr_Matr_Percentage, SUM(dbo.Table_Progress_Progress.Labor_Labor_Qty / Table_Progress_BOQ_1.Qty * 100) 
                                                                                AS Labor_Labor_Percentage, SUM(dbo.Table_Progress_Progress.Sub_Matr_Qty / Table_Progress_BOQ_1.Qty * 100) 
                                                                                AS Sub_Matr_Percentage
                                                         FROM          dbo.Table_Progress_BOQ AS Table_Progress_BOQ_1 INNER JOIN
                                                                                dbo.Table_Progress_Progress ON Table_Progress_BOQ_1.ItemID = dbo.Table_Progress_Progress.ItemID
                                                         GROUP BY dbo.Table_Progress_Progress.ItemID) AS TotalProgress ON dbo.Table_Progress_BOQ.ItemID = TotalProgress.ItemID
                            GROUP BY RTRIM(dbo.Table_Progress_BOQ.CostCode), dbo.Table_Progress_BOQ.ProjectID
                            HAVING      (dbo.Table_Progress_BOQ.ProjectID = @ProjectID_Performance)) AS ProgressPercentForCostReport ON 
                      dbo.Table_Budget.ProjectID = ProgressPercentForCostReport.ProjectID AND dbo.Table_Budget.CostCode = ProgressPercentForCostReport.CostCode LEFT OUTER JOIN
                      dbo.View_CountOfCommentOnBudget ON dbo.Table_Budget.BudgetID = dbo.View_CountOfCommentOnBudget.BudgetID LEFT OUTER JOIN
                      dbo.Table7_CostDivision ON dbo.Table7_CostCode.CostVidisionID = dbo.Table7_CostDivision.CostVidisionID LEFT OUTER JOIN
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4 ON 
                      dbo.Table_Budget.ProjectID = dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ProjectID AND 
                      dbo.Table_Budget.CostCode = dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.CostCode
WHERE     (dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ProjectID IS NULL) AND (dbo.Table_Budget.Currency = N'Rub') AND 
                      (dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.CostCode IS NULL) AND (dbo.Table_Budget.ProjectID = @ProjectID_Performance) AND 
                      (dbo.Table_Budget.CostCode NOT IN
                          (SELECT     dbo.Table2_PONo.CostCode
                            FROM          dbo.Table2_PONo INNER JOIN
                                                       (SELECT     PO_No, SUM(TotalPrice) AS TotalPrice
                                                         FROM          dbo.Table2_PONo_Sub
                                                         GROUP BY PO_No) AS DataSource117 ON DataSource117.PO_No = dbo.Table2_PONo.PO_No AND 
                                                   dbo.Table2_PONo.TotalPrice = DataSource117.TotalPrice
                            WHERE      (dbo.Table2_PONo.Project_ID = @ProjectID_Performance)
                            GROUP BY dbo.Table2_PONo.CostCode))
			) AS DataSource1
			ORDER BY DataSource1.[CostCode] ASC
			
IF (SELECT COUNT(BOQorderID) FROM Table_Progress_BOQ WHERE ProjectID = @ProjectID_Performance) = 0
SELECT [CostCode]
			,[CostCodeDesc]
			,[Budget]
			,[PlannedToSpend]
			,[PoTotalRubleExcVAT]
			,[DeletePossible]
			,[ActualDeliveredRuble]
			,[CompletionPercent]
			,[CostDivisionDescription]
			,costvidisionID
			,UpdatedPlannedRevenue
			,ProjectName
			,ProjectID
			,OriginalBOQ
			,CountOfComment
			,RublePaidExcVAT
			FROM (

SELECT     RTRIM(dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.CostCode) AS CostCode, RTRIM(dbo.Table7_CostCode.CodeDescription) 
                      AS CostCodeDesc, CASE WHEN Budget IS NULL THEN 0 ELSE Budget END AS Budget, (CASE WHEN PlannedToSpend IS NULL 
                      THEN 0 ELSE PlannedToSpend END) + (CASE WHEN PlannedToSpendCO IS NULL THEN 0 ELSE PlannedToSpendCO END) AS PlannedToSpend, 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.PoTotalRubleExcVAT, CASE WHEN PoTotalRubleExcVAT IS NULL 
                      THEN 1 ELSE 0 END AS DeletePossible, dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ActualDeliveredRuble, 
                      CASE WHEN CompletionPercent IS NULL THEN 0 ELSE CompletionPercent END AS CompletionPercent, 
                      RTRIM(dbo.Table7_CostDivision.CostDivisionDescription) AS CostDivisionDescription, dbo.Table7_CostDivision.CostVidisionID, 
                      CASE WHEN UpdatedPlannedRevenue IS NULL THEN 0 ELSE UpdatedPlannedRevenue END AS UpdatedPlannedRevenue, 
                      RTRIM(dbo.Table1_Project.ProjectName) AS ProjectName, dbo.Table1_Project.ProjectID, CASE WHEN OriginalBOQ IS NULL 
                      THEN 0 ELSE OriginalBOQ END AS OriginalBOQ, CASE WHEN CountOfComment IS NULL THEN 0 ELSE CountOfComment END AS CountOfComment, 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.RublePaidExcVAT
FROM         dbo.View_CountOfCommentOnBudget RIGHT OUTER JOIN
                      dbo.Table_Budget ON dbo.View_CountOfCommentOnBudget.BudgetID = dbo.Table_Budget.BudgetID RIGHT OUTER JOIN
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4 INNER JOIN
                      dbo.Table7_CostCode ON dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.CostCode = dbo.Table7_CostCode.CostCode INNER JOIN
                      dbo.Table1_Project ON dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
                      dbo.Table7_CostDivision ON dbo.Table7_CostCode.CostVidisionID = dbo.Table7_CostDivision.CostVidisionID ON 
                      dbo.Table_Budget.Currency = dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.Currency AND 
                      dbo.Table_Budget.ProjectID = dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ProjectID AND 
                      dbo.Table_Budget.CostCode = dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.CostCode LEFT OUTER JOIN
                      dbo.Table_ProgressPercent ON dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ProjectID = dbo.Table_ProgressPercent.ProjectID AND 
                      dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.CostCode = dbo.Table_ProgressPercent.CostCode
WHERE     (dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ProjectID = @ProjectID_Performance)

			UNION ALL

			SELECT     RTRIM(dbo.Table_Budget.CostCode) AS CostCode, RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCodeDesc, CASE WHEN Budget IS NULL
			THEN 0 ELSE Budget END AS Budget, ((CASE WHEN PlannedToSpend IS NULL
			THEN 0 ELSE PlannedToSpend END) + (CASE WHEN PlannedToSpendCO IS NULL
			THEN 0 ELSE PlannedToSpendCO END)) AS PlannedToSpend, 0 AS PoTotal,
			CASE WHEN PoTotalRubleExcVAT IS NULL THEN 1 ELSE 0 END AS DeletePossible, 0 AS TotalCollected, CASE WHEN CompletionPercent IS NULL
			THEN 0 ELSE CompletionPercent END AS CompletionPercent, RTRIM(dbo.Table7_CostDivision.CostDivisionDescription) AS CostDivisionDescription,
			dbo.Table7_CostDivision.CostVidisionID, CASE WHEN UpdatedPlannedRevenue IS NULL
			THEN 0 ELSE UpdatedPlannedRevenue END AS UpdatedPlannedRevenue, RTRIM(dbo.Table1_Project.ProjectName) AS ProjectName,
			dbo.Table1_Project.ProjectID, CASE WHEN OriginalBOQ IS NULL THEN 0 ELSE OriginalBOQ END AS OriginalBOQ,
			CASE WHEN CountOfComment IS NULL THEN 0 ELSE CountOfComment END AS CountOfComment, 0 AS RublePaidExcVAT
			FROM         dbo.Table7_CostCode INNER JOIN
			dbo.Table_Budget ON dbo.Table7_CostCode.CostCode = dbo.Table_Budget.CostCode INNER JOIN
			dbo.Table1_Project ON dbo.Table_Budget.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CountOfCommentOnBudget ON dbo.Table_Budget.BudgetID = dbo.View_CountOfCommentOnBudget.BudgetID LEFT OUTER JOIN
			dbo.Table7_CostDivision ON dbo.Table7_CostCode.CostVidisionID = dbo.Table7_CostDivision.CostVidisionID LEFT OUTER JOIN
			dbo.Table_ProgressPercent ON dbo.Table_Budget.ProjectID = dbo.Table_ProgressPercent.ProjectID AND
			dbo.Table_Budget.CostCode = dbo.Table_ProgressPercent.CostCode LEFT OUTER JOIN
			dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4 ON
			dbo.Table_Budget.ProjectID = dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ProjectID AND
			dbo.Table_Budget.CostCode = dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.CostCode
			WHERE     (dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.ProjectID IS NULL) AND (dbo.Table_Budget.Currency = N'Rub') AND
			(dbo.View_Qry_TotalPendingOrderPerCostCodeRubleREVFinal4.CostCode IS NULL) AND (dbo.Table_Budget.ProjectID = @ProjectID_Performance) AND
			(dbo.Table_Budget.CostCode NOT IN
			(SELECT     dbo.Table2_PONo.CostCode
			FROM          dbo.Table2_PONo INNER JOIN
			(SELECT     PO_No, SUM(TotalPrice) AS TotalPrice
			FROM          dbo.Table2_PONo_Sub
			GROUP BY PO_No) AS DataSource117 ON DataSource117.PO_No = dbo.Table2_PONo.PO_No AND
			dbo.Table2_PONo.TotalPrice = DataSource117.TotalPrice
			WHERE      (dbo.Table2_PONo.Project_ID = @ProjectID_Performance)
			GROUP BY dbo.Table2_PONo.CostCode))
