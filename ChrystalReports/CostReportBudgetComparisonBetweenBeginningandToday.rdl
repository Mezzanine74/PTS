  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSourceBudgetBackup</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	   UnionAll.[CostCode]
      ,UnionAll.[costvidisionID]
      ,UnionAll.[ProjectID]
      ,UnionAll.[CostDivision2ID]
	  ,UnionAll.CostCodeDesc
	  ,UnionAll.[CostDivisionDescription]
	  ,UnionAll.[CostDivision2Description]
      ,MIN_Report.[OriginalBOQ] AS [Contract BOQ]
      ,MIN_Report.[Budget] AS [Budget At Beginning of Project]
      ,MAX_Report.[Budget] AS [Budget T O D A Y]
	  ,CASE WHEN (- ISNULL(MIN_Report.[Budget],0) + ISNULL(MAX_Report.[Budget],0) ) = 0 THEN NULL
	  ELSE (- ISNULL(MIN_Report.[Budget],0) + ISNULL(MAX_Report.[Budget],0) ) END AS [Manual Increase in Budget]
      ,MAX_Report.[PoTotalExcVAT] AS [Committed Cost]
      ,MAX_Report.[PaidExcVAT] AS [Paid]
	  , CASE WHEN MAX_Report.[PoTotalExcVAT] &gt; MIN_Report.[Budget] THEN N'Commitment exceeds initial budget'
	    WHEN MIN_Report.[Budget] IS NULL AND ISNULL(MAX_Report.[PoTotalExcVAT],0) &gt; 0 THEN N'Commitment exceeds initial budget' END AS Notes
FROM (

SELECT 
	   [CostCode]
      ,[costvidisionID]
      ,[ProjectID]
      ,[CostDivision2ID]
	  ,[CostCodeDesc]
	  ,[CostDivisionDescription]
	  ,[CostDivision2Description]
 FROM (

SELECT 
	   [CostCode]
      ,[costvidisionID]
      ,[ProjectID]
      ,[CostDivision2ID]
	  ,[CostCodeDesc]
	  ,[CostDivisionDescription]
	  ,[CostDivision2Description]
  FROM [dbo].[Table_BudgetBackup]
  WHERE ProjectID = @ProjectID AND BackupDate = 
  (
	SELECT MIN([BackupDate])
	FROM [dbo].[Table_BudgetBackup]
	WHERE ProjectID = @ProjectID
  )

UNION ALL

SELECT 
       [CostCode]
      ,[costvidisionID]
      ,[ProjectID]
      ,[CostDivision2ID]
	  ,[CostCodeDesc]
	  ,[CostDivisionDescription]
	  ,[CostDivision2Description]
  FROM [dbo].[Table_BudgetBackup]
  WHERE ProjectID = @ProjectID AND BackupDate = 
  (
	SELECT MAX([BackupDate])
	FROM [dbo].[Table_BudgetBackup]
	WHERE ProjectID = @ProjectID
  )

  ) AS UnionAll
  GROUP BY 
       [CostCode]
      ,[costvidisionID]
      ,[ProjectID]
      ,[CostDivision2ID]
	  ,[CostCodeDesc]
	  ,[CostDivisionDescription]
	  ,[CostDivision2Description]

) AS UnionAll

LEFT OUTER JOIN 

(
SELECT 
	   [CostCode]
      ,[costvidisionID]
      ,[ProjectID]
      ,[CostDivision2ID]
	  ,[CostCodeDesc]
	  ,[CostDivisionDescription]
	  ,[CostDivision2Description]
      ,[OriginalBOQ]
      ,[Budget]
      ,[PoTotalExcVAT]
      ,[PaidExcVAT]
  FROM [dbo].[Table_BudgetBackup]
  WHERE ProjectID = @ProjectID AND BackupDate = 
  (
	SELECT MIN([BackupDate])
	FROM [dbo].[Table_BudgetBackup]
	WHERE ProjectID = @ProjectID
  )
) AS MIN_Report 
ON UnionAll.CostCode = MIN_Report.CostCode AND
 UnionAll.[costvidisionID] = MIN_Report.[costvidisionID] AND
 UnionAll.[ProjectID] = MIN_Report.[ProjectID] AND
 UnionAll.[CostDivision2ID] = MIN_Report.[CostDivision2ID] AND
 UnionAll.[CostCodeDesc] = MIN_Report.[CostCodeDesc] AND
 UnionAll.[CostDivisionDescription] = MIN_Report.[CostDivisionDescription] AND
 UnionAll.[CostDivision2Description] = MIN_Report.[CostDivision2Description]
 
LEFT OUTER JOIN 

(
SELECT 
	   [CostCode]
      ,[costvidisionID]
      ,[ProjectID]
      ,[CostDivision2ID]
	  ,[CostCodeDesc]
	  ,[CostDivisionDescription]
	  ,[CostDivision2Description]
      ,[OriginalBOQ]
      ,[Budget]
      ,[PoTotalExcVAT]
      ,[PaidExcVAT]
  FROM [dbo].[Table_BudgetBackup]
  WHERE ProjectID = @ProjectID AND BackupDate = 
  (
	SELECT MAX([BackupDate])
	FROM [dbo].[Table_BudgetBackup]
	WHERE ProjectID = @ProjectID
  )
) AS MAX_Report 
ON UnionAll.CostCode = MAX_Report.CostCode AND
 UnionAll.[costvidisionID] = MAX_Report.[costvidisionID] AND
 UnionAll.[ProjectID] = MAX_Report.[ProjectID] AND
 UnionAll.[CostDivision2ID] = MAX_Report.[CostDivision2ID]  AND
 UnionAll.[CostCodeDesc] = MAX_Report.[CostCodeDesc] AND
 UnionAll.[CostDivisionDescription] = MAX_Report.[CostDivisionDescription] AND
 UnionAll.[CostDivision2Description] = MAX_Report.[CostDivision2Description]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
