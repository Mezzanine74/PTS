  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     TOP (100) PERCENT RTRIM(dbo.Table1_Project.ProjectName) AS ProjectName, RTRIM(dbo.Table7_CostCode.CostCode) AS CostCode, 
                      RTRIM(dbo.Table7_CostCode.CodeDescription) AS CodeDescription, RTRIM(dbo.Table2_PONo.PO_No) AS PO_No, RTRIM(dbo.Table2_PONo.Description) 
                      AS Description, RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName, CONVERT(decimal(12, 2), 
                      ROUND((CASE WHEN dbo.Table6_Supplier.VAT_Free = 1 THEN dbo.Table2_POno.TotalPrice ELSE dbo.Table2_POno.TotalPrice / ((100 + dbo.Table2_POno.VATpercent) 
                      / 100) END), 2)) AS [PO Value In Ruble Exc. VAT]
FROM         dbo.Table2_PONo INNER JOIN
                      dbo.Table7_CostCode ON dbo.Table2_PONo.CostCode = dbo.Table7_CostCode.CostCode INNER JOIN
                      dbo.Table6_Supplier ON dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
                      dbo.Table1_Project ON dbo.Table2_PONo.Project_ID = dbo.Table1_Project.ProjectID
WHERE     (dbo.Table6_Supplier.SupplierID = N'7717148677') AND (dbo.Table2_PONo.CreatedBy &gt; GETDATE() - 1)
ORDER BY ProjectName, CostCode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
