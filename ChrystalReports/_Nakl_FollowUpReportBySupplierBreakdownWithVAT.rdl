  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Currency">
            <Value>=Parameters!Currency.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>if @Currency = N'Euro'
SELECT     TOP (100) PERCENT RTRIM(dbo.Table1_Project.ProjectName) AS ProjectName, dbo.Table1_Project.ProjectID, SUM(dbo.View_QryW3_WithVAT.PoTotalEuroWithVAT) 
                      AS TotalPoTotalWithVAT, SUM(dbo.View_QryW3_WithVAT.EuroPaidWithVAT) AS TotalPaidWithVAT, SUM(dbo.View_QryW3_WithVAT.BalanceEuroWithVAT) AS TotalBalanceWith, 
                      SUM(dbo.View_QryW3_WithVAT.EuroPendingWithVAT) AS TotalPendingWithVAT, SUM(dbo.View_QryW3_WithVAT.PoTotalEuroWithVAT * dbo.View_PO_TotalCollectedDocsCoeff.Coeff) 
                      AS TotalCollectedDocsWithVAT
FROM         dbo.View_QryW3_WithVAT INNER JOIN
                      dbo.Table2_PONo ON dbo.View_QryW3_WithVAT.PO_No = dbo.Table2_PONo.PO_No INNER JOIN
                      dbo.Table6_Supplier ON dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
                      dbo.View_PO_TotalCollectedDocsCoeff ON dbo.View_QryW3_WithVAT.PO_No = dbo.View_PO_TotalCollectedDocsCoeff.PO_No INNER JOIN
                      dbo.Table1_Project ON dbo.Table2_PONo.Project_ID = dbo.Table1_Project.ProjectID
WHERE     (dbo.Table2_PONo.SupplierID = @SupplierID)
GROUP BY RTRIM(dbo.Table1_Project.ProjectName), dbo.Table1_Project.ProjectID
ORDER BY TotalPoTotalWithVAT DESC

if @Currency = N'Dollar'
SELECT     TOP (100) PERCENT RTRIM(dbo.Table1_Project.ProjectName) AS ProjectName, dbo.Table1_Project.ProjectID, SUM(dbo.View_QryW3_WithVAT.PoTotalDollarWithVAT) 
                      AS TotalPoTotalWithVAT, SUM(dbo.View_QryW3_WithVAT.DollarPaidWithVAT) AS TotalPaidWithVAT, SUM(dbo.View_QryW3_WithVAT.BalanceDollarWithVAT) AS TotalBalanceWith, 
                      SUM(dbo.View_QryW3_WithVAT.DollarPendingWithVAT) AS TotalPendingWithVAT, SUM(dbo.View_QryW3_WithVAT.PoTotalDollarWithVAT * dbo.View_PO_TotalCollectedDocsCoeff.Coeff) 
                      AS TotalCollectedDocsWithVAT
FROM         dbo.View_QryW3_WithVAT INNER JOIN
                      dbo.Table2_PONo ON dbo.View_QryW3_WithVAT.PO_No = dbo.Table2_PONo.PO_No INNER JOIN
                      dbo.Table6_Supplier ON dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
                      dbo.View_PO_TotalCollectedDocsCoeff ON dbo.View_QryW3_WithVAT.PO_No = dbo.View_PO_TotalCollectedDocsCoeff.PO_No INNER JOIN
                      dbo.Table1_Project ON dbo.Table2_PONo.Project_ID = dbo.Table1_Project.ProjectID
WHERE     (dbo.Table2_PONo.SupplierID = @SupplierID)
GROUP BY RTRIM(dbo.Table1_Project.ProjectName), dbo.Table1_Project.ProjectID
ORDER BY TotalPoTotalWithVAT DESC

if @Currency = N'Ruble'
SELECT     TOP (100) PERCENT RTRIM(dbo.Table1_Project.ProjectName) AS ProjectName, dbo.Table1_Project.ProjectID, SUM(dbo.View_QryW3_WithVAT.PoTotalRubleWithVAT) 
                      AS TotalPoTotalWithVAT, SUM(dbo.View_QryW3_WithVAT.RublePaidWithVAT) AS TotalPaidWithVAT, SUM(dbo.View_QryW3_WithVAT.BalanceRubleWithVAT) AS TotalBalanceWith, 
                      SUM(dbo.View_QryW3_WithVAT.RublePendingWithVAT) AS TotalPendingWithVAT, SUM(dbo.View_QryW3_WithVAT.PoTotalRubleWithVAT * dbo.View_PO_TotalCollectedDocsCoeff.Coeff) 
                      AS TotalCollectedDocsWithVAT
FROM         dbo.View_QryW3_WithVAT INNER JOIN
                      dbo.Table2_PONo ON dbo.View_QryW3_WithVAT.PO_No = dbo.Table2_PONo.PO_No INNER JOIN
                      dbo.Table6_Supplier ON dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
                      dbo.View_PO_TotalCollectedDocsCoeff ON dbo.View_QryW3_WithVAT.PO_No = dbo.View_PO_TotalCollectedDocsCoeff.PO_No INNER JOIN
                      dbo.Table1_Project ON dbo.Table2_PONo.Project_ID = dbo.Table1_Project.ProjectID
WHERE     (dbo.Table2_PONo.SupplierID = @SupplierID)
GROUP BY RTRIM(dbo.Table1_Project.ProjectName), dbo.Table1_Project.ProjectID
ORDER BY TotalPoTotalWithVAT DESC</CommandText>
