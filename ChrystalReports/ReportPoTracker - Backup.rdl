      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT 
-- PROJECT_NAME
	Table1_Project_1.ProjectName AS ProjectName
-- COB ------------------------------------------------------------------------------------------------------------------------------------
,dbo.Table9_Date.Date AS COB
-- CURRENT_PROJECT_VALUE ------------------------------------------------------------------------------------------------------------------
                          ,(SELECT     SUM(ContractAmount) AS CurrenctAmount
                            FROM          (SELECT     ContractAmount
                                                    FROM          dbo.Table1_Project
                                                    WHERE      (ProjectID = 127)
                                                    UNION ALL
                                                    SELECT     SUM(AddendumAmount) AS TotalAddendum1
                                                    FROM         dbo.Table_ProjectAddendum
                                                    WHERE     (ProjectID = 127) AND (AddendumDate &lt;= dbo.Table9_Date.Date)) AS Datasource1) AS CurrentProjectValue
-- MARGIN----------------------------------------------------------------------------------------------------------------------------------
                                                    , Table1_Project_1.Margin
-- ALLOWABLE_SPEND-------------------------------------------------------------------------------------------------------------------------                                                    
                          ,(SELECT     SUM(ContractAmount) AS CurrenctAmount                          
                            FROM          (SELECT     ContractAmount
                                                    FROM          dbo.Table1_Project AS Table1_Project_2
                                                    WHERE      (ProjectID = 127)
                                                    UNION ALL
                                                    SELECT     SUM(AddendumAmount) AS TotalAddendum1
                                                    FROM         dbo.Table_ProjectAddendum AS Table_ProjectAddendum_1
                                                    WHERE     (ProjectID = 127) AND (AddendumDate &lt;= dbo.Table9_Date.Date)) AS Datasource1_1) * (100 - Table1_Project_1.Margin) 
                      / 100 AS Allowable_Spend
-- TOTAL_PO_PTS ---------------------------------------------------------------------------------------------------------------------------
                      , (SELECT  case (SELECT     RTRIM(ContractCurrency) AS ContractCurrency
													FROM         dbo.Table1_Project
													WHERE     (ProjectID = 127))
													when 'Rub' then OverallPOTotalRubleExcVAT
													when 'Dollar' then OverallPOTotalDollarExcVAT
													when 'Euro' then OverallPOTotalEuroExcVAT
													end AS PO_Total
													FROM         dbo.Table_FollowUpReportSummary
													WHERE     (ProjectID = 127) AND (DayOfRun = DATEADD(day, 1, dbo.Table9_Date.Date))) AS TotalPO_PTS
-- FCTS_Salary-----------------------------------------------------------------------------------------------------------------------------
                          ,(SELECT     SUM(dbo.Table_ProjectForecast.ForecastAmount) AS TotalForecast
                            FROM          dbo.Table_ProjectForecast INNER JOIN
                                                   dbo.Table1_Project AS Table1_Project_3 ON dbo.Table_ProjectForecast.ProjectID = Table1_Project_3.ProjectID
                            WHERE      (CONVERT(DATETIME, LEFT(dbo.Table_ProjectForecast.ForecastYearMonth, 4) + '-' + SUBSTRING(dbo.Table_ProjectForecast.ForecastYearMonth, 5, 2) 
                                                   + '-01 00:00:00', 102) &gt;= CONVERT(datetime, CONVERT(nchar(4), YEAR(dbo.Table9_Date.Date)) + '-' + CASE len(rtrim(CONVERT(nchar(2), 
                                                   month(table9_date.date)))) WHEN 2 THEN rtrim(CONVERT(nchar(2), month(table9_date.date))) WHEN 1 THEN '0' + rtrim(CONVERT(nchar(2), 
                                                   month(table9_date.date))) END + '-01 00:00:00', 102)) AND (Table1_Project_3.ProjectID = 127)) AS FCTS_Salary
-- Total_COM = TOTAL_PO_PTS + FCTS_Salary--------------------------------------------------------------------------------------------------
                          ,(SELECT  case (SELECT     RTRIM(ContractCurrency) AS ContractCurrency
													FROM         dbo.Table1_Project
													WHERE     (ProjectID = 127))
													when 'Rub' then OverallPOTotalRubleExcVAT
													when 'Dollar' then OverallPOTotalDollarExcVAT
													when 'Euro' then OverallPOTotalEuroExcVAT
													end AS PO_Total
													FROM         dbo.Table_FollowUpReportSummary
													WHERE     (ProjectID = 127) AND (DayOfRun = DATEADD(day, 1, dbo.Table9_Date.Date))) +
                          (SELECT     SUM(Table_ProjectForecast_1.ForecastAmount) AS TotalForecast
                            FROM          dbo.Table_ProjectForecast AS Table_ProjectForecast_1 INNER JOIN
                                                   dbo.Table1_Project AS Table1_Project_3 ON Table_ProjectForecast_1.ProjectID = Table1_Project_3.ProjectID
                            WHERE      (CONVERT(DATETIME, LEFT(Table_ProjectForecast_1.ForecastYearMonth, 4) + '-' + SUBSTRING(Table_ProjectForecast_1.ForecastYearMonth, 5, 2) 
                                                   + '-01 00:00:00', 102) &gt;= CONVERT(datetime, CONVERT(nchar(4), YEAR(dbo.Table9_Date.Date)) + '-' + CASE len(rtrim(CONVERT(nchar(2), 
                                                   month(table9_date.date)))) WHEN 2 THEN rtrim(CONVERT(nchar(2), month(table9_date.date))) WHEN 1 THEN '0' + rtrim(CONVERT(nchar(2), 
                                                   month(table9_date.date))) END + '-01 00:00:00', 102)) AND (Table1_Project_3.ProjectID = 127)) AS Total_COM
-- LeftToSpend = ALLOWABLE_SPEND - Total_COM ----------------------------------------------------------------------------------------------
,(SELECT     SUM(ContractAmount) AS CurrenctAmount                          
                            FROM          (SELECT     ContractAmount
                                                    FROM          dbo.Table1_Project AS Table1_Project_2
                                                    WHERE      (ProjectID = 127)
                                                    UNION ALL
                                                    SELECT     SUM(AddendumAmount) AS TotalAddendum1
                                                    FROM         dbo.Table_ProjectAddendum AS Table_ProjectAddendum_1
                                                    WHERE     (ProjectID = 127) AND (AddendumDate &lt;= dbo.Table9_Date.Date)) AS Datasource1_1) * (100 - Table1_Project_1.Margin) 
                      / 100 - ((SELECT  case (SELECT     RTRIM(ContractCurrency) AS ContractCurrency
													FROM         dbo.Table1_Project
													WHERE     (ProjectID = 127))
													when 'Rub' then OverallPOTotalRubleExcVAT
													when 'Dollar' then OverallPOTotalDollarExcVAT
													when 'Euro' then OverallPOTotalEuroExcVAT
													end AS PO_Total
													FROM         dbo.Table_FollowUpReportSummary
													WHERE     (ProjectID = 127) AND (DayOfRun = DATEADD(day, 1, dbo.Table9_Date.Date))) +
                          (SELECT     SUM(Table_ProjectForecast_1.ForecastAmount) AS TotalForecast
                            FROM          dbo.Table_ProjectForecast AS Table_ProjectForecast_1 INNER JOIN
                                                   dbo.Table1_Project AS Table1_Project_3 ON Table_ProjectForecast_1.ProjectID = Table1_Project_3.ProjectID
                            WHERE      (CONVERT(DATETIME, LEFT(Table_ProjectForecast_1.ForecastYearMonth, 4) + '-' + SUBSTRING(Table_ProjectForecast_1.ForecastYearMonth, 5, 2) 
                                                   + '-01 00:00:00', 102) &gt;= CONVERT(datetime, CONVERT(nchar(4), YEAR(dbo.Table9_Date.Date)) + '-' + CASE len(rtrim(CONVERT(nchar(2), 
                                                   month(table9_date.date)))) WHEN 2 THEN rtrim(CONVERT(nchar(2), month(table9_date.date))) WHEN 1 THEN '0' + rtrim(CONVERT(nchar(2), 
                                                   month(table9_date.date))) END + '-01 00:00:00', 102)) AND (Table1_Project_3.ProjectID = 127))) AS LeftToSpend
-- % committed = Total_COM / ALLOWABLE_SPEND ----------------------------------------------------------------------------------------------
,((SELECT  case (SELECT     RTRIM(ContractCurrency) AS ContractCurrency
													FROM         dbo.Table1_Project
													WHERE     (ProjectID = 127))
													when 'Rub' then OverallPOTotalRubleExcVAT
													when 'Dollar' then OverallPOTotalDollarExcVAT
													when 'Euro' then OverallPOTotalEuroExcVAT
													end AS PO_Total
													FROM         dbo.Table_FollowUpReportSummary
													WHERE     (ProjectID = 127) AND (DayOfRun = DATEADD(day, 1, dbo.Table9_Date.Date))) +
                          (SELECT     SUM(Table_ProjectForecast_1.ForecastAmount) AS TotalForecast
                            FROM          dbo.Table_ProjectForecast AS Table_ProjectForecast_1 INNER JOIN
                                                   dbo.Table1_Project AS Table1_Project_3 ON Table_ProjectForecast_1.ProjectID = Table1_Project_3.ProjectID
                            WHERE      (CONVERT(DATETIME, LEFT(Table_ProjectForecast_1.ForecastYearMonth, 4) + '-' + SUBSTRING(Table_ProjectForecast_1.ForecastYearMonth, 5, 2) 
                                                   + '-01 00:00:00', 102) &gt;= CONVERT(datetime, CONVERT(nchar(4), YEAR(dbo.Table9_Date.Date)) + '-' + CASE len(rtrim(CONVERT(nchar(2), 
                                                   month(table9_date.date)))) WHEN 2 THEN rtrim(CONVERT(nchar(2), month(table9_date.date))) WHEN 1 THEN '0' + rtrim(CONVERT(nchar(2), 
                                                   month(table9_date.date))) END + '-01 00:00:00', 102)) AND (Table1_Project_3.ProjectID = 127))) / ((SELECT     SUM(ContractAmount) AS CurrenctAmount                          
                            FROM          (SELECT     ContractAmount
                                                    FROM          dbo.Table1_Project AS Table1_Project_2
                                                    WHERE      (ProjectID = 127)
                                                    UNION ALL
                                                    SELECT     SUM(AddendumAmount) AS TotalAddendum1
                                                    FROM         dbo.Table_ProjectAddendum AS Table_ProjectAddendum_1
                                                    WHERE     (ProjectID = 127) AND (AddendumDate &lt;= dbo.Table9_Date.Date)) AS Datasource1_1) * (100 - Table1_Project_1.Margin) 
                      / 100) AS Percent_Committed
FROM         dbo.Table9_Date CROSS JOIN
                      dbo.Table1_Project AS Table1_Project_1
WHERE     (dbo.Table9_Date.Date &gt; CONVERT(DATETIME, '2011-02-01 00:00:00', 102)) AND (dbo.Table9_Date.Date &lt; CONVERT(DATETIME, '2011-02-28 00:00:00', 102)) AND 
                      (Table1_Project_1.ProjectID = 127)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
