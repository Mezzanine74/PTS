          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     TOP (100) PERCENT dbo.View_Projects_Balance_Pending.ProjectName, RTRIM(dbo.View_Projects_Balance_Pending.SupplierName) AS SupplierName, 
                      dbo.View_Projects_Balance_Pending.PoTotalDollarExcVAT, dbo.View_Projects_Balance_Pending.BalanceDollarExcVAT, 
                      dbo.View_Projects_Balance_Pending.DollarPendingExcVAT, dbo.View_Projects_Balance_Pending.DifferenceInDollar, RTRIM(dbo.View_QryW3_WthPrjId.PO_No) 
                      AS PO_No, dbo.View_QryW3.PoTotalDollarExcVAT AS Po_Sum_InDollarExcVAT, 
                      dbo.View_QryW3.BalanceDollarExcVAT - (CASE WHEN View_QryW3.DollarPendingExcVAT IS NULL THEN 0 ELSE View_QryW3.DollarPendingExcVAT END) 
                      AS Difference_PoDollar, RTRIM(dbo.View_QryW3_WthPrjId.Description) AS Description, RTRIM(dbo.View_QryW3_WthPrjId.Invoice_No) AS Invoice_No, 
                      dbo.View_QryW3_WthPrjId.Invoice_Date, dbo.View_QryW3_WthPrjId.OrderValueDollar AS InvoiceValueInEuroExcVAT, 
                      RTRIM(dbo.View_QryW3_WthPrjId.PO_Currency) AS PO_Currency, dbo.View_QryW3_WthPrjId.PaymentDate, dbo.View_QryW3_WthPrjId.DollarPaidExcVAT
FROM         dbo.View_Projects_Balance_Pending INNER JOIN
                      dbo.View_QryW3_WthPrjId ON dbo.View_Projects_Balance_Pending.ProjectID = dbo.View_QryW3_WthPrjId.ProjectID AND 
                      dbo.View_Projects_Balance_Pending.SupplierName = dbo.View_QryW3_WthPrjId.SupplierName INNER JOIN
                      dbo.View_QryW3 ON dbo.View_QryW3_WthPrjId.PO_No = dbo.View_QryW3.PO_No
WHERE     (dbo.View_Projects_Balance_Pending.ProjectID = @ProjectID)
GROUP BY dbo.View_Projects_Balance_Pending.ProjectName, RTRIM(dbo.View_Projects_Balance_Pending.SupplierName), 
                      dbo.View_Projects_Balance_Pending.PoTotalDollarExcVAT, dbo.View_Projects_Balance_Pending.BalanceDollarExcVAT, 
                      dbo.View_Projects_Balance_Pending.DollarPendingExcVAT, RTRIM(dbo.View_QryW3_WthPrjId.PO_No), dbo.View_QryW3.PoTotalDollarExcVAT, 
                      RTRIM(dbo.View_QryW3_WthPrjId.Description), RTRIM(dbo.View_QryW3_WthPrjId.Invoice_No), dbo.View_QryW3_WthPrjId.Invoice_Date, 
                      dbo.View_QryW3_WthPrjId.OrderValueDollar, RTRIM(dbo.View_QryW3_WthPrjId.PO_Currency), dbo.View_QryW3_WthPrjId.PaymentDate, 
                      dbo.View_QryW3_WthPrjId.DollarPaidExcVAT, dbo.View_QryW3.BalanceDollarExcVAT - (CASE WHEN View_QryW3.DollarPendingExcVAT IS NULL 
                      THEN 0 ELSE View_QryW3.DollarPendingExcVAT END), dbo.View_Projects_Balance_Pending.DifferenceInDollar
ORDER BY SupplierName, PO_No</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
