  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     TOP (100) PERCENT ProjectName, SUM(EuroWithVAT) AS EuroWithVAT, SUM(BalanceWithVAT) AS BalanceWithVAT, Date, SupplierName, SupplierID
FROM         dbo.Table_PendingReportCnvEuro
WHERE     (Revised = 1)
GROUP BY ProjectName, Date, SupplierName, SupplierID
HAVING      (Date &gt; (CASE WHEN DATEPART(weekday, GETDATE()) &lt;&gt; 7 THEN GETDATE() - DATEPART(weekday, GETDATE()) - 2 WHEN DATEPART(weekday, GETDATE()) 
                      = 7 THEN GETDATE() - 2 END))
ORDER BY ProjectName, Date DESC, EuroWithVAT DESC, BalanceWithVAT DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
