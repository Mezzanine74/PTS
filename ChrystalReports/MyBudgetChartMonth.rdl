    <DataSource Name="DataSourceMyBudget">
      <DataSourceReference>DataSourceMyBudget</DataSourceReference>
      <rd:DataSourceID>4e017702-caf1-4d16-a129-bbda3d1cd8ef</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Ideal">
          <DataField>Ideal</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSourceMyBudget</DataSourceName>
        <CommandText>SELECT     Date, CASE WHEN DATE &gt; GETDATE() - 0 THEN 0 ELSE Amount + COALESCE
                          ((SELECT     SUM(Amount)
                              FROM         View_TotalSpendDaily b
                              WHERE     b.Date &lt; a.Date), 0) END AS Amount, Ideal + COALESCE
                          ((SELECT     SUM(Ideal) AS Expr1
                              FROM         View_TotalSpendDaily AS b
                              WHERE     (Date &lt; a.Date)), 0) AS Ideal
FROM         View_TotalSpendDaily AS a
ORDER BY Date</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="DataSet2">
      <Fields>
        <Field Name="Category_Name">
          <DataField>Category_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSourceMyBudget</DataSourceName>
        <CommandText>SELECT     TOP (100) PERCENT RTRIM(dbo.Table_Category.Category_Name) AS Category_Name, CASE WHEN SUM(Table_Spend.Amount) IS NULL 
                      THEN 0 ELSE SUM(Table_Spend.Amount) END AS Amount
FROM         dbo.Table_Spend INNER JOIN
                      dbo.Table_Category ON dbo.Table_Spend.Category_Id = dbo.Table_Category.Category_Id RIGHT OUTER JOIN
                      dbo.Table9_Date ON dbo.Table_Spend.Date = dbo.Table9_Date.Date
WHERE     (dbo.Table9_Date.Date &gt;= CASE WHEN getdate() &lt;= CONVERT(SmallDateTime, CONVERT(NVarChar(4), YEAR(GETDATE())) 
                      + N'-' + CASE WHEN LEN(CONVERT(NVarChar(2), MONTH(GETDATE()))) = 1 THEN N'0' + CONVERT(NVarChar(2), MONTH(GETDATE())) END + N'-' + N'10') 
                      + 1 THEN DATEADD(m, - 1, CONVERT(SmallDateTime, CONVERT(NVarChar(4), YEAR(GETDATE())) + N'-' + CASE WHEN LEN(CONVERT(NVarChar(2), 
                      MONTH(GETDATE()))) = 1 THEN N'0' + CONVERT(NVarChar(2), MONTH(GETDATE())) END + N'-' + N'10')) ELSE CONVERT(SmallDateTime, 
                      CONVERT(NVarChar(4), YEAR(GETDATE())) + N'-' + CASE WHEN LEN(CONVERT(NVarChar(2), MONTH(GETDATE()))) 
                      = 1 THEN N'0' + CONVERT(NVarChar(2), MONTH(GETDATE())) END + N'-' + N'10') END) AND (dbo.Table9_Date.Date &lt;= DATEADD(m, 1, 
                      CASE WHEN getdate() &lt;= CONVERT(SmallDateTime, CONVERT(NVarChar(4), YEAR(GETDATE())) + N'-' + CASE WHEN LEN(CONVERT(NVarChar(2), 
                      MONTH(GETDATE()))) = 1 THEN N'0' + CONVERT(NVarChar(2), MONTH(GETDATE())) END + N'-' + N'10') + 1 THEN DATEADD(m, - 1, 
                      CONVERT(SmallDateTime, CONVERT(NVarChar(4), YEAR(GETDATE())) + N'-' + CASE WHEN LEN(CONVERT(NVarChar(2), MONTH(GETDATE()))) 
                      = 1 THEN N'0' + CONVERT(NVarChar(2), MONTH(GETDATE())) END + N'-' + N'10')) ELSE CONVERT(SmallDateTime, CONVERT(NVarChar(4), 
                      YEAR(GETDATE())) + N'-' + CASE WHEN LEN(CONVERT(NVarChar(2), MONTH(GETDATE()))) = 1 THEN N'0' + CONVERT(NVarChar(2), MONTH(GETDATE())) 
                      END + N'-' + N'10') END)) AND (dbo.Table_Category.Category_Name &lt;&gt; N'Other')
GROUP BY RTRIM(dbo.Table_Category.Category_Name)
ORDER BY Amount DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
