  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT ContractID, AddendumID FROM (
	SELECT     ContractID, 0 AS AddendumID
	FROM         dbo.Table_Contracts
	WHERE     (ProjectID = @ProjectID)

	UNION ALL

	SELECT dbo.Table_Contracts.ContractID, AddendumID
	FROM dbo.Table_Addendums
	INNER JOIN dbo.Table_Contracts ON dbo.Table_Contracts.ContractID = dbo.Table_Addendums.ContractID
	WHERE     (ProjectID = @ProjectID)
) Source
ORDER BY ContractID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ContractID">
          <DataField>ContractID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AddendumID">
          <DataField>AddendumID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
