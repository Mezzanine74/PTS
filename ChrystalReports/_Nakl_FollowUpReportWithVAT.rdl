      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>

			DECLARE @ProjectID_performance int
			SET @ProjectID_performance = @ProjectID

			DECLARE @Currency_Performance nVarChar(10)
			SET @Currency_Performance  = @Currency

			DECLARE @ProjectType nvarChar(20)

			DECLARE @ProjectID_Performance_Performance int
			DECLARE @Currency_Performance_Performance nVarChar(10)

			SET @ProjectID_Performance_Performance = @ProjectID_Performance
			SET @Currency_Performance_Performance = @Currency_Performance


			SET @ProjectType = (SELECT Type FROM Table1_Project WHERE ProjectID = @ProjectID_Performance_Performance)

			IF @Currency_Performance_Performance = N'Rub' AND @ProjectType = N'DataCenter'
			-- RUBLE dataCenter
			SELECT     TOP (100) PERCENT dbo.View_CostCodeSummary0_DCC_WithVAT.CostCode1, dbo.View_CostCodeSummary0_DCC_WithVAT.CostCode2, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary2.VATpaidRuble) AS VATpaid, dbo.View_Project_VATrec_outs.OutstandingVATRuble AS OutstandingVAT,
			dbo.View_Project_VATrec_outs.VATRecoveredRuble AS VATRecovered,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalRublePendingWithVAT AS TotalPendingWithVAT,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalRublePaidWithVAT AS TotalPaidWithVAT,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalPoTotalRubleWithVAT AS TotalPoTotalWithVAT,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalBalanceRubleWithVAT AS TotalBalanceWithVAT,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalCollectedDocRubleWithVAT AS TotalCollected
			FROM         dbo.View_CostCodeSummary2 RIGHT OUTER JOIN
			dbo.View_Project_VATrec_outs RIGHT OUTER JOIN
			dbo.Table1_Project ON dbo.View_Project_VATrec_outs.Project_ID = dbo.Table1_Project.ProjectID ON
			dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary0_DCC_WithVAT RIGHT OUTER JOIN
			dbo.View_DCcostCodeTitle ON dbo.View_CostCodeSummary0_DCC_WithVAT.CostCodeAlias = dbo.View_DCcostCodeTitle.CostCode AND
			dbo.View_CostCodeSummary0_DCC_WithVAT.ProjectID = dbo.View_DCcostCodeTitle.ProjectID ON
			dbo.Table1_Project.ProjectID = dbo.View_DCcostCodeTitle.ProjectID
			WHERE     (dbo.Table1_Project.ProjectID = @ProjectID_Performance_Performance)
			ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_DCC_WithVAT.CostCode1

			IF @Currency_Performance_Performance = N'Dollar' AND @ProjectType = N'DataCenter'
			-- DOLLAR dataCenter
			SELECT     TOP (100) PERCENT dbo.View_CostCodeSummary0_DCC_WithVAT.CostCode1, dbo.View_CostCodeSummary0_DCC_WithVAT.CostCode2, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary2.VATpaidDollar) AS VATpaid, dbo.View_Project_VATrec_outs.OutstandingVATDollar AS OutstandingVAT,
			dbo.View_Project_VATrec_outs.VATRecoveredDollar AS VATRecovered,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalDollarPendingWithVAT AS TotalPendingWithVAT,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalDollarPaidWithVAT AS TotalPaidWithVAT,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalPoTotalDollarWithVAT AS TotalPoTotalWithVAT,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalBalanceDollarWithVAT AS TotalBalanceWithVAT,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalCollectedDocDollarWithVAT AS TotalCollected
			FROM         dbo.View_CostCodeSummary2 RIGHT OUTER JOIN
			dbo.View_Project_VATrec_outs RIGHT OUTER JOIN
			dbo.Table1_Project ON dbo.View_Project_VATrec_outs.Project_ID = dbo.Table1_Project.ProjectID ON
			dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary0_DCC_WithVAT RIGHT OUTER JOIN
			dbo.View_DCcostCodeTitle ON dbo.View_CostCodeSummary0_DCC_WithVAT.CostCodeAlias = dbo.View_DCcostCodeTitle.CostCode AND
			dbo.View_CostCodeSummary0_DCC_WithVAT.ProjectID = dbo.View_DCcostCodeTitle.ProjectID ON
			dbo.Table1_Project.ProjectID = dbo.View_DCcostCodeTitle.ProjectID
			WHERE     (dbo.Table1_Project.ProjectID = @ProjectID_Performance_Performance)
			ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_DCC_WithVAT.CostCode1


			IF @Currency_Performance_Performance = N'Euro' AND @ProjectType = N'DataCenter'
			-- EURO dataCenter
			SELECT     TOP (100) PERCENT dbo.View_CostCodeSummary0_DCC_WithVAT.CostCode1, dbo.View_CostCodeSummary0_DCC_WithVAT.CostCode2, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary2.VATpaidEuro) AS VATpaid, dbo.View_Project_VATrec_outs.OutstandingVATEuro AS OutstandingVAT,
			dbo.View_Project_VATrec_outs.VATRecoveredEuro AS VATRecovered,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalEuroPendingWithVAT AS TotalPendingWithVAT,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalEuroPaidWithVAT AS TotalPaidWithVAT,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalPoTotalEuroWithVAT AS TotalPoTotalWithVAT,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalBalanceEuroWithVAT AS TotalBalanceWithVAT,
			dbo.View_CostCodeSummary0_DCC_WithVAT.TotalCollectedDocEuroWithVAT AS TotalCollected
			FROM         dbo.View_CostCodeSummary2 RIGHT OUTER JOIN
			dbo.View_Project_VATrec_outs RIGHT OUTER JOIN
			dbo.Table1_Project ON dbo.View_Project_VATrec_outs.Project_ID = dbo.Table1_Project.ProjectID ON
			dbo.View_CostCodeSummary2.ProjectID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary0_DCC_WithVAT RIGHT OUTER JOIN
			dbo.View_DCcostCodeTitle ON dbo.View_CostCodeSummary0_DCC_WithVAT.CostCodeAlias = dbo.View_DCcostCodeTitle.CostCode AND
			dbo.View_CostCodeSummary0_DCC_WithVAT.ProjectID = dbo.View_DCcostCodeTitle.ProjectID ON
			dbo.Table1_Project.ProjectID = dbo.View_DCcostCodeTitle.ProjectID
			WHERE     (dbo.Table1_Project.ProjectID = @ProjectID_Performance_Performance)
			ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_DCC_WithVAT.CostCode1

			-- RUBLE FitOut
			IF @Currency_Performance_Performance = N'Rub' AND @ProjectType = N'FitOut'
			SELECT     TOP (100) PERCENT dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_WithVAT.CostCode1, dbo.View_CostCodeSummary0_WithVAT.CostCode2,
			CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0_WithVAT.TotalRublePendingWithVAT) AS TotalPendingWithVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0_WithVAT.TotalRublePaidWithVAT) AS TotalPaidWithVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0_WithVAT.TotalPoTotalRubleWithVAT) AS TotalPoTotalWithVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0_WithVAT.TotalBalanceRubleWithVAT) AS TotalBalanceWithVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary2.VATpaidRuble) AS VATpaid, dbo.View_Project_VATrec_outs.OutstandingVATRuble AS OutstandingVAT,
			dbo.View_Project_VATrec_outs.VATRecoveredRuble AS VATRecovered,
			dbo.View_CostCodeSummary0_WithVAT.TotalCollectedDocRubleWithVAT AS TotalCollected
			FROM         dbo.View_Project_VATrec_outs RIGHT OUTER JOIN
			dbo.Table1_Project ON dbo.View_Project_VATrec_outs.Project_ID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary2 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary2.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary0_WithVAT ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary0_WithVAT.ProjectID
			WHERE     (dbo.Table1_Project.ProjectID = @ProjectID_Performance_Performance)
			ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_WithVAT.CostCode1

			-- DOLLAR FitOut
			IF @Currency_Performance_Performance = N'Dollar' AND @ProjectType = N'FitOut'
			SELECT     TOP (100) PERCENT dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_WithVAT.CostCode1, dbo.View_CostCodeSummary0_WithVAT.CostCode2,
			CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0_WithVAT.TotalDollarPendingWithVAT) AS TotalPendingWithVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0_WithVAT.TotalDollarPaidWithVAT) AS TotalPaidWithVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0_WithVAT.TotalPoTotalDollarWithVAT) AS TotalPoTotalWithVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0_WithVAT.TotalBalanceDollarWithVAT) AS TotalBalanceWithVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary2.VATpaidDollar) AS VATpaid, dbo.View_Project_VATrec_outs.OutstandingVATDollar AS OutstandingVAT,
			dbo.View_Project_VATrec_outs.VATRecoveredDollar AS VATRecovered,
			dbo.View_CostCodeSummary0_WithVAT.TotalCollectedDocDollarWithVAT AS TotalCollected
			FROM         dbo.View_Project_VATrec_outs RIGHT OUTER JOIN
			dbo.Table1_Project ON dbo.View_Project_VATrec_outs.Project_ID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary2 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary2.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary0_WithVAT ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary0_WithVAT.ProjectID
			WHERE     (dbo.Table1_Project.ProjectID = @ProjectID_Performance_Performance)
			ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_WithVAT.CostCode1

			-- Euro FitOut
			IF @Currency_Performance_Performance = N'Euro' AND @ProjectType = N'FitOut'
			SELECT     TOP (100) PERCENT dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_WithVAT.CostCode1, dbo.View_CostCodeSummary0_WithVAT.CostCode2,
			CONVERT(decimal(12, 2), dbo.View_CostCodeSummary0_WithVAT.TotalEuroPendingWithVAT) AS TotalPendingWithVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0_WithVAT.TotalEuroPaidWithVAT) AS TotalPaidWithVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0_WithVAT.TotalPoTotalEuroWithVAT) AS TotalPoTotalWithVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary0_WithVAT.TotalBalanceEuroWithVAT) AS TotalBalanceWithVAT, CONVERT(decimal(12, 2),
			dbo.View_CostCodeSummary2.VATpaidEuro) AS VATpaid, dbo.View_Project_VATrec_outs.OutstandingVATEuro AS OutstandingVAT,
			dbo.View_Project_VATrec_outs.VATRecoveredEuro AS VATRecovered,
			dbo.View_CostCodeSummary0_WithVAT.TotalCollectedDocEuroWithVAT AS TotalCollected
			FROM         dbo.View_Project_VATrec_outs RIGHT OUTER JOIN
			dbo.Table1_Project ON dbo.View_Project_VATrec_outs.Project_ID = dbo.Table1_Project.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary2 ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary2.ProjectID LEFT OUTER JOIN
			dbo.View_CostCodeSummary0_WithVAT ON dbo.Table1_Project.ProjectID = dbo.View_CostCodeSummary0_WithVAT.ProjectID
			WHERE     (dbo.Table1_Project.ProjectID = @ProjectID_Performance_Performance)
			ORDER BY dbo.Table1_Project.ProjectID, dbo.View_CostCodeSummary0_WithVAT.CostCode1</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Currency">
            <Value>=Parameters!Currency.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
