  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="PO_No">
          <DataField>PO_No</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartPoValue">
          <DataField>StartPoValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="FinishPoValue">
          <DataField>FinishPoValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Delta">
          <DataField>Delta</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSourceFollowUpReportBackUp</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT DATASOURCE_1.PO_No, DATASOURCE_1.SumOrderValueRuble AS StartPoValue,
  DATASOURCE_2.SumOrderValueRuble  AS FinishPoValue,
(DATASOURCE_2.SumOrderValueRuble - DATASOURCE_1.SumOrderValueRuble) AS Delta 

FROM(
SELECT     TOP (100) PERCENT BackupDate, ProjectID, PO_No, SUM(OrderValueRuble) AS SumOrderValueRuble
FROM         dbo.View_FollowUpReportRuble
GROUP BY BackupDate, ProjectID, PO_No, PO_Currency, SupplierName
HAVING      (ProjectID = @ProjectID) AND (BackupDate = @FinishDate)
) AS DATASOURCE_1

LEFT OUTER JOIN 

(SELECT     TOP (100) PERCENT BackupDate, ProjectID, PO_No, SUM(OrderValueRuble) AS SumOrderValueRuble
FROM         dbo.View_FollowUpReportRuble
GROUP BY BackupDate, ProjectID, PO_No, PO_Currency, SupplierName
HAVING      (ProjectID = @ProjectID) AND (BackupDate = @StartDate))
 AS DATASOURCE_2 ON DATASOURCE_1.PO_No = DATASOURCE_2.PO_No 

ORDER BY DATASOURCE_1.PO_No ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FinishDate">
            <Value>=Parameters!FinishDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
