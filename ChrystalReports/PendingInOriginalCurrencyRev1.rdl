  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT  
	Title, 
	DayOfRun, 
	PendingWithVAT, 
	BalanceWithVAT
FROM  dbo.Table_PendingReportOrgCurry
WHERE (Revised = 1 ) AND	(DayOfRun &gt; (CASE WHEN DATEPART(weekday, GETDATE()) &lt;&gt; 7  THEN
 GETDATE() - DATEPART(weekday, GETDATE()) - 2
     WHEN DATEPART(weekday, GETDATE()) = 7  THEN
GETDATE() - 2
END))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DayOfRun">
          <DataField>DayOfRun</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PendingWithVAT">
          <DataField>PendingWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="BalanceWithVAT">
          <DataField>BalanceWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
