      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     [Вид документа], Контрагент, [Договор контрагента], [Имя проекта], DocValue, [Номер проекта], SupplierID
FROM         dbo.[1SdeliveryDocEntries]
WHERE     ([Номер проекта] = @ProjectID) AND (SupplierID = @SupplierID)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
          </QueryParameter>
