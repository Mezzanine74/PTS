  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Date1">
            <Value>=Parameters!Date1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Date2">
            <Value>=Parameters!Date2.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PO_No">
            <Value>=Parameters!PO_No.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     Tbl_PO_No.PO_No, Delivery.View_DeliveryDetails.Delivery_Doc_Type, Delivery.View_DeliveryDetails.Inv_ContrNo, 
                      Delivery.View_DeliveryDetails.Delivery_Doc_Description, Delivery.View_DeliveryDetails.Delivery_Doc_Date, Delivery.View_DeliveryDetails.Delivery_Doc_Value, 
                      Delivery.View_DeliveryDetails.Delivery_Doc_Comment
FROM         (SELECT     PO_No
                       FROM          Delivery.View_PO_Nos
                       WHERE      (DateCriteria &gt; @Date1) AND (DateCriteria &lt; @Date2) AND (SupplierID = @SupplierID)
                       GROUP BY PO_No) AS Tbl_PO_No INNER JOIN
                      Delivery.View_DeliveryDetails ON Tbl_PO_No.PO_No = Delivery.View_DeliveryDetails.PO_No
WHERE     (Delivery.View_DeliveryDetails.SupplierID = @SupplierID) AND (Delivery.View_DeliveryDetails.Delivery_Doc_Date &gt; @Date1) AND 
                      (Delivery.View_DeliveryDetails.Delivery_Doc_Date &lt; @Date2) AND (Tbl_PO_No.PO_No = @PO_No)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PO_No">
          <DataField>PO_No</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Delivery_Doc_Type">
