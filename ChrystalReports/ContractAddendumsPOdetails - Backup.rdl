  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="ContractID">
          <DataField>ContractID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PO_No">
          <DataField>PO No</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Po_Total_With_VAT">
          <DataField>Po Total With VAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Paid_With_VAT">
          <DataField>Paid With VAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Pending_With_VAT">
          <DataField>Pending With VAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Exceptionally_Approved">
          <DataField>Exceptionally Approved</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     dbo.Table_Contracts.ContractID, ISNULL(dbo.Table_Contracts.PO_No, N'') AS [PO No], 
                      ISNULL(CASE WHEN ContractCurrency = N'Rub' THEN dbo.View_QryW3_WithVAT.PoTotalRubleWithVAT WHEN ContractCurrency = N'Euro' THEN dbo.View_QryW3_WithVAT.PoTotalEuroWithVAT
                       WHEN ContractCurrency = N'Dollar' THEN dbo.View_QryW3_WithVAT.PoTotalDollarWithVAT END, 0) AS [Po Total With VAT], 
                      ISNULL(CASE WHEN ContractCurrency = N'Rub' THEN dbo.View_QryW2_WithVAT.RublePaidWithVAT WHEN ContractCurrency = N'Euro' THEN dbo.View_QryW2_WithVAT.EuroPaidWithVAT
                       WHEN ContractCurrency = N'Dollar' THEN dbo.View_QryW2_WithVAT.DollarPaidWithVAT END, 0) AS [Paid With VAT], CONVERT(numeric(12, 2), 
                      ISNULL(CASE WHEN ContractCurrency = N'Rub' THEN dbo.View_QryW1_WithVAT.RublePendingWithVAT WHEN ContractCurrency = N'Euro' THEN dbo.View_QryW1_WithVAT.EuroPendingWithVAT
                       WHEN ContractCurrency = N'Dollar' THEN dbo.View_QryW1_WithVAT.DollarPendingWithVAT END, 0)) AS [Pending With VAT], 
                      CASE WHEN Exceptional = 1 THEN N'YES' ELSE N'NO' END AS [Exceptionally Approved]
FROM         dbo.Table_Contracts LEFT OUTER JOIN
                      dbo.View_QryW3_WithVAT ON dbo.Table_Contracts.PO_No = dbo.View_QryW3_WithVAT.PO_No LEFT OUTER JOIN
                      dbo.View_QryW1_WithVAT ON dbo.Table_Contracts.PO_No = dbo.View_QryW1_WithVAT.PO_No LEFT OUTER JOIN
                      dbo.View_QryW2_WithVAT ON dbo.Table_Contracts.PO_No = dbo.View_QryW2_WithVAT.PO_No
WHERE     (dbo.Table_Contracts.ContractID = @ContractID)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ContractID">
            <Value>=Parameters!ContractID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
