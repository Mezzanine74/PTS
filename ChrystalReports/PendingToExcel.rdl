          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ProjectIdPerformance int = @ProjectID
DECLARE @SupplierIdPerformance nvarChar(12) = @SupplierID

if @ProjectIdPerformance = 0  and  @SupplierIdPerformance = N'0'
      SELECT * FROM [View_PendingListRev1] ORDER BY OrderCurrency, PendingWithVAT DESC 

      if @ProjectIdPerformance &lt;&gt; 0 and  @SupplierIdPerformance = N'0'
      SELECT * FROM [View_PendingListRev1] WHERE (ProjectID=@ProjectIdPerformance) ORDER BY OrderCurrency, PendingWithVAT DESC

      if @ProjectIdPerformance &lt;&gt; 0 and  @SupplierIdPerformance &lt;&gt; N'0'
      SELECT * FROM [View_PendingListRev1] WHERE (ProjectID=@ProjectIdPerformance) AND (SupplierID=@SupplierIdPerformance) ORDER BY OrderCurrency, PendingWithVAT DESC

      if @ProjectIdPerformance = 0 and  @SupplierIdPerformance &lt;&gt; N'0'
      SELECT * FROM [View_PendingListRev1] WHERE (SupplierID=@SupplierIdPerformance) ORDER BY OrderCurrency, PendingWithVAT DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
