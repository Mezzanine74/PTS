    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ProjectIdPerformance int = @ProjectID
DECLARE @SupplierIdPerformance nvarChar(12) = @SupplierID

IF @ProjectIdPerformance = 0 AND @SupplierIdPerformance = N'0' SELECT     PO_Currency, SUM(PendingWithVAT) AS SumPendingWithVAT
                                                                                            FROM         View_PendingListRev1
                                                                                            GROUP BY PO_Currency IF @ProjectIdPerformance &lt;&gt; 0 AND @SupplierIdPerformance = N'0'
                                                                                                                       SELECT     PO_Currency, SUM(PendingWithVAT) AS SumPendingWithVAT
                                                                                                                        FROM         View_PendingListRev1
                                                                                                                        WHERE     (ProjectID = @ProjectIdPerformance)
                                                                                                                        GROUP BY PO_Currency IF @ProjectIdPerformance &lt;&gt; 0 AND @SupplierIdPerformance &lt;&gt; N'0'
                                                                                                                                                   SELECT     PO_Currency, SUM(PendingWithVAT) AS SumPendingWithVAT
                                                                                                                                                    FROM         View_PendingListRev1
                                                                                                                                                    WHERE     (ProjectID = @ProjectIdPerformance) AND (SupplierID = @SupplierIdPerformance)
                                                                                                                                                    GROUP BY PO_Currency IF @ProjectIdPerformance = 0 AND @SupplierIdPerformance &lt;&gt; N'0'
                                                                                                                                                                               SELECT     PO_Currency, SUM(PendingWithVAT) AS SumPendingWithVAT
                                                                                                                                                                                FROM         View_PendingListRev1
                                                                                                                                                                                WHERE     (SupplierID = @SupplierIdPerformance)
                                                                                                                                                                                GROUP BY PO_Currency</CommandText>
      </Query>
