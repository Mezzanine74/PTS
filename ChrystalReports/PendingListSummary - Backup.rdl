  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="ProjectName">
          <DataField>ProjectName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RubleWithVAT">
          <DataField>RubleWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DollarWithVAT">
          <DataField>DollarWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="EuroWithVAT">
          <DataField>EuroWithVAT</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     ProjectName, SUM(CASE WHEN PO_Currency = N'Rub' THEN RubleWithVAT ELSE 0 END) AS RubleWithVAT, 
                      SUM(CASE WHEN PO_Currency = N'Dollar' THEN DollarWithVAT ELSE 0 END) AS DollarWithVAT, SUM(CASE WHEN PO_Currency = N'Euro' THEN EuroWithVAT ELSE 0 END) 
                      AS EuroWithVAT
FROM         dbo.View_PendingList
GROUP BY ProjectName
order by ProjectName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
