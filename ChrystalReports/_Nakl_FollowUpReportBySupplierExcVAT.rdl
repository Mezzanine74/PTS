  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Currency">
            <Value>=Parameters!Currency.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>

			DECLARE @ProjectID_performance int
			SET @ProjectID_performance = @ProjectID

			DECLARE @Currency_Performance nVarChar(10)
			SET @Currency_Performance  = @Currency

			if @Currency_Performance = N'Euro'
			SELECT     TOP (100) PERCENT RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName, dbo.Table6_Supplier.SupplierID, SUM(dbo.View_QryW3.PoTotalEuroExcVAT)
			AS TotalPoTotalExcVAT, SUM(dbo.View_QryW3.EuroPaidExcVAT) AS TotalPaidExcVAT, SUM(dbo.View_QryW3.BalanceEuroExcVAT) AS TotalBalanceExcVAT,
			SUM(dbo.View_QryW3.EuroPendingExcVAT) AS TotalPendingExcVAT, SUM(dbo.View_QryW3.PoTotalEuroExcVAT * dbo.View_PO_TotalCollectedDocsCoeff.Coeff)
			AS TotalCollectedDocsExcVAT
			FROM         dbo.View_QryW3 INNER JOIN
			dbo.Table2_PONo ON dbo.View_QryW3.PO_No = dbo.Table2_PONo.PO_No INNER JOIN
			dbo.Table6_Supplier ON dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
			dbo.View_PO_TotalCollectedDocsCoeff ON dbo.View_QryW3.PO_No = dbo.View_PO_TotalCollectedDocsCoeff.PO_No
			WHERE     (dbo.Table2_PONo.Project_ID = @ProjectID_Performance)
			GROUP BY RTRIM(dbo.Table6_Supplier.SupplierName), dbo.Table6_Supplier.SupplierID
			ORDER BY TotalPoTotalExcVAT DESC

			if @Currency_Performance = N'Dollar'
			SELECT     TOP (100) PERCENT RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName, dbo.Table6_Supplier.SupplierID, SUM(dbo.View_QryW3.PoTotalDollarExcVAT)
			AS TotalPoTotalExcVAT, SUM(dbo.View_QryW3.DollarPaidExcVAT) AS TotalPaidExcVAT, SUM(dbo.View_QryW3.BalanceDollarExcVAT) AS TotalBalanceExcVAT,
			SUM(dbo.View_QryW3.DollarPendingExcVAT) AS TotalPendingExcVAT, SUM(dbo.View_QryW3.PoTotalDollarExcVAT * dbo.View_PO_TotalCollectedDocsCoeff.Coeff)
			AS TotalCollectedDocsExcVAT
			FROM         dbo.View_QryW3 INNER JOIN
			dbo.Table2_PONo ON dbo.View_QryW3.PO_No = dbo.Table2_PONo.PO_No INNER JOIN
			dbo.Table6_Supplier ON dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
			dbo.View_PO_TotalCollectedDocsCoeff ON dbo.View_QryW3.PO_No = dbo.View_PO_TotalCollectedDocsCoeff.PO_No
			WHERE     (dbo.Table2_PONo.Project_ID = @ProjectID_Performance)
			GROUP BY RTRIM(dbo.Table6_Supplier.SupplierName), dbo.Table6_Supplier.SupplierID
			ORDER BY TotalPoTotalExcVAT DESC

			if @Currency_Performance = N'Ruble'
			SELECT     TOP (100) PERCENT RTRIM(dbo.Table6_Supplier.SupplierName) AS SupplierName, dbo.Table6_Supplier.SupplierID, SUM(dbo.View_QryW3.PoTotalRubleExcVAT)
			AS TotalPoTotalExcVAT, SUM(dbo.View_QryW3.RublePaidExcVAT) AS TotalPaidExcVAT, SUM(dbo.View_QryW3.BalanceRubleExcVAT) AS TotalBalanceExcVAT,
			SUM(dbo.View_QryW3.RublePendingExcVAT) AS TotalPendingExcVAT, SUM(dbo.View_QryW3.PoTotalRubleExcVAT * dbo.View_PO_TotalCollectedDocsCoeff.Coeff)
			AS TotalCollectedDocsExcVAT
			FROM         dbo.View_QryW3 INNER JOIN
			dbo.Table2_PONo ON dbo.View_QryW3.PO_No = dbo.Table2_PONo.PO_No INNER JOIN
			dbo.Table6_Supplier ON dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
			dbo.View_PO_TotalCollectedDocsCoeff ON dbo.View_QryW3.PO_No = dbo.View_PO_TotalCollectedDocsCoeff.PO_No
			WHERE     (dbo.Table2_PONo.Project_ID = @ProjectID_Performance)
			GROUP BY RTRIM(dbo.Table6_Supplier.SupplierName), dbo.Table6_Supplier.SupplierID
			ORDER BY TotalPoTotalExcVAT DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
