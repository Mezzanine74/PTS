      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>IF @Currency = N'Euro'

SELECT     TOP (100) PERCENT dbo.Table1_Project.ProjectID, RTRIM(dbo.Table2_PONo.PO_No) AS PO_No, RTRIM(CONVERT(nchar(10), 
                      dbo.Table7_CostCode.CostCode)) + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCode, dbo.View_QryW3_PoDetail.Invoice_No, 
                      dbo.View_QryW3_PoDetail.Invoice_Date, dbo.View_QryW3_PoDetail.InvoiceValue, dbo.View_QryW3_PoDetail.PO_Currency, 
                      dbo.View_QryW3_PoDetail.SiteRecordNo, dbo.View_QryW3_PoDetail.PaymentDate, dbo.View_QryW3_PoDetail.SupplierName, 
                      dbo.View_QryW3_PoDetail.Description, dbo.View_QryW3_PoDetail.EuroPendingExcVAT AS PendingExcVAT, 
                      dbo.View_QryW3_PoDetail.EuroPaidExcVAT AS PaidExcVAT, dbo.View_QryW3_PoDetail.OrderValueEuro AS OrderValue, 
                      dbo.View_QryW3_PoDetail.VATpaidEuro AS VATpaid, dbo.View_QryW3_PoDetail.OutstandingVATEuro AS OutstandingVAT, 
                      dbo.View_QryW3_PoDetail.StatusNote, dbo.View_QryW3_PoDetail.DoneEuro AS Done, dbo.View_QryW3_PoDetail.PartialEuro AS Partial
FROM         dbo.Table1_Project INNER JOIN
                      dbo.Table2_PONo ON dbo.Table1_Project.ProjectID = dbo.Table2_PONo.Project_ID INNER JOIN
                      dbo.Table7_CostCode ON dbo.Table2_PONo.CostCode = dbo.Table7_CostCode.CostCode LEFT OUTER JOIN
                      dbo.View_QryW3_PoDetail ON dbo.Table2_PONo.PO_No = dbo.View_QryW3_PoDetail.PO_No
WHERE     (dbo.Table1_Project.ProjectID = @ProjectID) AND (RTRIM(CONVERT(nchar(10), dbo.Table7_CostCode.CostCode)) 
                      + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription) LIKE '%'+@CostCode+'%')
ORDER BY CostCode, PO_No


IF @Currency = N'Dollar'

SELECT     TOP (100) PERCENT dbo.Table1_Project.ProjectID, RTRIM(dbo.Table2_PONo.PO_No) AS PO_No, RTRIM(CONVERT(nchar(10), 
                      dbo.Table7_CostCode.CostCode)) + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCode, dbo.View_QryW3_PoDetail.Invoice_No, 
                      dbo.View_QryW3_PoDetail.Invoice_Date, dbo.View_QryW3_PoDetail.InvoiceValue, dbo.View_QryW3_PoDetail.PO_Currency, 
                      dbo.View_QryW3_PoDetail.SiteRecordNo, dbo.View_QryW3_PoDetail.PaymentDate, dbo.View_QryW3_PoDetail.SupplierName, 
                      dbo.View_QryW3_PoDetail.Description, dbo.View_QryW3_PoDetail.DollarPendingExcVAT AS PendingExcVAT, 
                      dbo.View_QryW3_PoDetail.DollarPaidExcVAT AS PaidExcVAT, dbo.View_QryW3_PoDetail.OrderValueDollar AS OrderValue, 
                      dbo.View_QryW3_PoDetail.VATpaidDollar AS VATpaid, dbo.View_QryW3_PoDetail.OutstandingVATDollar AS OutstandingVAT, 
                      dbo.View_QryW3_PoDetail.StatusNote, dbo.View_QryW3_PoDetail.DoneDollar AS Done, dbo.View_QryW3_PoDetail.PartialDollar AS Partial
FROM         dbo.Table1_Project INNER JOIN
                      dbo.Table2_PONo ON dbo.Table1_Project.ProjectID = dbo.Table2_PONo.Project_ID INNER JOIN
                      dbo.Table7_CostCode ON dbo.Table2_PONo.CostCode = dbo.Table7_CostCode.CostCode LEFT OUTER JOIN
                      dbo.View_QryW3_PoDetail ON dbo.Table2_PONo.PO_No = dbo.View_QryW3_PoDetail.PO_No
WHERE     (dbo.Table1_Project.ProjectID = @ProjectID) AND (RTRIM(CONVERT(nchar(10), dbo.Table7_CostCode.CostCode)) 
                      + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription) LIKE '%'+@CostCode+'%')
ORDER BY CostCode, PO_No

IF @Currency = N'Ruble'

SELECT     TOP (100) PERCENT dbo.Table1_Project.ProjectID, RTRIM(dbo.Table2_PONo.PO_No) AS PO_No, RTRIM(CONVERT(nchar(10), 
                      dbo.Table7_CostCode.CostCode)) + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCode, dbo.View_QryW3_PoDetail.Invoice_No, 
                      dbo.View_QryW3_PoDetail.Invoice_Date, dbo.View_QryW3_PoDetail.InvoiceValue, dbo.View_QryW3_PoDetail.PO_Currency, 
                      dbo.View_QryW3_PoDetail.SiteRecordNo, dbo.View_QryW3_PoDetail.PaymentDate, dbo.View_QryW3_PoDetail.SupplierName, 
                      dbo.View_QryW3_PoDetail.Description, dbo.View_QryW3_PoDetail.RublePendingExcVAT AS PendingExcVAT, 
                      dbo.View_QryW3_PoDetail.RublePaidExcVAT AS PaidExcVAT, dbo.View_QryW3_PoDetail.OrderValueRuble AS OrderValue, 
                      dbo.View_QryW3_PoDetail.VATpaidRuble AS VATpaid, dbo.View_QryW3_PoDetail.OutstandingVATRuble AS OutstandingVAT, 
                      dbo.View_QryW3_PoDetail.StatusNote, dbo.View_QryW3_PoDetail.DoneRuble AS Done, dbo.View_QryW3_PoDetail.PartialRuble AS Partial
FROM         dbo.Table1_Project INNER JOIN
                      dbo.Table2_PONo ON dbo.Table1_Project.ProjectID = dbo.Table2_PONo.Project_ID INNER JOIN
                      dbo.Table7_CostCode ON dbo.Table2_PONo.CostCode = dbo.Table7_CostCode.CostCode LEFT OUTER JOIN
                      dbo.View_QryW3_PoDetail ON dbo.Table2_PONo.PO_No = dbo.View_QryW3_PoDetail.PO_No
WHERE     (dbo.Table1_Project.ProjectID = @ProjectID) AND (RTRIM(CONVERT(nchar(10), dbo.Table7_CostCode.CostCode)) 
                      + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription) LIKE '%'+@CostCode+'%')
ORDER BY CostCode, PO_No</CommandText>
