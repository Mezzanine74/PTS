        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     TOP (100) PERCENT dbo.View_QryW3_WithVAT.PO_No, dbo.Table2_PONo.PO_Date, dbo.Table2_PONo.PO_Currency, 
                      dbo.Table6_Supplier.SupplierName, 
                      (CASE WHEN dbo.Table2_PONo.PO_Currency = N'Rub' THEN dbo.View_QryW3_WithVAT.PoTotalRubleWithVAT WHEN dbo.Table2_PONo.PO_Currency =
                       N'Dollar' THEN dbo.View_QryW3_WithVAT.PoTotalDollarWithVAT WHEN dbo.Table2_PONo.PO_Currency = N'Euro' THEN dbo.View_QryW3_WithVAT.PoTotalEuroWithVAT
                       END) AS POtotalWithVAT, 
                      (CASE WHEN dbo.Table2_PONo.PO_Currency = N'Rub' THEN dbo.View_QryW3_WithVAT.RublePaidWithVAT WHEN dbo.Table2_PONo.PO_Currency = N'Dollar'
                       THEN dbo.View_QryW3_WithVAT.DollarPaidWithVAT WHEN dbo.Table2_PONo.PO_Currency = N'Euro' THEN dbo.View_QryW3_WithVAT.EuroPaidWithVAT
                       END) AS PaidToDate, 
                      (CASE WHEN dbo.Table2_PONo.PO_Currency = N'Rub' THEN dbo.View_QryW3_WithVAT.BalanceRubleWithVAT WHEN dbo.Table2_PONo.PO_Currency =
                       N'Dollar' THEN dbo.View_QryW3_WithVAT.BalanceDollarWithVAT WHEN dbo.Table2_PONo.PO_Currency = N'Euro' THEN dbo.View_QryW3_WithVAT.BalanceEuroWithVAT
                       END) AS Os, dbo.Table5_PayLog.PaymentDate, 
                      (CASE WHEN dbo.Table2_PONo.PO_Currency = N'Rub' THEN dbo.View_QryW3_PoDetail_WithVAT.RublePaidWithVAT WHEN dbo.Table2_PONo.PO_Currency
                       = N'Dollar' THEN dbo.View_QryW3_PoDetail_WithVAT.DollarPaidWithVAT WHEN dbo.Table2_PONo.PO_Currency = N'Euro' THEN dbo.View_QryW3_PoDetail_WithVAT.EuroPaidWithVAT
                       END) AS PaidWithVAT
FROM         dbo.View_QryW3_WithVAT INNER JOIN
                      dbo.Table2_PONo ON dbo.View_QryW3_WithVAT.PO_No = dbo.Table2_PONo.PO_No INNER JOIN
                      dbo.Table3_Invoice ON dbo.Table2_PONo.PO_No = dbo.Table3_Invoice.PO_No INNER JOIN
                      dbo.View_QryW3_PoDetail_WithVAT ON dbo.Table3_Invoice.InvoiceID = dbo.View_QryW3_PoDetail_WithVAT.InvoiceID INNER JOIN
                      dbo.Table6_Supplier ON dbo.Table2_PONo.SupplierID = dbo.Table6_Supplier.SupplierID INNER JOIN
                      dbo.Table4_PaymentRequest ON dbo.Table3_Invoice.InvoiceID = dbo.Table4_PaymentRequest.InvoiceID INNER JOIN
                      dbo.Table5_PayLog ON dbo.Table4_PaymentRequest.PayReqNo = dbo.Table5_PayLog.PayReqNo INNER JOIN
                      dbo.Table1_Project ON dbo.Table2_PONo.Project_ID = dbo.Table1_Project.ProjectID
WHERE     (dbo.Table1_Project.ProjectID = @ProjectID) AND (dbo.Table2_PONo.PO_Currency LIKE N'%' + @Currency + N'%')
ORDER BY dbo.View_QryW3_WithVAT.PO_No, dbo.Table5_PayLog.PaymentDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Currency">
            <Value>=Parameters!Currency.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
