        <CommandType>Text</CommandType>
        <CommandText>SELECT     TOP (100) PERCENT dbo.Table1_Project.ProjectID, RTRIM(dbo.Table2_PONo.PO_No) AS PO_No, RTRIM(CONVERT(nchar(10), dbo.Table7_CostCode.CostCode)) 
                      + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription) AS CostCode, dbo.View_QryW3_PoDetail.Invoice_No, dbo.View_QryW3_PoDetail.Invoice_Date, 
                      dbo.View_QryW3_PoDetail.InvoiceValue, dbo.View_QryW3_PoDetail.PO_Currency, dbo.View_QryW3_PoDetail.SiteRecordNo, 
                      dbo.View_QryW3_PoDetail.PaymentDate, dbo.View_QryW3_PoDetail.SupplierName, dbo.View_QryW3_PoDetail.Description, 
                      dbo.View_QryW3_PoDetail.EuroPendingExcVAT, dbo.View_QryW3_PoDetail.EuroPaidExcVAT, dbo.View_QryW3_PoDetail.OrderValueEuro, 
                      dbo.View_QryW3_PoDetail.VATpaidEuro, dbo.View_QryW3_PoDetail.OutstandingVATEuro, dbo.View_QryW3_PoDetail.StatusNote, 
                      dbo.View_QryW3.PoTotalEuroExcVAT, dbo.View_QryW3.BalanceEuroExcVAT, dbo.View_QryW3.EuroPendingExcVAT AS POTotalEuroPendingExcVAT, 
                      dbo.View_QryW3.EuroPaidExcVAT AS POTotalEuroPaidExcVAT, dbo.View_QryW3_PoDetail.DoneEuro, dbo.View_QryW3_PoDetail.PartialEuro, 
                      dbo.View_QryW3.DoneEuroPO, dbo.View_QryW3.PartialEuroPO
FROM         dbo.Table1_Project INNER JOIN
                      dbo.Table2_PONo ON dbo.Table1_Project.ProjectID = dbo.Table2_PONo.Project_ID INNER JOIN
                      dbo.Table7_CostCode ON dbo.Table2_PONo.CostCode = dbo.Table7_CostCode.CostCode LEFT OUTER JOIN
                      dbo.View_QryW3_PoDetail ON dbo.Table2_PONo.PO_No = dbo.View_QryW3_PoDetail.PO_No LEFT OUTER JOIN
                      dbo.View_QryW3 ON dbo.Table2_PONo.PO_No = dbo.View_QryW3.PO_No
WHERE     (dbo.Table1_Project.ProjectID = @ProjectID) AND (RTRIM(CONVERT(nchar(10), dbo.Table7_CostCode.CostCode)) 
                      + N' - ' + RTRIM(dbo.Table7_CostCode.CodeDescription) LIKE '%' + @CostCode + '%')
ORDER BY CostCode, PO_No</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ProjectID">
            <Value>=Parameters!ProjectID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CostCode">
            <Value>=Parameters!CostCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="ProjectID">
      <DataType>String</DataType>
      <Prompt>Project ID</Prompt>
    </ReportParameter>
    <ReportParameter Name="CostCode">
      <DataType>String</DataType>
      <Prompt>Cost Code</Prompt>
    </ReportParameter>
  </ReportParameters>
</Report>